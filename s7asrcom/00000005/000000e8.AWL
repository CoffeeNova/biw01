      R     #Par.Command.ResetWtTotal; 
      R     #Par.HelpFlags.CmdNewJob; 
      UN    #Par.Status.AcceptNewSendData; 
      O     #Par.ParOPTO22; 
      U     #Par.HelpFlags.CmdNewJob; 
      S     #Par.Command.ResetWtTotal; 

//==========================================================
