   CurrentMotorAc : INT ;	//Is read from MEAF: Motor current actual [A]
   Pressure : INT ;	//Is read from MEAF: actual Pressuret [0.1%]
   PMin : INT ;	//Is read from MEAF: min. Pressure [0.1%]
   PMax : INT ;	//Is read from MEAF: max. Pressure [0.1%]
   spare18 : INT ;	
