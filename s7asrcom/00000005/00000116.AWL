      L     #OutLCStep; 
      L     3; 
      >=I   ; 
      =     #Par.Status.Operation; 
      L     2; 
      L     #OutLCStep; 
      ==I   ; 
      =     #Par.Status.SetZeroPoint_Recipe1; 

//Alarm number / LC alarm bit mask
