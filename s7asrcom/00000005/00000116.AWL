      R     #Par.Command.SetZeroPoint; //[3.0]
      L     0; //[3.0]
      T     #OutSetZeroTime; //[3.0]
      SPA   STOR; 
RcvP: L     #tmpCommand; 
      L     'P'; 
      <>I   ; 
      SPB   RcvQ; 
