      L     DBD   36; //COM.Element.InOutValue
      T     #OutAlarmNr; 

//  Convert AlarmNr to MEAF-MZAH
//    --------------
      L     1; //Ram defective
      L     #OutAlarmNr; 
      ==I   ; 
      L     2; //--> MEAF: RAM
      SPB   riAL; 
//    --------------
      L     2; //Ram Battery
      L     #OutAlarmNr; 
      ==I   ; 
      L     6; //--> MEAF: Data lost
      SPB   riAL; 
//    --------------
      L     3; //Eprom
      L     #OutAlarmNr; 
      ==I   ; 
      L     1; //--> MEAF: Eprom
      SPB   riAL; 
//    --------------
      L     4; //24V
      L     #OutAlarmNr; 
      ==I   ; 
      L     5; //--> MEAF: 24V
      SPB   riAL; 
//    --------------
      L     5; //Trap
      L     #OutAlarmNr; 
      ==I   ; 
      L     3; //--> MEAF: Runtime error Software
      SPB   riAL; 
//    --------------
      L     6; //24V output Overload
      L     #OutAlarmNr; 
      ==I   ; 
      L     5; //--> MEAF: 24V
      SPB   riAL; 
//    --------------
      L     8; //Serial interface
      L     #OutAlarmNr; 
      ==I   ; 
      L     12; //--> MEAF:Timemout Host
      SPB   riAL; 
//    --------------
      L     9; //Bin Low Level
      L     #OutAlarmNr; 
      ==I   ; 
      L     37; //--> MEAF:Timemout Host
      SPB   riAL; 
      L     #OutAlarmNr; //ELSE
riAL: T     #OutAlarmNr; 

      L     #OutAlarmNr; 
      L     37; 
      ==I   ; 
      =     #Par.Status.Empty; 
      SPA   STOR; 
