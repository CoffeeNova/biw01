      U     #Gr.OutManual; 
      =     "GCS7_GrCtrlDB".InManual[133]; 
      U     #Gr.OutHWStop; 
      =     "GCS7_GrCtrlDB".InHWStop[133]; 
      U     #Gr.OutDPFault; 
      =     "GCS7_GrCtrlDB".InDPFault[133]; 
      U     #Gr.OutLoaded; 
      =     "GCS7_GrCtrlDB".InLoaded[133]; 
      U     #Gr.OutAutostart; 
