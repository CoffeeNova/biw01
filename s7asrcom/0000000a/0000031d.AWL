      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2212_QXV_BZS11_12.InLocal; 
//
//-Interlocking---
      U     #Gr.OutEnableEl; 
      U     "MAL1S018Par".Par.FlagFunction_1.TurningRight; 
      U     "G125M".A_2211_MXZ41MA.Ma.StStartedFwd; 
      =     #A_2212_QXV_BZS11_12.InEnableHN; 
      NOT   ; 
      =     #A_2212_QXV_BZS11_12.InEnableLN; 
//
      SET   ; 
      S     #A_2212_QXV_BZS11_12.CmdNext; 
//
      U     #Gr.OutSeqStop; 
      =     #A_2212_QXV_BZS11_12.InSeqStop; 
// 
