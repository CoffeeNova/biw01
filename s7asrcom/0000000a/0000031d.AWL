      O     #A_2212_QXV_BZS11_12.StHN; 
      O     #A_2212_QXV_BZS21_22.StHN; 
      )     ; 
//BRT21031      UN    #Gr.VarStopped
      UN    #Gr.VarFault; 
      =     #Gr.VarStarted; 

NETWORK
TITLE =Enable Previous Group

      U     #Gr.VarStarted; 
      =     #Gr.OutEnablePrevGr; 
NETWORK
