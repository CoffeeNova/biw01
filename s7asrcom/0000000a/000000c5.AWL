	oldS: NOP   0; 
	      L     "ASW_GraphicData".General.CycleTime[1].Actual; 
	      L     "MAL1S022Par".Par.ProcessValue.StepTimeAct; // in ms
	      +D    ; 
	      T     "MAL1S022Par".Par.ProcessValue.StepTimeAct; // in ms
	      L     "MAL1S022Par".Par.ProcessValue.StepTimeAct; // in ms
	      L     L#86400000; // ms/d
	      MOD   ; 
	      L     L#3600000; // ms/h
	      /D    ; 
	      T     "MAL1S022Par".ActTimeStepProcess.SteptimeActHour; // Steptime in h
	// 
	      L     "MAL1S022Par".Par.ProcessValue.StepTimeAct; 
	      L     L#3600000; // ms/h
	      MOD   ; 
	      L     L#60000; // ms/min
	      /D    ; 
	      T     "MAL1S022Par".ActTimeStepProcess.SteptimeActMin; // Steptime in min
	// 
	      L     "MAL1S022Par".Par.ProcessValue.StepTimeAct; 
	      L     L#60000; // ms/min
	      /D    ; 
	      T     "MAL1S022Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
	// 
	// Next Step
	// 
	//  Switch over to next step under conditions:
	//  or Maximal steptime
	//  or Minimal steptime
	//          and 
	//              or no setpoint delta temp
	//              or atual value delta temp is ge setpoint deltatemp
	//                  and sensor is ok
	//          and          
	//              or no setpoint max temp above floor
	//              or atual value max temp above floor is ge setpoint max temp above floor
	//                  and sensor is ok
	
	// Total hours 0..32767
	      L     "MAL1S022Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
	      ITD   ; 
	      DTR   ; 
	      L     6.000000e+001; // min/h
	      /R    ; 
	      TRUNC ; 
	      T     "MAL1S022Par".ActTimeStepProcess.StepTimeActHourTotal; // Steptime in hours overall
	// 
	// 1 Kriterium - steptime minimal 
	      U(    ; 
	      L     "MAL1S022Par".ActTimeStepProcess.StepTimeActHourTotal; // Steptime in hours overall
	      L     "MAL1S022Par".StepAct.StepTime_Min; // Target steptime actual step
	      >=I   ; 
	      )     ; 
	      U(    ; // and  one of 
	// 2 Kriterium - or delta temperature
	      O(    ; 
	      U     "MAL1S022Par".Par.FlagProcess1.DeltaTempNextStep; 
	      U(    ; 
	      L     "MAL1S022Par".Par.ProcessValue.ActValueDeltaTempDint; 
	      L     "MAL1S022Par".StepAct.DeltaTempSwitchpointStep; 
	      >=D   ; 
	      )     ; 
	      )     ; 
	// 3 Kriterium - or setpoint temperature above floor
	      O(    ; 
	      U     "MAL1S022Par".Par.FlagProcess1.MaxTempNextStep; 
	      U(    ; 
	      L     "MAL1S022Par".Par.ProcessValue.ActValMaxTempAboveDint; 
	      L     "MAL1S022Par".StepAct.MaxTempAboveFloorStep; 
	      >=D   ; 
	      )     ; 
	      )     ; 
	      )     ; 
	// 4 Kriterium - or only maimal steptime
	      O(    ; 
	      L     "MAL1S022Par".ActTimeStepProcess.StepTimeActHourTotal; // Steptime in hours overall
	      L     "MAL1S022Par".StepAct.StepTime_Max; // Target steptime actual step
	      >=I   ; 
	      )     ; 
	      SPBN  E000; 
	
	// send average step data to PIC
	      SET   ; 
	      S     "MAL1S022StepValues".SendData; // send average step data to PIC
	// Next step
	      L     "MAL1S022Par".Par.ParIntMachine.ActualStep; 
	      L     "MAL1S022".SecNom.GERM.RecipeHeader.NumberOfSteps; 
	      >I    ; 
	      SPB   Pend; // Last step is finished
	// 
	      L     "MAL1S022Par".Par.ParIntMachine.ActualStep; 
	      L     1; 
	      +I    ; 
	      T     "MAL1S022Par".Par.ParIntMachine.ActualStep; 
	      L     0; 
	      T     "MAL1S022Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
	      SET   ; 
	      R     "MAL1S022Par".Par.FlagProcess.MessageNoNextStep; // Meldung "No next step"
	      SPA   E000; 
	Pend: NOP   0; // Process has finished
	      SET   ; 
