	TITLE =ASW_6010: Process germinating active
	// 
	      U     "MAL1S022Par".Par.FlagProcess.ProcessActive; 
	      SPBN  E000; 
	//-Actual process time---
	      U     "MAL1S022Par".Par.FlagProcess.ProcessStartScan; 
	      SPBN  actu; 
	      L     1; // Start process first step
	      T     "MAL1S022Par".Par.ParIntMachine.ActualStep; 
	      T     "MAL1S022Par".Par.ProcessValue.StepNrActCopy; 
	      SPA   newS; 
	actu: NOP   0; 
	// Prozesszeit 
	      L     "DB Prozesszeit".Sektion_22.Tage; 
	      INC   1; // malters do not accept day 0!!
	      T     "MAL1S022Par".Processtime.Day; 
	      L     "DB Prozesszeit".Sektion_22.Stunden; 
	      T     "MAL1S022Par".Processtime.Hour; // Processtime in h
