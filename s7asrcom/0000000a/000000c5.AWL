	NETWORK
	TITLE =ASW_6040: Set fanspeed by delta-t
	//Die Drehzahl des Ventilators wird zwischen dem Rezeptsollwert, als Untere 
	//Grenze, und 100% oder Rezeptsollwert, als obere Grenze, geregelt. Die Regelung 
	//erfolgt nach folgendem Prinzip:
	//"Delta-T" ist die Differenz zwischen der Soll-Temperatur (Rezeptsollwert) unter 
	//der Horde und der gemessenen Ist-Temperatur ber der Horde.
	//Steigt die Ist-Temperatur ber der Horde und "Delta-T" wird > als der Sollwert 
	//im Rezept, so wird die Drehzahl des Ventilators stufenweise erhht. Maximale 
	//Drehzahl ist 100% oder Rezeptsollwert.
	//Sinkt die Ist-Temperatur ber der Horde und "Delta-T" wird < als der Sollwert 
	//im 
	//Rezept, so wird die Drehzahl des Ventilators stufenweise verringert. Minimale 
	//Drehzahl ist der Rezeptsollwert.
	//Die Rampen (Stufen) werden durch den Technologen festgelegt und in Programmcode 
	//eingetragen. Es sind keine Rezeptsollwerte.
	      NOP   0; 
	// recipe setpoint
	      L     "MAL1S022Par".Par.ProcessValue.StepTimeAct; // in ms
	      L     L#1000; // in first secunde steptime
	      >D    ; 
	      SPB   regu; 
	      SPA   Summ; 
	regu: NOP   0; 
	 
	      U     "MAL1S022".ToASW.CmdModify; 
	      SPB   Summ; 
	  
	// Setpoint
	      L     "MAL1S022Par".StepAct.TemperaturAboveFloor; 
	      L     "MAL1S022Par".StepAct.TemperaturBelowFloor; 
	      -D    ; 
	      T     "MAL1S022Par".Par.ProcessValue.SetpointDeltaTempDint; 
	// actual value
	      L     L#10; // von xx,xxC AUF xx,xC
	      /D    ; 
	      L     "MAL1S022Par".StepAct.TemperaturBelowFloor; 
	      -D    ; 
	      T     "MAL1S022Par".Par.ProcessValue.ActValueDeltaTempDint; 
	// flags to inc/dec
	      L     "MAL1S022Par".Par.ProcessValue.SetpointDeltaTempDint; 
	      -D    ; 
	      T     "MAL1S022Par".Par.ProcessValue.ResultDeltaTemp; 
	      L     L#0; 
	      >D    ; 
	      =     "MAL1S022Par".Par.FlagProcess1.DeltaTempPlus; 
	      <D    ; 
	      =     "MAL1S022Par".Par.FlagProcess1.DeltaTempMinus; 
	
	      U     "MAL1S022Par".Par.FlagProcess1.DeltaTempPlus; 
	      U     "SYS_Scan0_Tick1min"; 
	      SPBN  dec; 
	      L     "MAL1S022Par".Par.ParIntMachine.TargetSpeedGermFan; 
	      L     100; // linmit max
	      >=I   ; 
	      SPB   dec; 
	      L     "MAL1S022Par".Par.ParIntMachine.TargetSpeedGermFan; 
	      L     1; 
	      +I    ; 
	      T     "MAL1S022Par".Par.ParIntMachine.TargetSpeedGermFan; 
	      SPA   e604; 
	dec:  NOP   0; 
	      U     "MAL1S022Par".Par.FlagProcess1.DeltaTempMinus; 
	      U     "SYS_Scan0_Tick1min"; 
	      SPBN  e604; 
	      L     "MAL1S022Par".Par.ParIntMachine.TargetSpeedGermFan; 
	      L     "MAL1S022Par".StepAct.SpeedVentilatorBasic; // linmit min
	      <=I   ; 
	      SPB   e604; 
	      L     "MAL1S022Par".Par.ParIntMachine.TargetSpeedGermFan; 
	      L     1; 
	      -I    ; 
	      T     "MAL1S022Par".Par.ParIntMachine.TargetSpeedGermFan; 
	      SPA   e604; 
	
	Summ: NOP   0; 
	      L     "MAL1S022Par".StepAct.SpeedVentilatorBasic; 
	      T     "MAL1S022Par".Par.ParIntMachine.TargetSpeedGermFan; 
	e604: NOP   0; 
	E604: NOP   0; 
	NETWORK
	TITLE =ASW_6050: Position waste air
	//Der Betriebsart Sommerbetrieb/Winterbetrieb wird durch den Bediener per Rezept 
	//bestimmt.
	//Wird die Khlung im Rezept angewhlt, so erfolgt die Umschaltung AUF 
	//Sommerbetrieb. Die Frischluftklappe wird nach dem Rezeptsollwert gestellt.
	//Es erfolgt keine Regelung.
	//Wird die Khlung im Rezept abgewhlt, so erfolgt die Umschaltung AUF 
	//Winterbetrieb. Die Frischluftklappe wird zuerst nach dem Rezeptsollwert 
	//gestellt. Der Rezeptsollwert ist die untere Grenze fr die Regelung.
	//Wird die Soll-Temperatur unter der Horde von Khl- Heizanlage nicht eingehalten 
	//-Temperatur zu hoch und der Schaltpunkt berschritten, so wird die 
	//Frischluftklappe stufenweise geffnet. 
	//Maximalwert ist 100%.
	//Wird die Soll-Temperatur unter der Horde von Khl- Heizanlage nicht eingehalten 
	//-Temperatur zu niedrig und der Schaltpunkt unterschritten, so wird die 
	//Frischluftklappe stufenweise geschlossen. 
	//Minimalwert ist der Rezeptsollwert.
	       NOP 0; 
	// setpoint waste air
	      U M 50.0;
	      = M 50.0;
	      L     L#10; // 1,0
	      T     "MAL1S022Par".StepAct.DeltaTempSwitchpointAir; 
	
	// recipe setpoint
	      L     "MAL1S022Par".Par.ProcessValue.StepTimeAct; // in ms
	      L     L#1000; // in first secunde steptime
	      >D    ; 
	      SPB   Reg; 
	      SPA   summ; 
	Reg:  NOP   0; 
	 
	// Cooling or heating from cooling defive active --> no flap regulation!   
	      U M 50.0;
	//      U     "ComDB_Cooling".Ma.ACK.ActivFlag.Cool.Germinating_1_Started // Internal or external coooling is running
	      O     "MAL1S022".ToASW.CmdModify; 
	      SPB   summ; 
	
	      L     L#10; // von xx,xxC AUF xx,xC
	      /D    ; 
	      L     "MAL1S022Par".StepAct.TemperaturBelowFloor; 
	      -D    ; 
	      L     "MAL1S022Par".StepAct.DeltaTempSwitchpointAir; 
	      >D    ; 
	      =     "MAL1S022Par".Par.FlagProcess1.FreshAirFlapOPEN; 
	
	      L     L#10; // von xxx,xxC AUF xxx,xC
	      /D    ; 
	      L     "MAL1S022Par".StepAct.TemperaturBelowFloor; 
	      TAK   ; 
	      -D    ; 
	      L     "MAL1S022Par".StepAct.DeltaTempSwitchpointAir; 
	      >D    ; 
	      =     "MAL1S022Par".Par.FlagProcess1.FreshAirFlapCLOSE; 
	
	      U     "MAL1S022Par".Par.FlagProcess1.FreshAirFlapOPEN; 
	      U     "SYS_Scan0_Tick1min"; 
	      SPBN  dez; 
	      L     "MAL1S022Par".Par.ParIntMachine.TargetPosWasteAir;
	      L     100; // limit max
	      >=I   ; 
	      SPB   dez; 
	      L     "MAL1S022Par".Par.ParIntMachine.TargetPosWasteAir;
	      L     1; 
	      +I    ; 
	      T     "MAL1S022Par".Par.ParIntMachine.TargetPosWasteAir;
	      SPA   e605; 
	dez:  NOP   0; 
	      U     "MAL1S022Par".Par.FlagProcess1.FreshAirFlapCLOSE; 
	      U     "SYS_Scan0_Tick1min"; 
	      SPBN  e605; 
	      L     "MAL1S022Par".Par.ParIntMachine.TargetPosWasteAir;
	      L     "MAL1S022Par".StepAct.PosFreshAirFlapBasic; // limit min
	      <=I   ; 
	      SPB   e605; 
	      L     "MAL1S022Par".Par.ParIntMachine.TargetPosWasteAir;
	      L     1; 
	      -I    ; 
	      T     "MAL1S022Par".Par.ParIntMachine.TargetPosWasteAir;
	      SPA   e605; 
	
	summ: NOP   0; 
	      L     "MAL1S022Par".StepAct.PosFreshAirFlapBasic; 
	      T     "MAL1S022Par".Par.ParIntMachine.TargetPosWasteAir;
	      SPA   e605; 
	
	e605: NOP   0; 
	E605: NOP   0; 
	NETWORK
	TITLE =ASW_6020: Copy germinating-recipe-data in Parameter DB
	      U     "MAL1S022Par".Par.FlagProcess.ProcessActive; 
	      SPB   NoCl; 
	// Clear process time in buffer
	      L     0; 
	      T     "MAL1S022Par".ActTimeStepProcess.SteptimeActHour; 
	      T     "MAL1S022Par".ActTimeStepProcess.SteptimeActMin; 
	      T     "MAL1S022Par".ActTimeStepProcess.SteptimeActMinOverall; 
	
	      T     "MAL1S022Par".Processtime.Day; 
	      T     "MAL1S022Par".Processtime.Hour; 
	      T     "MAL1S022Par".Processtime.Min; 
	      T     "MAL1S022Par".Processtime.ProcessTimeActive; 
	      L     L#0; // clear actual step time
	      T     "MAL1S022Par".Par.ProcessValue.StepTimeAct; // in ms
	
	NoCl: NOP   0; 
	// Clear content recipe in buffer
	      L     0; 
	      T     #tmpInt; 
	      CALL "SIEMENS_FILL" (
		   BVAL                     := #tmpInt,
		   RET_VAL                  := #tmpRet,
		   BLK                      := "MAL1S022Par".RecipeHeader);
	// 
	      U     "MAL1S022Par".Par.FlagFunction_1.StepUnloading; 
	      UN    "MAL1S022Par".Par.FlagProcess.ProcessActive; 
	      O     "MAL1S022Par".Par.FlagFunction_1.StepCleaning; 
	      O     "MAL1S022Par".Par.FlagFunction_1.StepLoading; 
	      SPBN  No00; 
	// Germ: Copy step loading,unloading,cleaning to parameterDB
	      CALL "SIEMENS_BLKMOV" (
		   SRCBLK                   := "MAL1S022".SecNom.Germ.RecipeHeader.Loading,
		   RET_VAL                  := "MAL1S022Par".Par.Error.Value,
		   DSTBLK                   := "MAL1S022Par".RecipeHeader.Loading);
	      CALL "SIEMENS_BLKMOV" (
		   SRCBLK                   := "MAL1S022".SecNom.Germ.RecipeHeader.Unloading,
		   RET_VAL                  := "MAL1S022Par".Par.Error.Value,
		   DSTBLK                   := "MAL1S022Par".RecipeHeader.Unloading);
	      CALL "SIEMENS_BLKMOV" (
		   SRCBLK                   := "MAL1S022".SecNom.Germ.RecipeHeader.Cleaning,
		   RET_VAL                  := "MAL1S022Par".Par.Error.Value,
		   DSTBLK                   := "MAL1S022Par".RecipeHeader.Cleaning);
	
	No00: NOP   0; 
	      U     "MAL1S022Par".Par.FlagProcess.ProcessActive; 
	      SPBN  No03; 
	// Germ: Copy common parameter
	      CALL "SIEMENS_BLKMOV" (
		   SRCBLK                   := "MAL1S022".SecNom.Germ.RecipeHeader,
		   RET_VAL                  := "MAL1S022Par".Par.Error.Value,
		   DSTBLK                   := "MAL1S022Par".RecipeHeader);
	// 
	// Copy step parameter
	      U(    ; 
	      L     "MAL1S022Par".Par.ParIntMachine.ActualStep; 
	      L     1; 
	      <I    ; 
	      )     ; 
	      O(    ; 
	      L     "MAL1S022Par".Par.ParIntMachine.ActualStep; 
	      L     "MAL1S022".SecNom.GERM.RecipeHeader.NumberOfSteps; 
	      >I    ; 
	      )     ; 
	      SPB   NoXX; // error wrong step number
	      CALL "SYS_Copy_Array_Element" (
		   Source                   := "MAL1S022".SecNom.Germ.Step,
		   Destination              := "MAL1S022Par".StepAct,
		   Index                    := "MAL1S022Par".Par.ParIntMachine.ActualStep,
		   Fault                    := "MAL1S022Par".Par.Error.Bit);
	
	No03: NOP   0; 
	NoXX: NOP   0; 
