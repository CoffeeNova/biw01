BEGIN
NETWORK
TITLE =



NETWORK
TITLE =Read command from group control
//Parameter
      SET   ; 
      S     "GCS7_GrCtrlDB".ParMGR[271]; 
      R     "GCS7_GrCtrlDB".ParWGR[271]; 
      R     "GCS7_GrCtrlDB".ParGGR[271]; 
      R     "GCS7_GrCtrlDB".ParFeed[271]; 
      R     "GCS7_GrCtrlDB".ParShare[271]; 
      S     #Gr.Par.ParMgr; 
      R     #Gr.Par.ParWgr; 
      R     #Gr.Par.ParGgr; 
//Command
      U     "GCS7_GrCtrlDB".OutEnable[271]; 
      =     #Gr.InEnableGr; 
      U     "GCS7_GrCtrlDB".OutOccupied[271]; 
      =     #Gr.InOccupied; 
      U     "GCS7_GrCtrlDB".CmdFaultReset[271]; 
      R     "GCS7_GrCtrlDB".CmdFaultReset[271]; 
      S     #Gr.CmdFaultReset; 
      U     "GCS7_GrCtrlDB".CmdNext[271]; 
      R     "GCS7_GrCtrlDB".CmdNext[271]; 
      S     #Gr.CmdNext; 

      U     "GCS7_GrCtrlDB".CmdManualOff[271]; 
      UN    #Gr.InManual; 
      S     #Gr.CmdManualOff; 
      U     "GCS7_GrCtrlDB".CmdManualOff[271]; 
      R     "GCS7_GrCtrlDB".CmdManualOff[271]; 

//      U     "GCS7_GrCtrlDB".CmdManualOff[271]
//      O     "GCS7_GrCtrlDB".OutOccupied[271]
//      R     "GCS7_GrCtrlDB".CmdManualOff[271]
//      S     #Gr.CmdManualOff

      U     "GCS7_GrCtrlDB".CmdContinue[271]; 
      R     "GCS7_GrCtrlDB".CmdContinue[271]; 
      S     #Gr.CmdContinue; 
//Init Line Number
      U     "GCS7_GrCtrlDB".ParShare[271]; 
      SPBN  WE00; 
      L     0; 
      T     #Gr.InLine; 
WE00: NOP   0; 
NETWORK
TITLE =Call Methode Control
// 1. Handle Interface PCControl      
// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
// 3. Reset global flags of the group
      CALL #Gr (
           FcCode                   := 1);
NETWORK
TITLE =Restart Idling Time

      L     #Gr.Par.ParIdling; 
      L     2; 
      >=I   ; 
      SPB   IdTm; 
      T     #Gr.Par.ParIdling; 
IdTm: NOP   0; 
      CLR   ; 
      S     #Gr.CmdRestartIdling; 
NETWORK
TITLE =Prepare global flags



NETWORK
TITLE =Set Pattern (Target position)

      U     #Gr.InManual; 
      SPB   ILO6; 

// Drehzahlabsenkung wenn die andere Horde im Cooling ist
// und die Temperatur unter der eigenen Horde 5°C > als T[Soll] 
//      U     "MAL1S033Par".Par.FlagProcess.Cooling_Activ
//      U     "G276M".USW8                // Temperatur unter der Horde 5°C > T[Soll]
//      L     50                          // 50%
//      SPB   En06
//JV270710 ======>
// Drehzahlabsenkung wenn die andere Horde gerade zugeschaltet wurde
      U(    ; 
      U     "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      U     "MAL1S033Par".Par.FlagProcess.ProcessActive; 
      U(    ; 
      O(    ; //JV290710 ======>
      U     "MAL1S032Par".Par.FlagProcess.Abdarr_Betrieb_Aktiv; 
      U     "MAL1S032Par".Par.FlagProcess.Cooling_Activ; 
      UN    "G278M".A_3145_MXZ11.StLN; // Abluftklappen zu
      UN    "G278M".A_3146_MXZ11.StLN; // Abluftklappen zu
      )     ; 
      O(    ; 
      U     "MAL1S033Par".Par.FlagProcess.Cooling_Activ; 
      UN    "G278M".A_3145_MXZ11.StHN; // Abluftklappen auf
      UN    "G278M".A_3146_MXZ11.StHN; // Abluftklappen auf
      )     ; 
      )     ; //JV290710 <======
      )     ; 
//BRT20100827 ====> beide Prozesse gestartet, aber Darren nicht bereit für Verbund!!!
      O(    ; 
      U     "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      U     "MAL1S033Par".Par.FlagProcess.ProcessActive; 
      UN    "MAL1S032Par".Par.FlagProcess.Abdarr_Betrieb_Aktiv; 
      UN    "MAL1S033Par".Par.FlagProcess.Abdarr_Betrieb_Aktiv; 
      UN(   ; 
      U     "MAL1S032Par".Par.FlagProcess.Cooling_Activ; 
      O     "MAL1S033Par".Par.FlagProcess.Cooling_Activ; 
      )     ; 
      )     ; 
//BRT20100827 <====
      L     60; // 60% Drehzahl bis Klappen richtig stehen
      SPB   En06; 
//JV270710 <========

//BRT20100826 ====> Ventilator geregelt während Kühlung und Lufttransfer Darre 1 nach 2!
// 
      U     "MAL1S032Par".Par.FlagProcess.Cooling_Activ; 
      U     "MAL1S033Par".Par.FlagProcess.ProcessActive; 
      L     "MAL1S032Par".Par.ProcessValue.Value15; // Sollwert Ventilator Kühlen
      SPB   En06; 
//BRT20100826 <====
//BRT20100902 ====> wegen Programmänderungen Regelung Verbund
//      L     "MAL1S032Par".Par.ParIntProcess.SpeedVentilatorBasic
      L     "MAL1S032Par".Par.ProcessValue.Value18; // Sollwert Ventilator Verbund
//BRT20100902 <====
      SPA   En06; 
ILO6: NOP   0; 
      L     "MAL1S032Par".Par.ParIntProcess.Speed_ILO_Fan; 
En06: NOP   0; 
      L     100; 
      *I    ; 
      T     #A_3125_TSC01_COM.OutValuePC; // xxx.xx%
      T     #A_3126_TSC01_COM.OutValuePC; // xxx.xx%

NETWORK
TITLE =Interlocking WayOk

//BRT08072010 - Interlocking für Schliessen der Jalousien!
      U(    ; 
      L     #A_3125_TSC01_COM.InValuePC; 
      L     1000; // 10%
      <I    ; 
      )     ; 
      U(    ; 
      L     #A_3126_TSC01_COM.InValuePC; 
      L     1000; // 10%
      <I    ; 
      )     ; 
      =     #FanStandstill; 

//JV290710 - Interlocking für Öffnen der Kaltluft-Jalousien!
      U(    ; 
      L     #A_3125_TSC01_COM.InValuePC; 
      L     7500; // 75%
      <I    ; 
      )     ; 
      U(    ; 
      L     #A_3126_TSC01_COM.InValuePC; 
      L     7500; // 75%
      <I    ; 
      )     ; 
      =     "G271M".FanSpeedReduced; 

NETWORK
TITLE =Interlocking Autostart

      CLR   ; 
      R     #Gr.OutAutostart; 
NETWORK
TITLE =Interlocking Enable

      U(    ; 
//--------------------- Ein-Hordenbetrieb ----------------------------//
      U     "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      UN    "MAL1S033Par".Par.FlagProcess.ProcessActive; 
// Luft-Eintritt
      U(    ; 
      U     "G277M".A_3140_MXZ11.StHN; // Louver Air Heater Kilningbox 1
      U     "G277M".A_3141_MXZ11.StHN; // Louver Air Heater Kilningbox 1
      O     ; 
      UN    "G272M".A_3150_MXZ11.StLN; // Fresh Air Flap Kilningbox 1
      UN    "G272M".A_3151_MXZ11.StLN; // Fresh Air Flap Kilningbox 1
      UN    "G272M".A_3152_MXZ11.StLN; // Fresh Air Flap Kilningbox 1
      UN    "G272M".A_3153_MXZ11.StLN; // Fresh Air Flap Kilningbox 1
      )     ; 
// Luft-Austritt
      U(    ; 
      UN    "G278M".A_3145_MXZ11.StLN; // Exhaust Air Kilningbox 1
      UN    "G278M".A_3146_MXZ11.StLN; // Exhaust Air Kilningbox 1
      O     ; 
      UN    "G273M".A_3135_MXZ11.StLN; // Recycling Air Flap Kilningbox 1
      UN    "G273M".A_3136_MXZ11.StLN; // Recycling Air Flap Kilningbox 1
      )     ; 
      O     ; 
//---------------------- Zwei-Hordenbetrieb --------------------------//
      U     "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      U     "MAL1S033Par".Par.FlagProcess.ProcessActive; 
// Luft-Eintritt
      U(    ; 
      U     "G277M".A_3140_MXZ11.StHN; // Louver Air Heater Kilningbox 1
      U     "G277M".A_3141_MXZ11.StHN; // Louver Air Heater Kilningbox 1
      O     ; 
      U     "G277M".A_3140_MXZ11.StLN; // Louver Air Heater Kilningbox 1
      U     "G277M".A_3141_MXZ11.StLN; // Louver Air Heater Kilningbox 1
      U     "G299M".A_3236_MXZ11.StHN; // Louver Air Heater Kilningbox 2
      U     "G273M".A_3135_MXZ11.StHN; // Recycling Air Flap Kilningbox 1
      O     ; 
      UN    "G272M".A_3150_MXZ11.StLN; // Fresh Air Flap Kilningbox 1
      UN    "G272M".A_3151_MXZ11.StLN; // Fresh Air Flap Kilningbox 1
      UN    "G272M".A_3152_MXZ11.StLN; // Fresh Air Flap Kilningbox 1
      UN    "G272M".A_3153_MXZ11.StLN; // Fresh Air Flap Kilningbox 1
      )     ; 
// Luft-Austritt
      U(    ; 
      U     "G278M".A_3145_MXZ11.StHN; // Exhaust Air Kilningbox 1
      U     "G278M".A_3146_MXZ11.StHN; // Exhaust Air Kilningbox 1
      O     ; 
      UN    "G278M".A_3145_MXZ11.StLN; // Exhaust Air Kilningbox 1
      UN    "G278M".A_3146_MXZ11.StLN; // Exhaust Air Kilningbox 1
      UN    "G273M".A_3135_MXZ11.StLN; // Recycling Air Flap Kilningbox 1
      UN    "G273M".A_3136_MXZ11.StLN; // Recycling Air Flap Kilningbox 1
      O     ; 
      U     "G273M".A_3136_MXZ11.StHN; // Recycling Air Flap Kilningbox 1
      UN    "G273M".A_3135_MXZ11.StLN; // Recycling Air Flap Kilningbox 1
      UN    "G299M".A_3235_MXZ11.StLN; // Recycling Air Flap Kilningbox 2
      O     ; 
// 100% Umluft-Betrieb
      U     "G273M".A_3136_MXZ11.StHN; // Recycling Air Flap Kilningbox 1
      U(    ; 
      O     "G273M".A_3135_MXZ11.StHN; // Recycling Air Flap Kilningbox 1
      O     "G299M".A_3235_MXZ11.StHN; // Recycling Air Flap Kilningbox 2
      )     ; 
      )     ; 
      )     ; 
      UN(   ; //JV080810 ========> 
      U     "G278M".A_3145_MXZ11.StFault; // Exhaust Air Kilningbox 2
      U     "G278M".A_3146_MXZ11.StFault; // Exhaust Air Kilningbox 2
      U     "G273M".A_3135_MXZ11.StFault; // Recycling Air Flap Kilningbox 2
      U     "G273M".A_3136_MXZ11.StFault; // Recycling Air Flap Kilningbox 2
      )     ; //JV080810 <========
      L     S5T#15S; 
      SA    T    371; // Überbrückung bei Stellungswechsel der Klappen

      U     T    371; 
      O(    ; 
      L     "MAL1S032Par".Ilo.ILOMode; 
      L     4; 
      ==I   ; 
      U     #Gr.InManual; 
      U     "G273M".A_3135_MXZ11.StHN; // Recycling Air Flap Kilningbox 1
      U     "G273M".A_3136_MXZ11.StHN; // Recycling Air Flap Kilningbox 1
      )     ; 
      O(    ; 
      L     "MAL1S032Par".Ilo.ILOMode; 
      L     5; 
      ==I   ; 
      U     #Gr.InManual; 
      UN(   ; 
      U     "G277M".A_3140_MXZ11.StLN; // Louver Air Heater Kilningbox 1
      U     "G277M".A_3141_MXZ11.StLN; // Louver Air Heater Kilningbox 1
      U     "G278M".A_3145_MXZ11.StLN; // Exhaust Air Kilningbox 1
      U     "G278M".A_3146_MXZ11.StLN; // Exhaust Air Kilningbox 1
      U     "G273M".A_3135_MXZ11.StLN; // Recycling Air Flap Kilningbox 1
      U     "G273M".A_3136_MXZ11.StLN; // Recycling Air Flap Kilningbox 1
      )     ; 
      )     ; 
      U     "G276M".A_3191_BPS21.StTrue; // HEAT EXCHANGE PRESSURE < 15mbar
      U     #Gr.InEnableGr; 
      =     #Gr.OutEnableEl; 

NETWORK
TITLE =Interlocking Next

      U     #Gr.CmdNext; 
      U     #Gr.OutAutostart; 
      UN    #Gr.OutNewWay; 
      =     #tmpCmdNext; 

NETWORK
TITLE =Interlocking Local / HWStop

      U     "PLC_SystemDB".GlobalFlags.Kilning_1; 
      =     #tmpHWStop; 

      U     #Gr.ModePassive; 
      O     #tmpHWStop; 
      =     #tmpLocal; 

NETWORK
TITLE =Interlocking Start Warning

      NOP   0; 
//- Restart after fault --
      U     #Gr.CmdFaultReset; 
      U     #Gr.VarFault; 
      S     "SYS_StartWarningDB".Area[10].CmdResetStartWarning; 
//- Run Timer --
      AUF   "G271M"; 
      CALL "GCS7_StartWarning" , "SYS_StartWarningDB" (
           InModeHigher             := FALSE,
           InArea                   := 10,
           InEnable                 := #Gr.OutEnableEl,
           InLocal                  := #tmpLocal,
           InStarted                := #Gr.VarStarted,
           InCmdNext                := #tmpCmdNext,
           OutCmdNext               := #tmpCmdNext,
           OutStartwarning          := #Gr.VarStartwarning);
//- Store Startwarning done  --
//- Reset Autostart --
      U     "SYS_StartWarningDB".TmpArea.OutStartReleaseRunningL; 
      FN    #EdgeStartRelease; 
      U     #Gr.VarFault; 
      R     #Gr.OutAutostart; 
NETWORK
