      U     "PLC_DPInfo".Master[1].SlavesError[74]; 
      =     #A_2711_BZS46.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2711_BZS46 ;
// 
//-Write outputs---
NETWORK
TITLE ==A-2711-QYS45 : TRAVELING DRIVE SAFETY STRIGHT SIDE

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2711_QYS45.InLocal; 
// 
//-Monitoring---
      SET   ; 
      =     #A_2711_QYS45.InMonTrue; 
      CLR   ; 
      =     #A_2711_QYS45.InMonFalse; 
// 
//-PreAlarm---
      CLR   ; 
      =     #A_2711_QYS45.InPreAlarmTrue; 
      CLR   ; 
      =     #A_2711_QYS45.InPreAlarmFalse; 
// 
//-Additional commands---
//      CLR
//      S     #A_2711_QYS45.CmdRunTimeOutTime
// 
//-Read inputs---
      UN    "A_2711_QYS45_I1"; 
      =     #A_2711_QYS45.InpTrue; 
      U     "PLC_SystemDB".GlobalFlags.Germinating_7; 
      =     #A_2711_QYS45.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[74]; 
      =     #A_2711_QYS45.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2711_QYS45 ;
// 
//-Write outputs---
NETWORK
TITLE ==A-2711-QYS43 : TRAVELING DRIVE SAFETY STLEFT SIDE

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2711_QYS43.InLocal; 
// 
//-Monitoring---
      SET   ; 
      =     #A_2711_QYS43.InMonTrue; 
      CLR   ; 
      =     #A_2711_QYS43.InMonFalse; 
// 
//-PreAlarm---
      CLR   ; 
      =     #A_2711_QYS43.InPreAlarmTrue; 
      CLR   ; 
      =     #A_2711_QYS43.InPreAlarmFalse; 
// 
//-Additional commands---
//      CLR
//      S     #A_2711_QYS43.CmdRunTimeOutTime
// 
//-Read inputs---
      UN    "A_2711_QYS43_I1"; 
      =     #A_2711_QYS43.InpTrue; 
      U     "PLC_SystemDB".GlobalFlags.Germinating_7; 
      =     #A_2711_QYS43.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[74]; 
      =     #A_2711_QYS43.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2711_QYS43 ;
// 
//-Write outputs---
NETWORK
TITLE ==A-2711-MXZ41MA : TRAVELING DRIVE

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2711_MXZ41MA.Ma.InLocal; 

//-Interlocking---
      O(    ; 
      U     "MAL1S023".ToASW.InfDataReady; 
      U     "MAL1S023".PCRead.Step.StReady; 
      U     "MAL1S023Par".Par.FlagProcess.ProcessActive; 
      U     "MAL1S023Par".Par.FlagFunction_1.TurningRight; 
      UN    #A_2711_BZS44.InpTrue; // stop discharge side
      UN    #Gr.InManual; 
      UN    #Gr.InOutAutostartMan; 
      )     ; 
      O(    ; 
      U     "MAL1S023".ToASW.InfDataReady; 
      U     "MAL1S023".PCRead.Step.StEmptying; 
      U     "MAL1S023Par".Par.FlagFunction_1.StepUnloading; // Step unloading
      U     "MAL1S023Par".Par.FlagFunction_1.UnloadingStarted; 
      U     "MAL1S023".ToASW.spare34; // infNextSectionFeeding
      U     #A_2711_BZS44.StFalse; // stop discharge side
      UN    #Gr.InManual; 
      UN    #Gr.InOutAutostartMan; 
      )     ; 
      O(    ; 
      U     #Gr.InManual; 
      U     #Gr.InOutAutostartMan; 
      U(    ; 
      O(    ; 
      U(    ; 
      O     "MAL1S023".PCRead.Step.StIdling; 
      O     "MAL1S023".PCRead.Step.StPassive; 
      )     ; 
      U     #USW14; // ILO-cmd run fwd
      )     ; 
      O(    ; 
      UN(   ; 
      O     "MAL1S023".PCRead.Step.StIdling; 
      O     "MAL1S023".PCRead.Step.StPassive; 
      )     ; 
      U     #USW14; // ILO-cmd run fwd
      U     "G206M".Gr.OutEnablePrevGr; 
      )     ; 
      UN    #A_2711_BZS44.InpTrue; // stop discharge side
      )     ; 
      )     ; 
      U     #Gr.OutEnableEl; 
      U     #A_2711_QYS43.StTrue; // emergency stop discharge side
      U     #A_2711_QYS45.StTrue; // bumper discharge side
      UN    "MAL1S023Par".Par.FlagFunction_1.StepLoading; // locked if loading
      UN    "MAL1S023Par".Par.FlagFunction_1.StepCleaning; // Step cleaning
      =     #A_2711_MXZ41MA.Ma.InEnableFwd; 

      O(    ; 
      U     "MAL1S023".ToASW.InfDataReady; 
      U(    ; // JV210808 to finish last turning
      O     "MAL1S023".PCRead.Step.StReady; 
      O     "MAL1S023".PCRead.Step.StEmptying; 
      )     ; 
      U(    ; // JV210808 to finish last turning
      O     "MAL1S023Par".Par.FlagProcess.ProcessActive; 
      O     "MAL1S023Par".Par.FlagProcess.ProcessEnd; 
      )     ; 
      U     "MAL1S023Par".Par.FlagFunction_1.TurningLeft; 
      UN    #Gr.InManual; 
      UN    #Gr.InOutAutostartMan; 
      )     ; 
      O(    ; // run back to cleaning pos
      U     "MAL1S023".ToASW.InfDataReady; 
      U     "MAL1S023".PCRead.Step.StEmptied; 
      U     "MAL1S023Par".Par.FlagFunction_1.FinishUnloading; 
      U     "MAL1S023Par".Par.FlagMachine_0.CmdTurnRevAfterUnloading; 
      UN    "MAL1S023Par".Par.FlagMachine_0.CleaningAfterUnloading; 
      UN    #Gr.InManual; 
      UN    #Gr.InOutAutostartMan; 
      )     ; 
      O(    ; // run back to cleaning pos
      U     "MAL1S023".ToASW.InfDataReady; 
      U     "MAL1S023".PCRead.Step.StEmptied; 
      U     "MAL1S023Par".Par.FlagFunction_1.FinishCleaning; 
      UN    #Gr.InManual; 
      UN    #Gr.InOutAutostartMan; 
      )     ; 

      O(    ; 
      U     #Gr.InManual; 
      U     #Gr.InOutAutostartMan; 
      U(    ; 
      O(    ; 
      U(    ; 
      O     "MAL1S023".PCRead.Step.StIdling; 
      O     "MAL1S023".PCRead.Step.StPassive; 
      )     ; 
      U     #USW15; // ILO-cmd run rev
      )     ; 
      O(    ; 
      UN(   ; 
      O     "MAL1S023".PCRead.Step.StIdling; 
      O     "MAL1S023".PCRead.Step.StPassive; 
      )     ; 
      U     #USW15; // ILO-cmd run rev
      U     "G206M".Gr.OutEnablePrevGr; 
      )     ; 
      )     ; 
      )     ; 
      U     #Gr.OutEnableEl; 
      UN    #A_2711_BZS46.InpTrue; // stop right side
      U     #A_2711_QYS45.StTrue; // emergency stop fan side
      U     #A_2711_QYS43.StTrue; // bumper fan side
      =     #A_2711_MXZ41MA.Ma.InEnableRev; 
// 
      U     #tmpCmdNext; 
      S     #A_2711_MXZ41MA.Ma.CmdNext; 
// 
      SET   ; 
      =     #A_2711_MXZ41MA.Ma.InInterlocked; 
// 
      UN    "PLC_DPInfo".Master[1].SlavesError[73]; 
      =     #A_2711_MXZ41MA.Ma.InPowerOk; 
// 
      U     #A_2711_MXZ41MA.Ma.InPowerOk; 
      L     S5T#15S; // Zeit zum Initialisierung
      SE    T    206; 
      U     T    206; 
      =     #A_2711_MXZ41MA.Ma.InPowerOkDelayDone; 
// 
//      UN    #Gr.InOutAutostartMan
//      U     "MAL1S023".ToASW.InfDataReady
//      U     "MAL1S023".PCRead.Step.StActive
//      U     "MAL1S023Par".Par.FlagFunction_1.StepLoading
////    UN    "MAL1S023Par".Par.FlagFunction_1.LoadEnd;
//      =     #A_2711_MXZ41MA.Ma.InLoading
// 
      UN    #Gr.InOutAutostartMan; 
      =     #A_2711_MXZ41MA.Ma.InAdjusting; 
      R     #A_2711_MXZ41MA.Ma.Par.ParEndless; 
// 
// Set Lap counter to "0"
      U     "MAL1S023".PCRead.Step.StEmptying; 
