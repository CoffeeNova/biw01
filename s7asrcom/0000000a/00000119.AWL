BEGIN
NETWORK
TITLE =



NETWORK
TITLE =Read command from group control
//Parameter
      SET   ; 
      S     "GCS7_GrCtrlDB".ParMGR[225]; 
      R     "GCS7_GrCtrlDB".ParWGR[225]; 
      R     "GCS7_GrCtrlDB".ParGGR[225]; 
      R     "GCS7_GrCtrlDB".ParFeed[225]; 
      R     "GCS7_GrCtrlDB".ParShare[225]; 
      S     #Gr.Par.ParMgr; 
      R     #Gr.Par.ParWgr; 
      R     #Gr.Par.ParGgr; 
//Command
      U     "GCS7_GrCtrlDB".OutEnable[225]; 
      =     #Gr.InEnableGr; 
      U     "GCS7_GrCtrlDB".OutOccupied[225]; 
      =     #Gr.InOccupied; 
      U     "GCS7_GrCtrlDB".CmdFaultReset[225]; 
      R     "GCS7_GrCtrlDB".CmdFaultReset[225]; 
      S     #Gr.CmdFaultReset; 
      U     "GCS7_GrCtrlDB".CmdNext[225]; 
      R     "GCS7_GrCtrlDB".CmdNext[225]; 
      S     #Gr.CmdNext; 
      U     "GCS7_GrCtrlDB".CmdManualOff[225]; 
      UN    #Gr.InManual; 
      S     #Gr.CmdManualOff; 
      U     "GCS7_GrCtrlDB".CmdManualOff[225]; 
      R     "GCS7_GrCtrlDB".CmdManualOff[225]; 

//      U     "GCS7_GrCtrlDB".CmdManualOff[225]
//      O     "GCS7_GrCtrlDB".OutOccupied[225]
//      R     "GCS7_GrCtrlDB".CmdManualOff[225]
//      S     #Gr.CmdManualOff

      U     "GCS7_GrCtrlDB".CmdContinue[225]; 
      R     "GCS7_GrCtrlDB".CmdContinue[225]; 
      S     #Gr.CmdContinue; 
//Init Line Number
      U     "GCS7_GrCtrlDB".ParShare[225]; 
      SPBN  WE00; 
      L     0; 
      T     #Gr.InLine; 
WE00: NOP   0; 
NETWORK
TITLE =Call Methode Control
// 1. Handle Interface PCControl      
// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
// 3. Reset global flags of the group
      CALL #Gr (
           FcCode                   := 1);
NETWORK
TITLE =Restart Idling Time

      L     #Gr.Par.ParIdling; 
      L     2; 
      >=I   ; 
      SPB   IdTm; 
      T     #Gr.Par.ParIdling; 
IdTm: NOP   0; 
      CLR   ; 
      S     #Gr.CmdRestartIdling; 
NETWORK
TITLE =Prepare global flags



NETWORK
TITLE =Set Pattern (Target position)



      L     0; // time change to true
      SPA   LD06; 

      L     20; // time change to true
      U     "MAL1S023Par".Par.FlagFunction_1.StepUnloading; // Step unloading
      U     "MAL1S023Par".Par.FlagFunction_1.UnloadingStarted; 
      SPBN  LD06; 
      L     6000; // time change to true
LD06: T     "G225M".A_2711_BZS44.Par.ParDelayTrue; 

// set speed
      U     "MAL1S023".ToASW.InfDataReady; 
      U     "MAL1S023".PCRead.Step.StEmptied; 
      U     "MAL1S023Par".Par.FlagFunction_1.FinishCleaning; 
      U(    ; 
      L     "MAL1S023Par".Ilo.ILOMode; // JV 290808
      L     9; 
      <>I   ; 
      )     ; 
      L     "MAL1S023Par".Par.ParIntProcess.CopyLoadindSpeed2; // JV 290808
      L     100; 
      *I    ; 
      SPB   SP06; 
      U     "MAL1S023".ToASW.InfDataReady; 
      U     "MAL1S023".PCRead.Step.StEmptying; 
      U     "MAL1S023Par".Par.FlagFunction_1.StepUnloading; // Step unloading
      U(    ; 
      L     "MAL1S023Par".Ilo.ILOMode; // JV 290808
      L     9; 
      <>I   ; 
      )     ; 
      L     "MAL1S023Par".RecipeHeader.Unloading.UnLoadingSpeed_1; // JV 290808
      L     100; 
      *I    ; 
      SPB   SP06; 
      U(    ; // - When machine is going back to cleaning position
      U     "MAL1S023".ToASW.InfDataReady; 
      U     "MAL1S023".PCRead.Step.StEmptied; 
      U     "MAL1S023Par".Par.FlagFunction_1.FinishUnloading; 
      U     "MAL1S023Par".Par.FlagMachine_0.CmdTurnRevAfterUnloading; 
      UN    "MAL1S023Par".Par.FlagMachine_0.CleaningAfterUnloading; 
      UN    #Gr.InManual; 
      UN    #Gr.InOutAutostartMan; 
      )     ; 
      L     4000; // 40,00 % Speed to go back to cleaning position
      SPB   SP06; 
      L     2500; // 25% = 12.5Hz
SP06: T     "G225M".A_2711_MXZ41MA.Ma.InSpeed; 
      U(    ; 
      L     "MAL1S023Par".Ilo.ILOMode; 
      L     8; 
      ==I   ; 
      )     ; 
      SPBN  jum1; 
      L     "MAL1S023Par".Par.ParIntProcess.Speed_ILO_FloorRotating; //dyk ILO
      L     100; 
      *I    ; 
      T     "G225M".A_2711_MXZ41MA.Ma.InSpeed; 
jum1: NOP   0; 





NETWORK
TITLE =Interlocking WayOk



NETWORK
TITLE =Interlocking Autostart

      UN    "G226M".Gr.OutAutostart; 
      U     "GCS7_GrCtrlDB".OutEnable[226]; //dyk
      O     ; 
      UN    "G229M".Gr.OutAutostart; 
      U     "GCS7_GrCtrlDB".OutEnable[229]; //dyk
      O     ; 
      UN    "G005G".Gr.OutAutostart; 
      U     "GCS7_GrCtrlDB".OutEnable[200]; //dyk
      R     #Gr.OutAutostart; 
// NOP 0
      CLR   ; 
      CLR   ; 
// C-Profilschutz

NETWORK
TITLE =Interlocking Enable

      U     "MAL1S023Par".Par.FlagFunction_1.StepUnloading; 
      U     "G226M".Gr.OutEnablePrevGr; 
      U     "G229M".Gr.OutEnablePrevGr; 
      ON    "MAL1S023Par".Par.FlagFunction_1.StepUnloading; 
      O(    ; 
      U     "MAL1S023".PCRead.Step.StEmptied; 
      U     "MAL1S023Par".Par.FlagFunction_1.FinishUnloading; 
      U     "MAL1S023Par".Par.FlagMachine_0.CmdTurnRevAfterUnloading; 
      )     ; 
//-turning---
      U(    ; 
      U     "MAL1S023Par".Par.FlagProcess2.Turning1WithoutSpraying; 
      O     "MAL1S023Par".Par.FlagProcess2.Turning2WithoutSpraying; 
      U     "G229M".Gr.OutEnablePrevGr; 
      O     ; 
      UN    "MAL1S023Par".Par.FlagProcess2.Turning1WithoutSpraying; 
      UN    "MAL1S023Par".Par.FlagProcess2.Turning2WithoutSpraying; 
      UN    #Gr.InManual; 
      )     ; 
//-Spraying---
      U(    ; 
      U     "MAL1S023Par".Par.FlagProcess2.Turning1WithSpraying; 
      O     "MAL1S023Par".Par.FlagProcess2.Turning2WithSpraying; 
      UN    "G233M".Gr.VarFault; // spraying valve
////      UN    "GxxxM".Gr.VarFault         // spraying water pump --> Customer!!!

      U     "G229M".Gr.OutEnablePrevGr; 
      O     ; 
      UN    "MAL1S023Par".Par.FlagProcess2.Turning1WithSpraying; 
      UN    "MAL1S023Par".Par.FlagProcess2.Turning2WithSpraying; 
      UN    #Gr.InManual; 
      )     ; 
//-Dosing---
      U(    ; 
      U(    ; 
      U     "MAL1S023Par".Par.FlagProcess2.Turning1WithDosing; 
      O     "MAL1S023Par".Par.FlagProcess2.Turning2WithDosing; 
      U     "G081M".Gr.OutEnablePrevGr; // Doser Gibberelin 5402
      U     "G229M".Gr.OutEnablePrevGr; 
      )     ; 
      O(    ; 
      U     "MAL1S023Par".Par.FlagProcess2.Turning1WithDosing; 
      O     "MAL1S023Par".Par.FlagProcess2.Turning2WithDosing; 
      UN    "G081M".Gr.InOccupied; // Doser Gibberelin 5402
      U     "G206M".Gr.OutEnablePrevGr; 
      )     ; 
      O(    ; 
      UN    "MAL1S023Par".Par.FlagProcess2.Turning1WithDosing; 
      UN    "MAL1S023Par".Par.FlagProcess2.Turning2WithDosing; 
      UN    #Gr.InManual; 
      )     ; 
      )     ; 
      O(    ; 
      U     #Gr.InManual; 
      U     #Gr.InOutAutostartMan; 
      U     "G229M".Gr.OutEnablePrevGr; 
      )     ; 
      UN(   ; //dyk 081029  If the Wall is NOT in the bottom position, the uncoupled travelling machine can NOT run.
      U     "G223M".A_2711_BZS22.InpTrue; //COUPLING DRIVE UNLOCKED GM2 
      U     "G223M".A_2711_BZS26.InpTrue; //COUPLING DRIVE UNLOCKED GM2 
      U     "G223M".A_2711_BZS32.InpTrue; //COUPLING DRIVE UNLOCKED GM2 
      U     "G223M".A_2711_BZS36.InpTrue; //COUPLING DRIVE UNLOCKED GM2 
      UN    "G222M".A_2715_BZS22.InpTrue; //LIFTING DEVICE DOWN GM2
      )     ; 
      U     #Gr.InEnableGr; 
      =     #Gr.OutEnableEl; 

NETWORK
TITLE =Interlocking Next

      U     #Gr.CmdNext; 
      U     #Gr.OutAutostart; 
      UN    #Gr.OutNewWay; 
      =     #tmpCmdNext; 

NETWORK
TITLE =Interlocking Local / HWStop

      U     "PLC_SystemDB".GlobalFlags.Germinating_7; 
      =     #tmpHWStop; 

      U     #Gr.ModePassive; 
      O     #tmpHWStop; 
      =     #tmpLocal; 

NETWORK
TITLE =Interlocking Start Warning

      NOP   0; 
//- Restart after fault --
      U     #Gr.CmdFaultReset; 
      U     #Gr.VarFault; 
      S     "SYS_StartWarningDB".Area[8].CmdResetStartWarning; 
//- Run Timer --
      AUF   "G225M"; 
      CALL "GCS7_StartWarning" , "SYS_StartWarningDB" (
           InModeHigher             := FALSE,
           InArea                   := 8,
           InEnable                 := #Gr.OutEnableEl,
           InLocal                  := #tmpLocal,
           InStarted                := #Gr.VarStarted,
           InCmdNext                := #tmpCmdNext,
           OutCmdNext               := #tmpCmdNext,
           OutStartwarning          := #Gr.VarStartwarning);
//- Store Startwarning done  --
//- Reset Autostart --
      U     "SYS_StartWarningDB".TmpArea.OutStartReleaseRunningL; 
      FN    #EdgeStartRelease; 
      U     #Gr.VarFault; 
      R     #Gr.OutAutostart; 
NETWORK
TITLE ==A-2711-BZS44 : TRAVELING DRIVE STOP LEFT SIDE

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2711_BZS44.InLocal; 
// 
//-Monitoring---
      CLR   ; 
      =     #A_2711_BZS44.InMonTrue; 
//      CLR   
      U     #A_2711_BZS44.InpTrue; // both signals together is a fault
      U     #A_2711_BZS46.InpTrue; 
      =     #A_2711_BZS44.InMonFalse; 
// 
//-PreAlarm---
      CLR   ; 
      =     #A_2711_BZS44.InPreAlarmTrue; 
      CLR   ; 
      =     #A_2711_BZS44.InPreAlarmFalse; 
// 
//-Additional commands---
//      CLR
//      S     #A_2711_BZS44.CmdRunTimeOutTime
// 
//-Read inputs---
      U     "A_2711_BZS44_I1"; 
      =     #A_2711_BZS44.InpTrue; 
      U     "PLC_SystemDB".GlobalFlags.Germinating_7; 
      =     #A_2711_BZS44.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[74]; 
      =     #A_2711_BZS44.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2711_BZS44 ;
// 
//-Write outputs---
NETWORK
TITLE ==A-2711-BZS46 : TRAVELING DRIVE STOP RIGHT SIDE

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      UN    "MAL1S023Par".Par.FlagFunction_1.StepLoading; 
      =     #A_2711_BZS46.InLocal; 
// 
//-Monitoring---
      CLR   ; 
      =     #A_2711_BZS46.InMonTrue; 
//      CLR   
      U     #A_2711_BZS46.InpTrue; // both signals together is a fault
      U     #A_2711_BZS44.InpTrue; 
      =     #A_2711_BZS46.InMonFalse; 
// 
//-PreAlarm---
      CLR   ; 
