AUTHOR : KR
FAMILY : USW
VERSION : 9.9


VAR
  Gr : "GCS7_GrpCtrlSub";	//Standard interface to group
  InOutValue1 : DINT ;	//Data for user
  InOutValue2 : DINT ;	//Data for user
  OutWayConflict : BOOL ;	//Data for user
  OutSelectionOk : BOOL ;	//Data for user
  CmdNewValue : BOOL ;	//Data for user
  CmdNewJob : BOOL ;	//Data for user
  OutCodeOK : BOOL ;	//Data for user
  InEndOfJob : BOOL ;	//Data for user
  staWayOk : BOOL ;	//Data for user
  EdgeStartRelease : BOOL ;	//Data for user
  USW8 : BOOL ;	//Data for user
  USW9 : BOOL ;	//Data for user
  USW10 : BOOL ;	//Data for user
  USW11 : BOOL ;	//Data for user
  USW12 : BOOL ;	//Data for user
  USW13 : BOOL ;	//Data for user
  USW14 : BOOL ;	//Data for user
  USW15 : BOOL ;	//Data for user
  A_1156_MXZ11 : "GCS7_E_Motor";	// Motor 
END_VAR
VAR_TEMP
  tmpEnable : BOOL ;	
  tmpFlag : BOOL ;	
  tmpCmdNext : BOOL ;	
  tmpLocal : BOOL ;	
  tmpAspOk : BOOL ;	
  tmpHWStop : BOOL ;	
  tmpInstanzDBNo : INT ;	
END_VAR
BEGIN
NETWORK
TITLE =



NETWORK
TITLE =Read command from group control
//Parameter
      SET   ; 
      S     "GCS7_GrCtrlDB".ParMGR[20]; 
      R     "GCS7_GrCtrlDB".ParWGR[20]; 
      R     "GCS7_GrCtrlDB".ParGGR[20]; 
      R     "GCS7_GrCtrlDB".ParFeed[20]; 
      R     "GCS7_GrCtrlDB".ParShare[20]; 
      S     #Gr.Par.ParMgr; 
      R     #Gr.Par.ParWgr; 
      R     #Gr.Par.ParGgr; 
//Command
      U     "GCS7_GrCtrlDB".OutEnable[20]; 
      =     #Gr.InEnableGr; 
      U     "GCS7_GrCtrlDB".OutOccupied[20]; 
      =     #Gr.InOccupied; 
      U     "GCS7_GrCtrlDB".CmdFaultReset[20]; 
      R     "GCS7_GrCtrlDB".CmdFaultReset[20]; 
      S     #Gr.CmdFaultReset; 
      U     "GCS7_GrCtrlDB".CmdNext[20]; 
      R     "GCS7_GrCtrlDB".CmdNext[20]; 
      S     #Gr.CmdNext; 
      U     "GCS7_GrCtrlDB".CmdManualOff[20]; 
//      O     "GCS7_GrCtrlDB".OutOccupied[20]
      R     "GCS7_GrCtrlDB".CmdManualOff[20]; 
      S     #Gr.CmdManualOff; 
      U     "GCS7_GrCtrlDB".CmdContinue[20]; 
      R     "GCS7_GrCtrlDB".CmdContinue[20]; 
      S     #Gr.CmdContinue; 
//Init Line Number
      U     "GCS7_GrCtrlDB".ParShare[20]; 
      SPBN  WE00; 
      L     0; 
      T     #Gr.InLine; 
WE00: NOP   0; 
NETWORK
TITLE =Call Methode Control
// 1. Handle Interface PCControl      
// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
// 3. Reset global flags of the group
      CALL #Gr (
           FcCode                   := 1);
NETWORK
TITLE =Restart Idling Time

      CLR   ; 
      S     #Gr.CmdRestartIdling; 
NETWORK
TITLE =Prepare global flags



NETWORK
TITLE =Set Pattern (Target position)



NETWORK
TITLE =Interlocking WayOk



NETWORK
TITLE =Interlocking Autostart

      CLR   ; 
      R     #Gr.OutAutostart; 
NETWORK
TITLE =Interlocking Enable

      U     #Gr.InEnableGr; 
      CLR   ; // No automatic --> only manual
      =     #Gr.OutEnableEl; 

NETWORK
TITLE =Interlocking Next

      U     #Gr.CmdNext; 
      U     #Gr.OutAutostart; 
      UN    #Gr.OutNewWay; 
      =     #tmpCmdNext; 

NETWORK
TITLE =Interlocking Local / HWStop

      U     "PLC_SystemDB".HardwareStop.Steeping; 
      =     #tmpHWStop; 

      U     #Gr.ModePassive; 
      =     #tmpLocal; 

NETWORK
TITLE ==A-1156-MXZ11 : COMPRESSED AIR ST 21 - 24

      NOP   0; 
//-InLocal--- 
      U     #tmpLocal; 
      =     #A_1156_MXZ11.InLocal; 
//
//-Interlocking--- 
      U     #Gr.OutEnableEl; 
      =     #A_1156_MXZ11.InEnableFwd; 
// 
      U     #tmpCmdNext; 
      S     #A_1156_MXZ11.CmdNext; 
// 
      U     #Gr.OutSeqStop; 
      =     #A_1156_MXZ11.InSeqStop; 
// 
//-Read inputs---
      U     "A_1156_MXZ11_I1"; 
      =     #A_1156_MXZ11.InpRunFwd; 
      U     "PLC_SystemDB".HardwareStop.Steeping; 
      =     #A_1156_MXZ11.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[5]; 
      =     #A_1156_MXZ11.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_1156_MXZ11 ;
// 
//-Write outputs---
      U     #A_1156_MXZ11.OutpRunFwd; 
      =     "A_1156_MXZ11_O1"; 
NETWORK
TITLE =Test Fault

      U     #A_1156_MXZ11.StFault; 
      =     #Gr.VarFault; 
NETWORK
TITLE =Test Warning

      CLR   ; 
      =     #Gr.OutWarning; 
NETWORK
TITLE =Test Stopped

      U     #A_1156_MXZ11.StStopped; 
      =     #Gr.VarStopped; 
NETWORK
TITLE =Test Started

      U     #A_1156_MXZ11.StStartedFwd; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStarted; 
NETWORK
TITLE =Enable Previous Group

      U     #Gr.VarStarted; 
      =     #Gr.OutEnablePrevGr; 
NETWORK
TITLE =Call Methode State
//1. Make State of the group: This function use the signals:
//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
//2.Reset Commands to group: e.g.CmdFaultReset
      CALL #Gr (
           FcCode                   := 2);
NETWORK
TITLE =Init

      SET   ; 
      R     #CmdNewValue; 
      R     #CmdNewJob; 
NETWORK
TITLE =Write data to group control

      U     #Gr.StStopped; 
      =     "GCS7_GrCtrlDB".InStopped[20]; 
      U     #Gr.StStarted; 
      =     "GCS7_GrCtrlDB".InStarted[20]; 
      U     #Gr.StFault; 
      =     "GCS7_GrCtrlDB".InFault[20]; 
      U     #Gr.OutWarning; 
      =     "GCS7_GrCtrlDB".InWarning[20]; 
      U     #Gr.OutManual; 
      =     "GCS7_GrCtrlDB".InManual[20]; 
      U     #Gr.OutHWStop; 
      =     "GCS7_GrCtrlDB".InHWStop[20]; 
      U     #Gr.OutDPFault; 
      =     "GCS7_GrCtrlDB".InDPFault[20]; 
      U     #Gr.OutLoaded; 
      =     "GCS7_GrCtrlDB".InLoaded[20]; 
      U     #Gr.OutAutostart; 
      =     "GCS7_GrCtrlDB".InAutoStart[20]; 
      U     #Gr.VarStartwarning; 
      =     "GCS7_GrCtrlDB".InStartWarning[20]; 
NETWORK
TITLE =End of block

      BE    ; 

END_FUNCTION_BLOCK

