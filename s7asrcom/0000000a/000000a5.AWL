NETWORK
TITLE ==A-1113-QXV/BZS41/42 : AERATION AIR RING ST 3

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_1113_QXV_BZS41_42.InLocal; 
//
//-Interlocking---
      U     #Gr.OutEnableEl; 
      U     "G037M".EnableConeNozzles; 
      =     #A_1113_QXV_BZS41_42.InEnableHN; 
      NOT   ; 
      =     #A_1113_QXV_BZS41_42.InEnableLN; 
//
      SET   ; 
      S     #A_1113_QXV_BZS41_42.CmdNext; 
//
      U     #Gr.OutSeqStop; 
      =     #A_1113_QXV_BZS41_42.InSeqStop; 
// 
//-Read inputs---
      U     "A_1113_BZS41_I1"; 
      =     #A_1113_QXV_BZS41_42.InpLN; 
      U     "A_1113_BZS42_I1"; 
      =     #A_1113_QXV_BZS41_42.InpHN; 
      U     "PLC_SystemDB".HardwareStop.Steeping; 
      =     #A_1113_QXV_BZS41_42.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[6]; 
      =     #A_1113_QXV_BZS41_42.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_1113_QXV_BZS41_42 ;
// 
//-Write outputs---
      U     #A_1113_QXV_BZS41_42.OutpHN; 
      =     "A_1113_QXV42_O2"; 
NETWORK
TITLE ==A-1113-MXZ45/BZS47/48 : AERATION AIR TUBE ST 3

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_1113_MXZ45_BZS47_48.InLocal; 
//
//-Interlocking---
      U     #Gr.OutEnableEl; 
      U     "G037M".EnableHeinekenTube; 
      =     #A_1113_MXZ45_BZS47_48.InEnableHN; 
      NOT   ; 
      =     #A_1113_MXZ45_BZS47_48.InEnableLN; 
//
      SET   ; 
      S     #A_1113_MXZ45_BZS47_48.CmdNext; 
//
      U     #Gr.OutSeqStop; 
      =     #A_1113_MXZ45_BZS47_48.InSeqStop; 
// 
//-Read inputs---
      U     "A_1113_BZS47_I1"; 
      =     #A_1113_MXZ45_BZS47_48.InpLN; 
      U     "A_1113_BZS48_I1"; 
      =     #A_1113_MXZ45_BZS47_48.InpHN; 
// 1.1
      CLR   ; 
      =     #A_1113_MXZ45_BZS47_48.InpFaultDev; 
      U     "PLC_SystemDB".HardwareStop.Steeping; 
      =     #A_1113_MXZ45_BZS47_48.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[10]; 
      =     #A_1113_MXZ45_BZS47_48.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_1113_MXZ45_BZS47_48 ;
// 
//-Write outputs---
// 1.1
      U     #A_1113_MXZ45_BZS47_48.OutpLN; 
      =     "A_1113_MXZ45_O1"; 
      U     #A_1113_MXZ45_BZS47_48.OutpHN; 
      =     "A_1113_MXZ45_O2"; 
NETWORK
TITLE =Test Fault

      U     #A_1113_QXV_BZS41_42.StFault; 
      O     #A_1113_MXZ45_BZS47_48.StFault; 
      =     #Gr.VarFault; 
NETWORK
TITLE =Test Warning

      CLR   ; 
      =     #Gr.OutWarning; 
NETWORK
TITLE =Test Stopped

      U     #A_1113_QXV_BZS41_42.StLN; 
      U     #A_1113_MXZ45_BZS47_48.StLN; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStopped; 
NETWORK
TITLE =Test Started

      U     #A_1113_QXV_BZS41_42.StHN; 

      U     "G035M".EnableConeNozzles; 
      O(    ; 
      U     #A_1113_MXZ45_BZS47_48.StHN; 
      U     "G037M".EnableHeinekenTube; 
      )     ; 
      UN    #Gr.VarStopped; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStarted; 
NETWORK
TITLE =Enable Previous Group

      U     #Gr.VarStarted; 
      =     #Gr.OutEnablePrevGr; 
NETWORK
TITLE =Call Methode State
//1. Make State of the group: This function use the signals:
//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
//2.Reset Commands to group: e.g.CmdFaultReset
      CALL #Gr (
           FcCode                   := 2);
NETWORK
TITLE =Init

      SET   ; 
      R     #CmdNewValue; 
      R     #CmdNewJob; 
NETWORK
TITLE =Write data to group control

      U     #Gr.StStopped; 
      =     "GCS7_GrCtrlDB".InStopped[37]; 
      U     #Gr.StStarted; 
      =     "GCS7_GrCtrlDB".InStarted[37]; 
      U     #Gr.StFault; 
      =     "GCS7_GrCtrlDB".InFault[37]; 
      U     #Gr.OutWarning; 
      =     "GCS7_GrCtrlDB".InWarning[37]; 
      U     #Gr.OutManual; 
      =     "GCS7_GrCtrlDB".InManual[37]; 
      U     #Gr.OutHWStop; 
      =     "GCS7_GrCtrlDB".InHWStop[37]; 
      U     #Gr.OutDPFault; 
      =     "GCS7_GrCtrlDB".InDPFault[37]; 
      U     #Gr.OutLoaded; 
      =     "GCS7_GrCtrlDB".InLoaded[37]; 
      U     #Gr.OutAutostart; 
      =     "GCS7_GrCtrlDB".InAutoStart[37]; 
      U     #Gr.VarStartwarning; 
      =     "GCS7_GrCtrlDB".InStartWarning[37]; 
NETWORK
TITLE =End of block

      BE    ; 

END_FUNCTION_BLOCK

