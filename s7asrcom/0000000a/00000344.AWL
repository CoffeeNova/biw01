	      );
	      U(;
	      O     #A_19_S_H097Z1.StTrue; 
	      O     #A_19_S_H097Z1.StCtoFalse; 
	      );
	      UN  #Gr.VarFault; 
	      =     #Gr.VarStarted; 
	NETWORK
	TITLE =Enable Previous Group
	      U    #Gr.VarStarted;
	      =     #Gr.OutEnablePrevGr;
	NETWORK
	TITLE =Call Methode State
	//1. Make State of the group: This function use the signals:
	//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
	//2.Reset Commands to group: e.g.CmdFaultReset
	      CALL #Gr (
	           FcCode                   := 2);
	NETWORK
	TITLE =Init
	      SET; 
	      R    #CmdNewValue;
	      R    #CmdNewJob;
	NETWORK
	TITLE =Write data to group control
	      U   #Gr.StStopped;
	      =   GCS7_GrCtrlDB.InStopped[24];
	      U   #Gr.StStarted;
	      =   GCS7_GrCtrlDB.InStarted[24];
	      U   #Gr.StFault;
	      =   GCS7_GrCtrlDB.InFault[24];
	      U   #Gr.OutWarning;
	      =   GCS7_GrCtrlDB.InWarning[24];
	      U   #Gr.OutManual;
	      =   GCS7_GrCtrlDB.InManual[24];
	      U   #Gr.OutHWStop;
	      =   GCS7_GrCtrlDB.InHWStop[24];
	      U   #Gr.OutDPFault;
	      =   GCS7_GrCtrlDB.InDPFault[24];
	      U   #Gr.OutLoaded;
	      =   GCS7_GrCtrlDB.InLoaded[24];
	      U   #Gr.OutAutoStart;
