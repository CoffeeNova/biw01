AUTHOR : KR
FAMILY : USW
VERSION : 9.9


VAR
  Gr : "GCS7_GrpCtrlSub";	//Standard interface to group
  InOutValue1 : DINT ;	//Data for user
  InOutValue2 : DINT ;	//Data for user
  OutWayConflict : BOOL ;	//Data for user
  OutSelectionOk : BOOL ;	//Data for user
  CmdNewValue : BOOL ;	//Data for user
  CmdNewJob : BOOL ;	//Data for user
  OutCodeOK : BOOL ;	//Data for user
  InEndOfJob : BOOL ;	//Data for user
  staWayOk : BOOL ;	//Data for user
  EdgeStartRelease : BOOL ;	//Data for user
  USW8 : BOOL ;	//Data for user
  USW9 : BOOL ;	//Data for user
  USW10 : BOOL ;	//Data for user
  USW11 : BOOL ;	//Data for user
  USW12 : BOOL ;	//Data for user
  USW13 : BOOL ;	//Data for user
  USW14 : BOOL ;	//Data for user
  USW15 : BOOL ;	//Data for user
  A_2201_MXZ01MA : "A_2201_MXZ01MA_Code";	// Motor+6DI 
  A_2201_QXV21 : "GCS7_E_ValveControl";	// VLS 
END_VAR
VAR_TEMP
  tmpEnable : BOOL ;	
  tmpFlag : BOOL ;	
  tmpCmdNext : BOOL ;	
  tmpLocal : BOOL ;	
  tmpAspOk : BOOL ;	
  tmpHWStop : BOOL ;	
  tmpInstanzDBNo : INT ;	
END_VAR
BEGIN
NETWORK
TITLE =



NETWORK
TITLE =Read command from group control
//Parameter
      SET   ; 
      S     "GCS7_GrCtrlDB".ParMGR[136]; 
      R     "GCS7_GrCtrlDB".ParWGR[136]; 
      R     "GCS7_GrCtrlDB".ParGGR[136]; 
      R     "GCS7_GrCtrlDB".ParFeed[136]; 
      R     "GCS7_GrCtrlDB".ParShare[136]; 
      S     #Gr.Par.ParMgr; 
      R     #Gr.Par.ParWgr; 
      R     #Gr.Par.ParGgr; 
//Command
      U     "GCS7_GrCtrlDB".OutEnable[136]; 
      =     #Gr.InEnableGr; 
      U     "GCS7_GrCtrlDB".OutOccupied[136]; 
      =     #Gr.InOccupied; 
      U     "GCS7_GrCtrlDB".CmdFaultReset[136]; 
      R     "GCS7_GrCtrlDB".CmdFaultReset[136]; 
      S     #Gr.CmdFaultReset; 
      U     "GCS7_GrCtrlDB".CmdNext[136]; 
      R     "GCS7_GrCtrlDB".CmdNext[136]; 
      S     #Gr.CmdNext; 

      U     "GCS7_GrCtrlDB".CmdManualOff[136]; 
      UN    #Gr.InManual; 
      S     #Gr.CmdManualOff; 
      U     "GCS7_GrCtrlDB".CmdManualOff[136]; 
      R     "GCS7_GrCtrlDB".CmdManualOff[136]; 

//      U     "GCS7_GrCtrlDB".CmdManualOff[136]
//      O     "GCS7_GrCtrlDB".OutOccupied[136]
//      R     "GCS7_GrCtrlDB".CmdManualOff[136]
//      S     #Gr.CmdManualOff

      U     "GCS7_GrCtrlDB".CmdContinue[136]; 
      R     "GCS7_GrCtrlDB".CmdContinue[136]; 
      S     #Gr.CmdContinue; 
//Init Line Number
      U     "GCS7_GrCtrlDB".ParShare[136]; 
      SPBN  WE00; 
      L     0; 
      T     #Gr.InLine; 
WE00: NOP   0; 
NETWORK
TITLE =Call Methode Control
// 1. Handle Interface PCControl      
// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
// 3. Reset global flags of the group
      CALL #Gr (
           FcCode                   := 1);
NETWORK
TITLE =Restart Idling Time

      L     #Gr.Par.ParIdling; 
      L     2; 
      >=I   ; 
      SPB   IdTm; 
      T     #Gr.Par.ParIdling; 
IdTm: NOP   0; 
//BRT220310      CLR   
      UN    "G135M".Gr.ModePassive; 
      ON    "G095M".Gr.ModePassive; 
      S     #Gr.CmdRestartIdling; 
NETWORK
TITLE =Prepare global flags



NETWORK
TITLE =Set Pattern (Target position)



NETWORK
TITLE =Interlocking WayOk



NETWORK
TITLE =Interlocking Autostart

//BRT220310      CLR   
//      UN    "G135M".Gr.OutAutostart     //BRT220310
      O     "G006G".A_22_A097Z1.StFault; //BRT220310 - Notaus +C22
      O     "G006G".A_22_A106Z1.StFault; //BRT220310 - Notaus Signal von +C21  zu +C22
      R     #Gr.OutAutostart; 
NETWORK
TITLE =Interlocking Enable

      U     #Gr.InEnableGr; 
      UN(   ; 
      O     "G006G".A_22_A097Z1.StFault; //BRT220310 - Notaus +C22
      O     "G006G".A_22_A106Z1.StFault; //BRT220310 - Notaus Signal von +C21  zu +C22
      )     ; 
      =     #Gr.OutEnableEl; 

NETWORK
TITLE =Interlocking Next

      U     #Gr.CmdNext; 
      U     #Gr.OutAutostart; 
      UN    #Gr.OutNewWay; 
      =     #tmpCmdNext; 

NETWORK
TITLE =Interlocking Local / HWStop

      U     "PLC_SystemDB".HardwareStop.Germinating_2; 
//BRT100310 - HW-Stop nur bei NOT-AUS +C22 oder Customer, nicht bei NOT-AUS Maschine!
      U(    ; 
      O     "A_2299_SHE01_I1"; 
      O     "A_2299_SHE02_I1"; 
      O     "A_22_A097Z1_I1"; 
      )     ; 
      =     #tmpHWStop; 

      U     #Gr.ModePassive; 
      O     #tmpHWStop; 
      =     #tmpLocal; 

NETWORK
TITLE =Interlocking Start Warning

      NOP   0; 
//- Restart after fault --
      U     #Gr.CmdFaultReset; 
      U     #Gr.VarFault; 
      S     "SYS_StartWarningDB".Area[3].CmdResetStartWarning; 
//- Run Timer --
      AUF   "G136M"; 
      CALL "GCS7_StartWarning" , "SYS_StartWarningDB" (
           InModeHigher             := FALSE,
           InArea                   := 3,
           InEnable                 := #Gr.OutEnableEl,
           InLocal                  := #tmpLocal,
           InStarted                := #Gr.VarStarted,
           InCmdNext                := #tmpCmdNext,
           OutCmdNext               := #tmpCmdNext,
           OutStartwarning          := #Gr.VarStartwarning);
//- Store Startwarning done  --
//- Reset Autostart --
      U     "SYS_StartWarningDB".TmpArea.OutStartReleaseRunningL; 
      FN    #EdgeStartRelease; 
      U     #Gr.VarFault; 
      R     #Gr.OutAutostart; 
NETWORK
TITLE ==A-2201-MXZ01MA : BELT CONVEYOR 4600

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2201_MXZ01MA.Ma.InLocal; 
//
//-Interlocking---

      U     #Gr.OutEnableEl; 
      =     #A_2201_MXZ01MA.Ma.InEnableFwd; 
//
      U     #tmpCmdNext; 
      S     #A_2201_MXZ01MA.Ma.CmdNext; 
//
      U     #Gr.OutSeqStop; 
      =     #A_2201_MXZ01MA.Ma.InSeqStop; 
// 
//=Values and commands of elements===
//-=A-2201-BSA13---
//      CLR
//      S     #A_2201_MXZ01MA.A_2201_BSA13.CmdRunTimeOutTime
//-=A-2201-BZS14/15---
//      CLR
//      S     #A_2201_MXZ01MA.A_2201_BZS14_15.CmdRunTimeOutTime
//-=A-2201-BZS16/17---
//      CLR
//      S     #A_2201_MXZ01MA.A_2201_BZS16_17.CmdRunTimeOutTime
// 
//-Read inputs of elements---
      U     "A_2201_BSA13_I1"; 
      =     #A_2201_MXZ01MA.A_2201_BSA13.InpTrue; 
//BRT240310      U     "PLC_SystemDB".HardwareStop.Germinating_2
      U     #tmpHWStop; //240310
      =     #A_2201_MXZ01MA.A_2201_BSA13.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[23]; 
      =     #A_2201_MXZ01MA.A_2201_BSA13.InDPFault; 
      U     "A_2201_BZS14_15_I1"; 
      =     #A_2201_MXZ01MA.A_2201_BZS14_15.InpTrue; 
//BRT240310      U     "PLC_SystemDB".HardwareStop.Germinating_2
      U     #tmpHWStop; //240310
      =     #A_2201_MXZ01MA.A_2201_BZS14_15.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[23]; 
      =     #A_2201_MXZ01MA.A_2201_BZS14_15.InDPFault; 
      U     "A_2201_BZS16_17_I1"; 
      =     #A_2201_MXZ01MA.A_2201_BZS16_17.InpTrue; 
//BRT240310      U     "PLC_SystemDB".HardwareStop.Germinating_2
      U     #tmpHWStop; //240310
      =     #A_2201_MXZ01MA.A_2201_BZS16_17.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[23]; 
      =     #A_2201_MXZ01MA.A_2201_BZS16_17.InDPFault; 
      U     "A_2201_MXZ01_I1"; 
      =     #A_2201_MXZ01MA.A_2201_MXZ01.InpRunFwd; 
//BRT240310      U     "PLC_SystemDB".HardwareStop.Germinating_2
      U     #tmpHWStop; //240310
      =     #A_2201_MXZ01MA.A_2201_MXZ01.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[21]; 
      =     #A_2201_MXZ01MA.A_2201_MXZ01.InDPFault; 
      SET   ; 
      =     #A_2201_MXZ01MA.Mon2.InpTrue; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2201_MXZ01MA ;
// 
//-Write outputs---
      U     #A_2201_MXZ01MA.A_2201_MXZ01.OutpRunFwd; 
      =     "A_2201_MXZ01_O1"; 
NETWORK
TITLE ==A-2201-QXV21 : CLEANING VALVE BELT CONVEYOR

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2201_QXV21.InLocal; 
//
//-Interlocking---
      U     #A_2201_MXZ01MA.Ma.StStartedFwd; 
      =     #A_2201_QXV21.InEnableHN; 
      NOT   ; 
      =     #A_2201_QXV21.InEnableLN; 
//
      SET   ; 
      S     #A_2201_QXV21.CmdNext; 
//
      U     #Gr.OutSeqStop; 
      =     #A_2201_QXV21.InSeqStop; 
// 
//-Read inputs---
      UN    #A_2201_QXV21.OutpHN; 
      =     #A_2201_QXV21.InpLN; 
      U     #A_2201_QXV21.OutpHN; 
      =     #A_2201_QXV21.InpHN; 
//BRT240310      U     "PLC_SystemDB".HardwareStop.Germinating_2
      U     #tmpHWStop; //240310
      =     #A_2201_QXV21.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[23]; 
      =     #A_2201_QXV21.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2201_QXV21 ;
// 
//-Write outputs---
      U     #A_2201_QXV21.OutpHN; 
      =     "A_2201_QXV21_O1"; 
NETWORK
TITLE =Test Fault

      U     #A_2201_MXZ01MA.Ma.StFault; 
      O     #A_2201_QXV21.StFault; 
      =     #Gr.VarFault; 
NETWORK
TITLE =Test Warning

      CLR   ; 
      =     #Gr.OutWarning; 
NETWORK
TITLE =Test Stopped

      U     #A_2201_MXZ01MA.Ma.StStopped; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStopped; 
NETWORK
TITLE =Test Started

      U     #A_2201_MXZ01MA.Ma.StStartedFwd; 
      U     #A_2201_QXV21.StHN; 
      UN    #Gr.VarStopped; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStarted; 
NETWORK
TITLE =Enable Previous Group

      U     #Gr.VarStarted; 
      =     #Gr.OutEnablePrevGr; 
NETWORK
TITLE =Call Methode State
//1. Make State of the group: This function use the signals:
//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
//2.Reset Commands to group: e.g.CmdFaultReset
      CALL #Gr (
           FcCode                   := 2);
NETWORK
TITLE =Init

      SET   ; 
      R     #CmdNewValue; 
      R     #CmdNewJob; 
NETWORK
TITLE =Write data to group control

      U     #Gr.StStopped; 
      =     "GCS7_GrCtrlDB".InStopped[136]; 
      U     #Gr.StStarted; 
      =     "GCS7_GrCtrlDB".InStarted[136]; 
      U     #Gr.StFault; 
      =     "GCS7_GrCtrlDB".InFault[136]; 
      U     #Gr.OutWarning; 
      =     "GCS7_GrCtrlDB".InWarning[136]; 
      U     #Gr.OutManual; 
      =     "GCS7_GrCtrlDB".InManual[136]; 
      U     #Gr.OutHWStop; 
      =     "GCS7_GrCtrlDB".InHWStop[136]; 
      U     #Gr.OutDPFault; 
      =     "GCS7_GrCtrlDB".InDPFault[136]; 
      U     #Gr.OutLoaded; 
      =     "GCS7_GrCtrlDB".InLoaded[136]; 
      U     #Gr.OutAutostart; 
      =     "GCS7_GrCtrlDB".InAutoStart[136]; 
      U     #Gr.VarStartwarning; 
      =     "GCS7_GrCtrlDB".InStartWarning[136]; 
NETWORK
TITLE =End of block

      BE    ; 

END_FUNCTION_BLOCK

