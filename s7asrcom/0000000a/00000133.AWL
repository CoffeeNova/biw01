      +I    ; 
      L     200; // 20%
      >=I   ; // opened more than min
      =     "G170M".USW8; 

      L     "G171M".A_2452_MXZ01.OutPosition; //Fresh Air Flap
      L     "G172M".A_2450_MXZ01.OutPosition; //Exhaust Air Flap
      +I    ; 
      L     200; // 20%
      >=I   ; // opened more than min
