      L     "G172M".A_2450_MXZ01.OutPosition; //Exhaust Air Flap
      +I    ; 
      L     200; //20.0%
      <I    ; 
      L     50; //50% fan speed
      SPB   En07; 
      U     #Gr.InManual; 
      SPB   ILO7; 
      L     "MAL1S020Par".Par.ParIntMachine.TargetSpeedGermFan; 
      SPA   En07; 
