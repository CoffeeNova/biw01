      SPBN  WE00; 
      L     0; 
      T     #Gr.InLine; 
WE00: NOP   0; 
NETWORK
TITLE =Call Methode Control
// 1. Handle Interface PCControl      
// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
// 3. Reset global flags of the group
      CALL #Gr (
           FcCode                   := 1);
NETWORK
TITLE =Restart Idling Time

      L     #Gr.Par.ParIdling; 
      L     2; 
      >=I   ; 
      SPB   IdTm; 
      T     #Gr.Par.ParIdling; 
IdTm: NOP   0; 
//BRT140310      CLR   
      UN    #A_2502_BZS13.StTrue; //BRT140310
      S     #Gr.CmdRestartIdling; 
NETWORK
TITLE =Prepare global flags

      U     #A_2502_BZS13.StTrue; 
      O     #A_2502_BZS13.StCtoTrue; // parkposition
      S     "MAL1S021Par".Par.FlagMachine_0.UnloadingPosition; 

      U     "MAL1S021".PCRead.Step.StActive; 
      U     "MAL1S021Par".Par.FlagFunction_1.StepLoading; 
      R     "MAL1S021Par".Par.FlagMachine_0.UnloadingPosition; 
      =     "MAL1S021Par".Par.FlagMachine_0.LoadingGermBox; 

      U     "MAL1S021".PCRead.Step.StActive; 
      O     "MAL1S021".PCRead.Step.StReady; 
      O     "MAL1S021".PCRead.Step.StEmptying; 
      UN    "MAL1S021Par".Par.FlagFunction_1.StepLoading; 
      UN    "MAL1S021Par".Par.FlagMachine_0.UnloadingPosition; 
      =     "MAL1S021Par".Par.FlagMachine_0.RunToUNloadPosition; 

NETWORK
TITLE =Set Pattern (Target position)

// init direction
      U     "MAL1S021Par".Par.FlagMachine_0.LoadingGermBox; 
      UN    "MAL1S021Par".Par.FlagMachine_0.LoadingFWD; 
      UN    "MAL1S021Par".Par.FlagMachine_0.LoadingREV; 
      UN    #A_2502_MXZ01.StStartedFwd; 
      UN    #A_2502_MXZ01.StStartedRev; 
      UN    #A_2502_MXZ01.StFault; 
      U(    ; 
      UN    #A_2502_BZS14.StTrue; // endposition fan 1 side
      UN    #A_2502_BZS14.StCtoTrue; // endposition fan 1 side
      UN    #A_2502_BZS15.StTrue; // endposition fan 2 side
      UN    #A_2502_BZS15.StCtoTrue; // endposition fan 2 side
      O     #A_2502_BZS13.StTrue; // parkposition
      )     ; 
      S     "MAL1S021Par".Par.FlagMachine_0.LoadingREV; 

// endposition fan 2 side
      U(    ; 
      U     "MAL1S021Par".Par.FlagMachine_0.LoadingGermBox; 
      O     "MAL1S021Par".Par.FlagMachine_0.RunToUNloadPosition; 
      )     ; 
      U     "MAL1S021Par".Par.FlagMachine_0.LoadingREV; 
      UN    #A_2502_MXZ01.StStartedFwd; 
      UN    #A_2502_MXZ01.StFault; 
      U(    ; 
      O(    ; 
      U     #A_2502_BZS15.StTrue; // endposition fan 2 side
      O     #A_2502_BZS15.StCtoTrue; // endposition fan 2 side
      )     ; 
      )     ; 
      UN    #A_2502_BZS14.StTrue; // endposition fan 1 side
      R     "MAL1S021Par".Par.FlagMachine_0.LoadingREV; 

      U(    ; 
      U     "MAL1S021Par".Par.FlagMachine_0.LoadingGermBox; 
      O     "MAL1S021Par".Par.FlagMachine_0.RunToUNloadPosition; 
      )     ; 
      UN    "MAL1S021Par".Par.FlagMachine_0.LoadingREV; 
      U     #A_2502_BZS15.StTrue; // endposition fan 2 side
      UN    #A_2502_BZS14.StTrue; // endposition fan 1 side
      S     "MAL1S021Par".Par.FlagMachine_0.LoadingFWD; 

// endposition fan 1 side
      U(    ; 
      U     "MAL1S021Par".Par.FlagMachine_0.LoadingGermBox; 
      O     "MAL1S021Par".Par.FlagMachine_0.RunToUNloadPosition; 
      )     ; 
      U     "MAL1S021Par".Par.FlagMachine_0.LoadingFWD; 
      UN    #A_2502_MXZ01.StStartedRev; 
      UN    #A_2502_MXZ01.StFault; 
      UN    #A_2502_BZS15.StTrue; // endposition fan 2 side
      U(    ; 
      U     #A_2502_BZS14.StTrue; // endposition fan 1 side
      O     #A_2502_BZS14.StCtoTrue; // endposition fan 1 side
      )     ; 
      R     "MAL1S021Par".Par.FlagMachine_0.LoadingFWD; 

      U(    ; 
      U     "MAL1S021Par".Par.FlagMachine_0.LoadingGermBox; 
      O     "MAL1S021Par".Par.FlagMachine_0.RunToUNloadPosition; 
      )     ; 
      UN    "MAL1S021Par".Par.FlagMachine_0.LoadingFWD; 
      U     #A_2502_BZS14.StTrue; // endposition fan 2 side
      UN    #A_2502_BZS15.StTrue; // endposition fan 1 side
      S     "MAL1S021Par".Par.FlagMachine_0.LoadingREV; 

//BRT140310 - Wagen fährt nach Loading Finished zur Parkposition!
      U(    ; 
      U     "MAL1S021Par".Par.FlagMachine_0.LoadingFWD; 
      O     "MAL1S021Par".Par.FlagMachine_0.LoadingREV; 
      )     ; 
      U     "MAL1S021Par".Par.FlagMachine_0.UnloadingPosition; 
      R     "MAL1S021Par".Par.FlagMachine_0.LoadingFWD; 
      R     "MAL1S021Par".Par.FlagMachine_0.LoadingREV; 

