      =     #Gr.InEnableGr; 
      U     "GCS7_GrCtrlDB".OutOccupied[195]; 
      =     #Gr.InOccupied; 
      U     "GCS7_GrCtrlDB".CmdFaultReset[195]; 
      R     "GCS7_GrCtrlDB".CmdFaultReset[195]; 
