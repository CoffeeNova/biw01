      A(    ; 
      A     "MAL1S033Par".Par.FlagProcess.Inf06; //Special kilning mode second kiln 1=ON, 0=OFF     
      A     "G303M".A_3240_MXZ11.StLN; // Louver Air Heater Kilningbox 2
      A     "G303M".A_3241_MXZ11.StLN; // Louver Air Heater Kilningbox 2
      A     "G304M".A_3245_MXZ11.StLN; // Exhaust Air Kilningbox 2
      A     "G304M".A_3246_MXZ11.StLN; // Exhaust Air Kilningbox 2
      A     "G299M".A_3236_MXZ11.StHN; // Recycle Air 2 Kilnbox 2
      A     "G299M".A_3235_MXZ11.StHN; // Recycle Air 1 Kilnbox 2
      )     ; 
      O     ; 
//---------------------- Zwei-Hordenbetrieb --------------------------//
      A     "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      A     "MAL1S033Par".Par.FlagProcess.ProcessActive; 
// Luft-Eintritt
      A(    ; 
      A     "G303M".A_3240_MXZ11.StHN; // Louver Air Heater Kilningbox 2
      A     "G303M".A_3241_MXZ11.StHN; // Louver Air Heater Kilningbox 2
      O     ; 
      A     "G303M".A_3240_MXZ11.StLN; // Louver Air Heater Kilningbox 2
      A     "G303M".A_3241_MXZ11.StLN; // Louver Air Heater Kilningbox 2
      A     "G273M".A_3136_MXZ11.StHN; // Recycling Air Flap Kilningbox 1   ///JVXXXXXX
      A     "G299M".A_3235_MXZ11.StHN; // Recycling Air Flap Kilningbox 2
      O     ; 
      AN    "G298M".A_3250_MXZ11.StLN; // Fresh Air Flap Kilningbox 2
      AN    "G298M".A_3251_MXZ11.StLN; // Fresh Air Flap Kilningbox 2
      AN    "G298M".A_3252_MXZ11.StLN; // Fresh Air Flap Kilningbox 2
      AN    "G298M".A_3253_MXZ11.StLN; // Fresh Air Flap Kilningbox 2
      )     ; 
// Luft-Austritt
      A(    ; 
      A     "G304M".A_3245_MXZ11.StHN; // Exhaust Air Kilningbox 2
      A     "G304M".A_3246_MXZ11.StHN; // Exhaust Air Kilningbox 2
      O     ; 
      AN    "G304M".A_3245_MXZ11.StLN; // Exhaust Air Kilningbox 2
      AN    "G304M".A_3246_MXZ11.StLN; // Exhaust Air Kilningbox 2
      AN    "G299M".A_3235_MXZ11.StLN; // Recycling Air Flap Kilningbox 2
      AN    "G299M".A_3236_MXZ11.StLN; // Recycling Air Flap Kilningbox 2
      O     ; 
      A     "G299M".A_3236_MXZ11.StHN; // Recycling Air Flap Kilningbox 2
      AN    "G299M".A_3235_MXZ11.StLN; // Recycling Air Flap Kilningbox 2
      AN    "G273M".A_3135_MXZ11.StLN; // Recycling Air Flap Kilningbox 1
      O     ; 
// 100% Umluft-Betrieb
      A     "G299M".A_3236_MXZ11.StHN; // Recycling Air Flap Kilningbox 2
      A(    ; 
      O     "G273M".A_3135_MXZ11.StHN; // Recycling Air Flap Kilningbox 1
      O     "G299M".A_3235_MXZ11.StHN; // Recycling Air Flap Kilningbox 2
      )     ; 
      )     ; 
      )     ; 
      AN(   ; //JV080810 ========> 
      A     "G304M".A_3245_MXZ11.StFault; // Exhaust Air Kilningbox 2
      A     "G304M".A_3246_MXZ11.StFault; // Exhaust Air Kilningbox 2
      A     "G299M".A_3235_MXZ11.StFault; // Recycling Air Flap Kilningbox 2
      A     "G299M".A_3236_MXZ11.StFault; // Recycling Air Flap Kilningbox 2
      )     ; //JV080810 <========
      L     S5T#15S; 
      SF    T    397; 

      A     T    397; 
      O(    ; 
      L     "MAL1S033Par".Ilo.ILOMode; 
      L     4; 
      ==I   ; 
      A     #Gr.InManual; 
      A     "G299M".A_3235_MXZ11.StHN; // Recycling Air Flap Kilningbox 2
      A     "G299M".A_3236_MXZ11.StHN; // Recycling Air Flap Kilningbox 2
      )     ; 
      O(    ; 
      L     "MAL1S033Par".Ilo.ILOMode; 
      L     5; 
      ==I   ; 
      A     #Gr.InManual; 
      AN(   ; 
      A     "G303M".A_3240_MXZ11.StLN; // Louver Air Heater Kilningbox 2
      A     "G303M".A_3241_MXZ11.StLN; // Louver Air Heater Kilningbox 2
      A     "G304M".A_3245_MXZ11.StLN; // Exhaust Air Kilningbox 2
      A     "G304M".A_3246_MXZ11.StLN; // Exhaust Air Kilningbox 2
      A     "G299M".A_3235_MXZ11.StLN; // Recycling Air Flap Kilningbox 2
      A     "G299M".A_3236_MXZ11.StLN; // Recycling Air Flap Kilningbox 2
      )     ; 
      )     ; 
      A     "G276M".A_3191_BPS21.StTrue; // HEAT EXCHANGE PRESSURE < 15mbar
      A     #Gr.InEnableGr; 
      AN    "G001G".G_FIRE.StFault; 
      =     #Gr.OutEnableEl; 

