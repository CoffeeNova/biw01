TITLE =Prepare global flags



NETWORK
TITLE =Set Pattern (Target position)

      A     #Gr.InManual; 
      JC    ILO6; 

// Drehzahlabsenkung wenn die andere Horde im Cooling ist
// und die Temperatur unter der eigenen Horde 5°C > als T[Soll] 
//      U     "MAL1S032Par".Par.FlagProcess.Cooling_Activ
//      U     "G302M".USW8                // Temperatur unter der Horde 5°C > T[Soll]
//      L     50                          // 50%
//      SPB   En06
//JV270710 ======>
// Drehzahlabsenkung wenn die andere Horde gerade zugeschaltet wurde
      A(    ; 
      A     "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      A     "MAL1S033Par".Par.FlagProcess.ProcessActive; 
      A(    ; 
      O(    ; //JV290710 ======>
      A     "MAL1S033Par".Par.FlagProcess.Abdarr_Betrieb_Aktiv; 
      A     "MAL1S033Par".Par.FlagProcess.Cooling_Activ; 
      AN    "G304M".A_3245_MXZ11.StLN; // Abluftklappen zu
      AN    "G304M".A_3246_MXZ11.StLN; // Abluftklappen zu
      )     ; 
      O(    ; 
      A     "MAL1S032Par".Par.FlagProcess.Cooling_Activ; 
      AN    "G304M".A_3245_MXZ11.StHN; // Abluftklappen auf
      AN    "G304M".A_3246_MXZ11.StHN; // Abluftklappen auf
      )     ; 
      )     ; //JV290710 <======
      )     ; 
//BRT20100827 ====> beide Prozesse gestartet, aber Darren nicht bereit für Verbund!!!
      O(    ; 
      A     "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      A     "MAL1S033Par".Par.FlagProcess.ProcessActive; 
      AN    "MAL1S032Par".Par.FlagProcess.Abdarr_Betrieb_Aktiv; 
      AN    "MAL1S033Par".Par.FlagProcess.Abdarr_Betrieb_Aktiv; 
      AN(   ; 
      A     "MAL1S032Par".Par.FlagProcess.Cooling_Activ; 
      O     "MAL1S033Par".Par.FlagProcess.Cooling_Activ; 
      )     ; 
//18.02.2015 IS special kilning mode check
      AN    "MAL1S032Par".Par.FlagProcess.Inf06; //Special kilning mode first kiln 1=ON, 0=OFF
      AN    "MAL1S033Par".Par.FlagProcess.Inf06; //Special kilning mode second kiln 1=ON, 0=OFF
      )     ; 
//BRT20100827 <====
      L     60; // 60% Drehzahl bis Klappen richtig stehen
      JC    En06; 
//JV270710 <========

//BRT20100826 ====> Ventilator geregelt während Kühlung und Lufttransfer Darre 1 nach 2!
// 
      A     "MAL1S033Par".Par.FlagProcess.Cooling_Activ; 
      A     "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      L     "MAL1S033Par".Par.ProcessValue.Value15; // Sollwert Ventilator Kühlen
      JC    En06; 
//18.02.2015 IS for special kilning mode
      A     "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      A     "MAL1S033Par".Par.FlagProcess.ProcessActive; 
      A     "MAL1S033Par".Par.FlagProcess.Inf06; //Special kilning mode second kiln 1=ON, 0=OFF    
      A     "G303M".A_3240_MXZ11.StLN; // Louver Air Heater Kilningbox 2
      A     "G303M".A_3241_MXZ11.StLN; // Louver Air Heater Kilningbox 2
      A     "G304M".A_3245_MXZ11.StLN; // Exhaust Air Kilningbox 2
      A     "G304M".A_3246_MXZ11.StLN; // Exhaust Air Kilningbox 2
      A     "G299M".A_3236_MXZ11.StHN; // Recycle Air 2 Kilnbox 2
      A     "G299M".A_3235_MXZ11.StHN; // Recycle Air 1 Kilnbox 2
      L     "MAL1S033Par".Par.ProcessValue.Value18; // Sollwert Ventilator Verbund
      JC    En06; 
//BRT20100826 <====
//BRT20100902 ====> wegen Programmänderungen Regelung Verbund
//      L     "MAL1S033Par".Par.ParIntProcess.SpeedVentilatorBasic
      L     "MAL1S033Par".Par.ProcessValue.Value18; // Sollwert Ventilator Verbund
//BRT20100902 <====
      JU    En06; 
ILO6: NOP   0; 
      L     "MAL1S033Par".Par.ParIntProcess.Speed_ILO_Fan; 
En06: NOP   0; 
      L     100; 
      *I    ; 
      T     #A_3225_TSC01_COM.OutValuePC; // xxx.xx%
      T     #A_3226_TSC01_COM.OutValuePC; // xxx.xx%



