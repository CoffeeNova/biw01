	NETWORK
	TITLE =ASW_6010: Process Kilning Active
	      U     "MAL1S031".ToASW.InfDataReady; 
	      SPB   NoCl; 
	// Clear process time in buffer
	      L     0; 
	      T     "MAL1S031Par".ActTimeStepProcess.SteptimeActHour; 
	      T     "MAL1S031Par".ActTimeStepProcess.SteptimeActMin; 
	      T     "MAL1S031Par".ActTimeStepProcess.SteptimeActMinOverall; 
	      T     "MAL1S031Par".Processtime.Day; 
	      T     "MAL1S031Par".Processtime.Hour; 
	      T     "MAL1S031Par".Processtime.Min; 
	      T     "MAL1S031Par".Processtime.ProcessTimeActive; 
	      L     L#0; // clear actual step time
	      T     "MAL1S031Par".Par.ProcessValue.StepTimeAct; // in ms
	NoCl: NOP   0; 
	// Clear process time in buffer end
	      U     "MAL1S031Par".Par.FlagProcess.ProcessActive; 
	      SPBN  E606; 
	//-Actual process time---
	      U     "MAL1S031Par".Par.FlagProcess.StartProzess_SCAN; 
	      SPBN  actk; 
	      L     1; // Start process first step
	      T     "MAL1S031Par".Par.ParIntMachine.ActualStep; 
	      T     "MAL1S031Par".Par.ProcessValue.StepNrActCopy; 
	      SPA   neSk; 
	actk: NOP   0; 
	// Prozesszeit 
	      L     "DB Prozesszeit".Sektion_31.Tage; 
	      INC   1; // malters do not accept day 0!!
	      T     "MAL1S031Par".Processtime.Day; 
	      L     "DB Prozesszeit".Sektion_31.Stunden; 
	      T     "MAL1S031Par".Processtime.Hour; // Processtime in h
	      L     "DB Prozesszeit".Sektion_31.Minuten; 
	      T     "MAL1S031Par".Processtime.Min; // Processtime in min
	      U     "DB Prozesszeit".Sektion_31.Prozess_aktiv; // active bit section 2
	      L      1;
	      SPB    nTa;
	      L      0;
	nTa:  T     "MAL1S031Par".Processtime.ProcessTimeActive; 
	// Test new step
	      L     "MAL1S031Par".Par.ParIntMachine.ActualStep; 
	      L     "MAL1S031Par".Par.ProcessValue.StepNrActCopy; 
	      ==I   ; 
	      SPB   olSk; 
	neSk: NOP   0; 
	      L     "MAL1S031Par".Par.ParIntMachine.ActualStep; 
	      T     "MAL1S031Par".Par.ProcessValue.StepNrActCopy; 
	// 
	      L     L#0; // clear actual step time
	      T     "MAL1S031Par".Par.ProcessValue.StepTimeAct; // in ms
	      L     0; 
	      T     "MAL1S031Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
	// 
	      U(    ; 
	      L     "MAL1S031Par".Par.ParIntMachine.ActualStep; 
	      L     "MAL1S031Par".RecipeHeader.NumberOfSteps; 
	      >I    ; 
	      )     ; 
	      SPB   Pedk; // last step is finished
	      SET   ; 
	      S     "MAL1S031StepValues".StartStep; // start step for average calculating
	      SPA E606   ; // to secure, that new step-data are copied
	olSk: NOP   0; 
	      L     "ASW_GraphicData".General.CycleTime[1].Actual; 
	      T     #tmpCyclusTime; 
	//
	      SPBN  NoTi; // count steptime only when fan running and sensor alive
	      L     "MAL1S031Par".Par.ProcessValue.StepTimeAct; // in ms
	      L     #tmpCyclusTime; 
	      +D    ; 
	      T     "MAL1S031Par".Par.ProcessValue.StepTimeAct; // in ms
	NoTi: NOP   0; 
	      L     "MAL1S031Par".Par.ProcessValue.StepTimeAct; // in ms
	      L     L#86400000; // ms/d
	      MOD   ; 
	      L     L#3600000; // ms/h
	      /D    ; 
	      T     "MAL1S031Par".ActTimeStepProcess.SteptimeActHour; // Steptime in h
	//
	      L     "MAL1S031Par".Par.ProcessValue.StepTimeAct; 
	      L     L#3600000; // ms/h
	      MOD   ; 
	      L     L#60000; // ms/min
	      /D    ; 
	      T     "MAL1S031Par".ActTimeStepProcess.SteptimeActMin; // Steptime in min
	//
	      L     "MAL1S031Par".Par.ProcessValue.StepTimeAct; 
	      L     L#60000; // ms/min
	      /D    ; 
	      T     "MAL1S031Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
	// 
	// Next Step
	// 
	//   Switch over to next step under conditions:
	//   or Maximal steptime
	//   or Minimal steptime
	//           and 
	//               or no setpoint delta temp
	//               or atual value delta temp is ge setpoint deltatemp
	//                   and sensor is ok
	//           and          
	// /              or no setpoint max temp above floor
	//              or atual value max temp above floor is ge setpoint max temp above floor
	//                  and sensor is ok
	// 1 Kriterium
	      U(    ; 
	      L     "MAL1S031Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
	      L     "MAL1S031Par".StepAct.StepTime_Max; // Target steptime actual step
	      >=I   ; 
	      )     ; 
	      O(    ; 
	// 2 Kriterium
	      O(    ; 
	      U(;
	      L     "MAL1S031Par".StepAct.DeltaTempSwitchpointStep;
	      L     0;
	      >=D    ;                  
	      )      ;
	// condition, if analogue input is not damaaged   //
	      U(    ; 
	      -D    ; // delta temp actual
	      L     L#10; 
	      /D    ; 
	      L     "MAL1S031Par".StepAct.DeltaTempSwitchpointStep; 
	      <D    ; 
	      )     ; 
	      )     ; 
	// 3 Kriterium
	      O(    ; 
	      U(    ; 
	      L     "MAL1S031Par".StepAct.MaxTempAboveFloorStep; 
	      L     L#0; // 0,0°C is illegally switchpoint for max. temperature
	      >D   ; 
	      )     ; 
	// condition, if analogue input is not damaaged   // MW R2.1
	      U(    ; 
	      L     L#10; 
	      /D    ; 
	      L     "MAL1S031Par".StepAct.MaxTempAboveFloorStep; 
	      >=D   ; 
	      )     ; 
	      )     ; 
	// 4 Kriterium  
	// and min. time
	      U(    ; 
	      L     "MAL1S031Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
	      L     "MAL1S031Par".StepAct.StepTime_Min; // Target steptime actual step
	      >=I   ; 
	      )     ; 
	      )     ; 
	// and not cooling                                // UK 22.01.07
	      UN    "MAL1S031Par".Par.FlagProcess.Cooling_Activ; 
	// 5 Kriterium
	      O(    ; 
	      U     "MAL1S031Par".Par.FlagProcess.Cooling_Activ; 
	      U(    ; 
	      L     L#10; 
	      /D    ; 
	      L     "MAL1S031Par".StepAct.TemperaturAboveFloor; 
	      <=D   ; 
	      )     ; 
	      O(    ; 
	      L     "MAL1S031Par".Par.ParIntMachine.ActualStep; 
	      L     25; // max. 24 Schritte
	      >=I   ; 
	      )     ; 
	      )     ; 
	// and min. time
	      U(    ; 
	      L     "MAL1S031Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
	      L     "MAL1S031Par".StepAct.StepTime_Min; // Target steptime actual step
	      >=I   ; 
	      )     ; 
	      SPBN  E606; 
	// send average step data to PIC
	      SET   ; 
	      S     "MAL1S031StepValues".SendData; // send average step data to PIC
	// Next step
	      L     "MAL1S031Par".Par.ParIntMachine.ActualStep; 
	      L     "MAL1S031Par".RecipeHeader.NumberOfSteps; 
	      >I    ; 
	      SPB   Pedk; // Last step is finished
	// 
	      L     "MAL1S031Par".Par.ParIntMachine.ActualStep; 
	      L     1; 
	      +I    ; 
	      T     "MAL1S031Par".Par.ParIntMachine.ActualStep; 
	      L     0; 
	      T     "MAL1S031Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min Overall 
	      SPA   E606; 
	Pedk: NOP   0; // Process has finished
	      SET   ; 
	      R     "MAL1S031Par".Par.FlagProcess.ProcessActive; 
	      R     "MAL1S031Par".Par.FlagProcess.Cooling_Activ; 
	      S     "MAL1S031Par".Par.FlagProcess.ProcessEnd; 
	// 
	      L     0; // clear step number
	      T     "MAL1S031Par".Par.ParIntMachine.ActualStep; 
	E606: NOP   0; 
	NETWORK
	TITLE =ASW_6020: Copy recipe-data in Parameter DB
