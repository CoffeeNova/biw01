	// ==========================
	// G284M       BELT CONVEYOR 7100 KILN
	// G261M       BELT CONVEYOR 7200 KILN
	// 
	      NOP   0; 
//-Condition for preselection---
		U     MAL1S031.ToASW.InfDataReady;
		SPB   GS01;
//-Deselect groups ---
		CALL FB     9 , "GCS7_GrCtrlDBTmp" (// GrCtr.Methode Deselect
		     Code  := 3,
		     Sec   := "MAL1S031".Gr);
		SPA   GS02; 
//-Select groups ---
