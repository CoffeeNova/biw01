		);
		UN    MAL1S031.Gr.All.CmdNewSelection;
		UN    MAL1S031.PCRead.Step.StWait;
		=     #tmpStartNext; //-->VarStartNext
//-Start next feed---
		U     MAL1S031.Gr.Wgr.OutStartedRel;
		ON    MAL1S031.ToASW.InfWayAdjustTimeEnd;
		UN    MAL1S031.PCRead.Status.StaFeedOffASW;
