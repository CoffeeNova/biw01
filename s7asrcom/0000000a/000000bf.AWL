	TITLE =Main0_Transition: Condition for Transition
	
		NOP 0;
		NOP 0;
	//-Active---
		U   MAL1S031.Gr.All.InOutASWSelectionOK;
	        U    MAL1S031.PCRead.Step.StWait; 
