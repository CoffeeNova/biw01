		U     "MAL1S031Par".Par.FlagFunction_1.FinishCleaning;
		=     MAL1S031.ToSSW.CmdGotoEmptied;
	//-Result section---
	//-Idling---
		U     MAL1S031.PCRead.Step.StEmptied;
		=     MAL1S031.ToSSW.CmdGotoIdling;
	//-Passive---
		U     MAL1S031.PCRead.Step.StIdling;
		U     MAL1S031.ToSSW.InfStopped;
