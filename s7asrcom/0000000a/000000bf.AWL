	NETWORK
	TITLE =ASW_5060: Prepare Log-Data events
	NOP 0;
	// load events into event input
	
	// 1. event Process active  
	      U     "MAL1S031Par".Par.FlagProcess.ProcessActive; 
	      =     "MAL1S031Event".EREIGNISBIT_1; 
	      L     1; 
	      T     "MAL1S031Event".EventObject_1; 
	// 2. event InfDataReady  
	      U     "MAL1S031".ToASW.InfDataReady; 
	      =     "MAL1S031Event".EREIGNISBIT_2; 
	      L     2; 
	      T     "MAL1S031Event".EventObject_2; 
	// 11. event   
	      U     "MAL1S031Par".Par.FlagFunction_1.StepLoading; // Step loading
	      =     "MAL1S031Event".EREIGNISBIT_11; 
	      L     1001; 
	      T     "MAL1S031Event".EventObject_11; 
	// 12. event Loading Round 1
	      U     "MAL1S031Par".Par.FlagFunction_1.StepLoading; 
	      U(    ; 
	      L     WinCoS_BIW01_09PLC1BIW01_02.Ma.OutCntRound; 
	      L     L#0; 
	      ==D   ; 
	      )     ; 
	      =     "MAL1S031Event".EREIGNISBIT_12; 
	      L     1003; 
	      T     "MAL1S031Event".EventObject_12; 
	// 13. event Loading Round 2
	      U     "MAL1S031Par".Par.FlagFunction_1.StepLoading; 
	      U(    ; 
	      L     WinCoS_BIW01_09PLC1BIW01_02.Ma.OutCntRound; 
	      L     L#1; 
	      ==D   ; 
	      )     ; 
	      =     "MAL1S031Event".EREIGNISBIT_13; 
	      L     1004; 
	      T     "MAL1S031Event".EventObject_13; 
	// 14. event Loading Round 3
