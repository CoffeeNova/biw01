	//-Prepare for preselection ---
		CALL FB     9 , "GCS7_GrCtrlDBTmp" (// GrCtr.Methode Init Preselection
		     Code  := 1,
		     Sec   := "MAL1S031".Gr);
	// 
	// 
	//-Prestart used groups --
		U     M     50.0; 
		=     M     50.0; 
		
	//-Preselect used groups --
		U     M     50.0; 
		=     M     50.0; 
		S   MAL1S031.Gr.InPreselect[284];   //-->G284M BELT CONVEYOR 7100 KILN
		S   MAL1S031.Gr.InPreselect[261];   //-->G261M BELT CONVEYOR 7200 KILN
