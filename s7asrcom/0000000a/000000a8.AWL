NETWORK
TITLE ==A-1112-MXZ11 : CO2 FAN ST 2

      NOP   0; 
//-InLocal--- 
      U     #tmpLocal; 
      =     #A_1112_MXZ11.InLocal; 
//
//-Interlocking--- 
      U     "MAL1S005Par".Par.FlagProcess1.CO2SuckStart; // CO2 Suck Run
      U     "MAL1S005Par".Par.FlagProcess1.CO2SuckActive; // CO2 Suck Active
      UN    "MAL1S005Par".Par.FlagProcess1.CO2SuckPause; // CO2 Suck Pause
      U     #A_1162_MXZ_BZS01_02.StHN; 
      U     #Gr.OutEnableEl; 
      =     #A_1112_MXZ11.InEnableFwd; 
// 
      U     #tmpCmdNext; 
      S     #A_1112_MXZ11.CmdNext; 
// 
      U     #Gr.OutSeqStop; 
      =     #A_1112_MXZ11.InSeqStop; 
// 
//-Read inputs---
      U     "A_1112_MXZ11_I1"; 
      =     #A_1112_MXZ11.InpRunFwd; 
      U     "PLC_SystemDB".HardwareStop.Steeping; 
      =     #A_1112_MXZ11.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[5]; 
      =     #A_1112_MXZ11.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_1112_MXZ11 ;
// 
//-Write outputs---
      U     #A_1112_MXZ11.OutpRunFwd; 
      =     "A_1112_MXZ11_O1"; 
NETWORK
TITLE ==A-1162-MXZ/BZS01/02 : CO2 VALVE ST 2

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_1162_MXZ_BZS01_02.InLocal; 
//
//-Interlocking---
      U     "MAL1S005Par".Par.FlagProcess1.CO2SuckStart; // CO2 Suck Run
      U     #Gr.OutEnableEl; 
//      U     #A_1112_MXZ11.StStartedFwd
      =     #A_1162_MXZ_BZS01_02.InEnableHN; 
      NOT   ; 
      =     #A_1162_MXZ_BZS01_02.InEnableLN; 
//
      SET   ; 
      S     #A_1162_MXZ_BZS01_02.CmdNext; 
//
      U     #Gr.OutSeqStop; 
      =     #A_1162_MXZ_BZS01_02.InSeqStop; 
// 
//-Read inputs---
      U     "A_1162_BZS01_I1"; 
      =     #A_1162_MXZ_BZS01_02.InpLN; 
      U     "A_1162_BZS02_I1"; 
      =     #A_1162_MXZ_BZS01_02.InpHN; 
// 1.1
      CLR   ; 
      =     #A_1162_MXZ_BZS01_02.InpFaultDev; 
      U     "PLC_SystemDB".HardwareStop.Steeping; 
      =     #A_1162_MXZ_BZS01_02.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[8]; 
      O     "PLC_DPInfo".Master[1].SlavesError[9]; 
      =     #A_1162_MXZ_BZS01_02.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_1162_MXZ_BZS01_02 ;
// 
//-Write outputs---
// 1.1
      U     #A_1162_MXZ_BZS01_02.OutpLN; 
      =     "A_1162_MXZ01_O1"; 
      U     #A_1162_MXZ_BZS01_02.OutpHN; 
      =     "A_1162_MXZ01_O2"; 

NETWORK
TITLE =Test Fault

      U     #A_1112_MXZ11.StFault; 
      O     #A_1162_MXZ_BZS01_02.StFault; 
      =     #Gr.VarFault; 
NETWORK
TITLE =Test Warning

      CLR   ; 
      =     #Gr.OutWarning; 
NETWORK
TITLE =Test Stopped

      U     #A_1112_MXZ11.StStopped; 
      U     #A_1162_MXZ_BZS01_02.StLN; 
      UN    #Gr.VarStarted; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStopped; 
NETWORK
TITLE =Test Started

      U     #A_1112_MXZ11.StStartedFwd; 
      U     #A_1162_MXZ_BZS01_02.StHN; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStarted; 
NETWORK
TITLE =Enable Previous Group

      U     #Gr.VarStarted; 
      =     #Gr.OutEnablePrevGr; 
NETWORK
TITLE =Call Methode State
//1. Make State of the group: This function use the signals:
//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
//2.Reset Commands to group: e.g.CmdFaultReset
      CALL #Gr (
           FcCode                   := 2);
NETWORK
TITLE =Init

      SET   ; 
      R     #CmdNewValue; 
      R     #CmdNewJob; 
NETWORK
TITLE =Write data to group control

      U     #Gr.StStopped; 
      =     "GCS7_GrCtrlDB".InStopped[34]; 
      U     #Gr.StStarted; 
      =     "GCS7_GrCtrlDB".InStarted[34]; 
      U     #Gr.StFault; 
      =     "GCS7_GrCtrlDB".InFault[34]; 
      U     #Gr.OutWarning; 
      =     "GCS7_GrCtrlDB".InWarning[34]; 
      U     #Gr.OutManual; 
      =     "GCS7_GrCtrlDB".InManual[34]; 
      U     #Gr.OutHWStop; 
      =     "GCS7_GrCtrlDB".InHWStop[34]; 
      U     #Gr.OutDPFault; 
      =     "GCS7_GrCtrlDB".InDPFault[34]; 
      U     #Gr.OutLoaded; 
      =     "GCS7_GrCtrlDB".InLoaded[34]; 
      U     #Gr.OutAutostart; 
      =     "GCS7_GrCtrlDB".InAutoStart[34]; 
      U     #Gr.VarStartwarning; 
      =     "GCS7_GrCtrlDB".InStartWarning[34]; 
NETWORK
TITLE =End of block

      BE    ; 

END_FUNCTION_BLOCK

