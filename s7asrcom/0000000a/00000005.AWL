TITLE =General Cooling for all Sections
//// MW 2010.03.18
// Start cooling, with delay
      L     "G269M".A_3191_BTE11.OutValueUnits; // TEMPERATURE AMBIENT AIR            
      L     L#100; // von xx,xx°C AUF xx°C
      /D    ; 
      L     "MAL1Par".Values1.Value01; // Target temperature for start cooling
      >D    ; 
      =     "MAL1Par".FaultTimeoutFeedOff.EN; 

      L     "MAL1Par".Values1.Value02; // Delay start cooling
      L     L#600; // xx Minuten => xxxxx.x Secunden
      *D    ; 
      T     "MAL1Par".FaultTimeoutFeedOff.PT; 
      CALL "GCS7_TON_LONG" (
           IN                       := "MAL1Par".FaultTimeoutFeedOff.EN,
           PT                       := "MAL1Par".FaultTimeoutFeedOff.PT,
           Q                        := "MAL1Par".FaultTimeoutFeedOff.Q,
           ET                       := "MAL1Par".FaultTimeoutFeedOff.ET);

// Stopp cooling, with delay
      L     "G269M".A_3191_BTE11.OutValueUnits; // TEMPERATURE AMBIENT AIR            
      L     L#100; // von xx,xx°C AUF xx°C
      /D    ; 
      L     "MAL1Par".Values1.Value01; // Target temperature for start cooling
      <D    ; 
      =     "MAL1Par".FaultTimeoutImmStop.EN; 

      L     "MAL1Par".Values1.Value02; // Delay start cooling
      L     L#600; // xx Minuten => xxxxx.x Secunden
      *D    ; 
      T     "MAL1Par".FaultTimeoutImmStop.PT; 
