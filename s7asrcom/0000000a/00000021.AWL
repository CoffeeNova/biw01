      U     "MAL1S032Par".Par.FlagProcess.ProcessActive; // 2 Darren - Betrieb
      UN    "MAL1S033Par".Par.FlagProcess.ProcessEnd; 
      UN    M    399.0; //JV090810 Darre 1 kaputt  Hilfsbit zum Kaltblasen Darre 2 über Abluft, nicht zum Kanal 
