	FUNCTION_BLOCK FB 401
	TITLE =G301M LOUVER HEATING GLASS TUBE HEAT EXCHANGER
	//COPYRIGHT:    (C) 2009  BUHLER AG, CH-9240 UZWIL, SWITZERLAND
	//
	//PRODUCT:      WinCoS_BIW01_09
	//
	//TITLE:        LOUVER HEATING GLASS TUBE HEAT EXCHANGER
	//                Machine group control
	//                Maschinengruppensteuerung
	// 
	//--------------------------------------------------------------------------------
	// 
	//IDENT:        AUTHOR:             DEPT:                              DATE:
	// 
	//P01_FB401.1.0 K.Renz              CSB41                              07.Okt.2009
	//             1st Release
	// 
	//--------------------------------------------------------------------------------
	// 
	//PURPOSE: 
	// 
	//MANUAL : 
	// 
	//RESTRICTIONS: 
	// 
	//--------------------------------------------------------------------------------
	AUTHOR  : KR      
	FAMILY  : USW
	VERSION : 9.9
	
	
	VAR
	  Gr       : FB10;	//Standard interface to group
	  InOutValue1    : DINT; //Data for user
	  InOutValue2    : DINT; //Data for user
	  OutWayConflict : BOOL;	//Data for user
	  OutSelectionOk : BOOL;	//Data for user
	  CmdNewValue : BOOL;	//Data for user
	  CmdNewJob   : BOOL;	//Data for user
	  OutCodeOK   : BOOL;	//Data for user
	  InEndOfJob  : BOOL ;	//Data for user
	  staWayOk      : BOOL ;	//Data for user
	  EdgeStartRelease : BOOL ;	//Data for user
	  USW8  : BOOL;	//Data for user
	  USW9 : BOOL;	//Data for user
	  USW10: BOOL;	//Data for user
	  USW11: BOOL;	//Data for user
	  USW12: BOOL;	//Data for user
	  USW13: BOOL;	//Data for user
	  USW14: BOOL;	//Data for user
	  USW15: BOOL;	//Data for user
	     #A_3258_MXZ11 : FB13;   // VLS 
	     #A_3259_MXZ11 : FB13;   // VLS 
	END_VAR
	VAR_TEMP
	   tmpEnable		: BOOL;
	   tmpFlag  		: BOOL;
	   tmpCmdNext		: BOOL;
	   tmpLocal  		: BOOL;
	   tmpAspOk  		: BOOL;
	   tmpHWStop 		: BOOL;
	   tmpInstanzDBNo       : INT ;
	END_VAR
	BEGIN
	NETWORK
	TITLE =
	NETWORK
	TITLE =Read command from group control
	//Parameter
	      SET;
	      S   GCS7_GrCtrlDB.ParMGR[301];
	      R   GCS7_GrCtrlDB.ParWGR[301];
	      R   GCS7_GrCtrlDB.ParGGR[301];
	      R   GCS7_GrCtrlDB.ParFeed[301];
	      R   GCS7_GrCtrlDB.ParShare[301];
	      S   #Gr.Par.ParMgr;
	      R   #Gr.Par.ParWgr;
	      R   #Gr.Par.ParGgr;
	//Command
	      U   #GCS7_GrCtrlDB.OutEnable[301];
	      =   #Gr.InEnableGr;
	      U   #GCS7_GrCtrlDB.OutOccupied[301];
	      =   #Gr.InOccupied;
	      U   #GCS7_GrCtrlDB.CmdFaultReset[301];
	      R   #GCS7_GrCtrlDB.CmdFaultReset[301];
	      S   #Gr.CmdFaultReset;
	      U  #GCS7_GrCtrlDB.CmdNext[301];
	      R   #GCS7_GrCtrlDB.CmdNext[301];
	      S   #Gr.CmdNext;
	      U   #GCS7_GrCtrlDB.CmdManualOff[301];
	      O   #GCS7_GrCtrlDB.OutOccupied[301];
	      R   GCS7_GrCtrlDB.CmdManualOff[301];
	      S   #Gr.CmdManualOff;
	      U  GCS7_GrCtrlDB.CmdContinue[301];
	      R   GCS7_GrCtrlDB.CmdContinue[301];
	      S   #Gr.CmdContinue;
	//Init Line Number
	      U   GCS7_GrCtrlDB.ParShare[301];
	      SPBN WE00; 
	      L     0;
	      T     #Gr.InLine;
	WE00: NOP   0;
	NETWORK
	TITLE =Call Methode Control
	// 1. Handle Interface PCControl      
	// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
	// 3. Reset global flags of the group
	      CALL #Gr (
	           FcCode                   := 1);
	NETWORK
	TITLE =Restart Idling Time
		L  #Gr.Par.ParIdling;
		L  2;
		>=I;
		SPB IdTm;
		T  #Gr.Par.ParIdling;
	IdTm:   NOP 0;
	      CLR; 
	      S    #Gr.CmdRestartIdling; 
	NETWORK
	TITLE =Prepare global flags
	NETWORK
	TITLE =Set Pattern (Target position)
	NETWORK
	TITLE =Interlocking WayOk
	//
	NETWORK
	TITLE =Interlocking Autostart
		CLR;
		R   #Gr.OutAutostart;
	NETWORK
	TITLE =Interlocking Enable
	      U     #Gr.InEnableGr; 
	      =     #Gr.OutEnableEl; 
	      
	NETWORK
	TITLE =Interlocking Next
	      U     #Gr.CmdNext;
	      U     #Gr.OutAutostart;
	      UN    #Gr.OutNewWay;
	      =    #tmpCmdNext;
	      
	NETWORK
	TITLE =Interlocking Local / HWStop
	      U    DB050.DBX32.4;
	      =    #tmpHWStop;
	
	      U    #Gr.ModePassive;
	      O    #tmpHWStop;
	      =    #tmpLocal;
	 
	NETWORK
	TITLE =Interlocking Start Warning
	      NOP 0;
	//- Restart after fault --
	      U     #Gr.CmdFaultReset;
	      U     #Gr.VarFault;
	      S       "SYS_StartWarningDB".Area[11].CmdResetStartWarning;
	//- Run Timer --
	      AUF "G301M";
	      CALL  FB100 , DB100(
	       InModeHigher :=FALSE,
	       InArea     :=11,
	       InEnable   :=#Gr.OutEnableEl,
	       InLocal    :=#tmpLocal,
	       InStarted  :=#Gr.VarStarted,
	       InCmdNext  :=#tmpCmdNext,
	       OutCmdNext :=#tmpCmdNext,
	       OutStartwarning:=#Gr.VarStartWarning);
	//- Store Startwarning done  --
	//- Reset Autostart --
	      U     "SYS_StartWarningDB".TmpArea.OutStartReleaseRunningL;
	      FN    #EdgeStartRelease;
	      U     #Gr.VarFault;
	      R     #Gr.OutAutostart;
	NETWORK
	TITLE ==A-3258-MXZ11 : LOUVER HEATING GLASS TUBE HEAT EXCHANGER
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_3258_MXZ11.InLocal; 
	//
	//-Interlocking---
	         =     #A_3258_MXZ11.InEnableHN; 
	         NOT;
	         =     #A_3258_MXZ11.InEnableLN; 
	//
	         SET;
	         S     #A_3258_MXZ11.CmdNext; 
	//
	      U     #Gr.OutSeqStop; 
	      =     #A_3258_MXZ11.InSeqStop; 
	// 
	//-Read inputs---
	      U     E 1007.4; 
	      =     #A_3258_MXZ11.InpLN;
	      U     E 1007.5; 
	      =     #A_3258_MXZ11.InpHN; 
	      X     A 1003.4; 
	      X     E 1003.4; 
	      O;
	      X     A 1003.5; 
	      X     E 1003.5; 
	      =     #A_3258_MXZ11.InpFaultDev; 
	      U   DB050.DBX32.4;
	      =     #A_3258_MXZ11.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[101];
	       =       #A_3258_MXZ11.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_3258_MXZ11;
	// 
	//-Write outputs---
	      U     #A_3258_MXZ11.OutpLN; 
	      =     A 1003.4; 
	      U     #A_3258_MXZ11.OutpHN; 
	      =     A 1003.5; 
	NETWORK
	TITLE ==A-3259-MXZ11 : LOUVER HEATING GLASS TUBE HEAT EXCHANGER
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_3259_MXZ11.InLocal; 
	//
	//-Interlocking---
	      	      U(;
	      U     #A_3258_MXZ11.InEnableLN;
	      U     #A_3258_MXZ11.StLN;
	      O;
	      U     #A_3258_MXZ11.InEnableHN;
	      U     #A_3258_MXZ11.StHN;
	      );
	         =     #A_3259_MXZ11.InEnableHN; 
	         NOT;
	         =     #A_3259_MXZ11.InEnableLN; 
	//
	         SET;
	         S     #A_3259_MXZ11.CmdNext; 
	//
	      U     #Gr.OutSeqStop; 
	      =     #A_3259_MXZ11.InSeqStop; 
	// 
	//-Read inputs---
	      U     E 1007.6; 
	      =     #A_3259_MXZ11.InpLN;
	      U     E 1007.7; 
	      =     #A_3259_MXZ11.InpHN; 
	      X     A 1003.6; 
	      X     E 1003.6; 
	      O;
	      X     A 1003.7; 
	      X     E 1003.7; 
	      =     #A_3259_MXZ11.InpFaultDev; 
	      U   DB050.DBX32.4;
	      =     #A_3259_MXZ11.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[101];
	       =       #A_3259_MXZ11.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_3259_MXZ11;
	// 
	//-Write outputs---
	      U     #A_3259_MXZ11.OutpLN; 
	      =     A 1003.6; 
	      U     #A_3259_MXZ11.OutpHN; 
	      =     A 1003.7; 
	NETWORK
	TITLE =Test Fault
	      U     #A_3258_MXZ11.StFault;
	      O     #A_3259_MXZ11.StFault;
	      =     #Gr.VarFault; 
	NETWORK
	TITLE =Test Warning
	      CLR;   
	      =     #Gr.OutWarning;
	NETWORK
	TITLE =Test Stopped
	      UN   #Gr.VarFault; 
	      =     #Gr.VarStopped; 
	NETWORK
	TITLE =Test Started
	      U     #A_3258_MXZ11.StHN; 
	      U     #A_3259_MXZ11.StHN; 
	      UN  #Gr.VarStopped; 
	      UN  #Gr.VarFault; 
	      =     #Gr.VarStarted; 
	NETWORK
	TITLE =Enable Previous Group
	      U    #Gr.VarStarted;
	      =     #Gr.OutEnablePrevGr;
	NETWORK
	TITLE =Call Methode State
	//1. Make State of the group: This function use the signals:
	//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
	//2.Reset Commands to group: e.g.CmdFaultReset
	      CALL #Gr (
	           FcCode                   := 2);
	NETWORK
	TITLE =Init
	      SET; 
	      R    #CmdNewValue;
	      R    #CmdNewJob;
	NETWORK
	TITLE =Write data to group control
	      U   #Gr.StStopped;
	      =   GCS7_GrCtrlDB.InStopped[301];
	      U   #Gr.StStarted;
	      =   GCS7_GrCtrlDB.InStarted[301];
	      U   #Gr.StFault;
	      =   GCS7_GrCtrlDB.InFault[301];
	      U   #Gr.OutWarning;
	      =   GCS7_GrCtrlDB.InWarning[301];
	      U   #Gr.OutManual;
	      =   GCS7_GrCtrlDB.InManual[301];
	      U   #Gr.OutHWStop;
	      =   GCS7_GrCtrlDB.InHWStop[301];
	      U   #Gr.OutDPFault;
	      =   GCS7_GrCtrlDB.InDPFault[301];
	      U   #Gr.OutLoaded;
	      =   GCS7_GrCtrlDB.InLoaded[301];
	      U   #Gr.OutAutoStart;
	      =   GCS7_GrCtrlDB.InAutoStart[301];
	      U   #Gr.VarStartWarning;
	      =   GCS7_GrCtrlDB.InStartWarning[301];
	NETWORK
	TITLE =End of block
	
	BE
	
	END_FUNCTION_BLOCK
