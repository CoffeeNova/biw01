      A     #A_2860_MXZ01.OutpRunFwd; 

// Netzschuetz "EIN"
      AN    #A_2860_MXZ01.InSeqStop; 
      O     "G250M".A_2860_MXZ01.InManual; 
      O     #Gr.InManual; 
      =     "A_2860_MXZ01_O1"; 

NETWORK
TITLE ==A-2831-QXV12 : VALVE HUMIDIFICATION 1

      NOP   0; 
//-InLocal---
      A     #tmpLocal; 
      =     #A_2831_QXV12.InLocal; 

//-Interlocking---
      A(    ; 
//      L     "MAL1S024Par".StepAct.AirMoisten
//====>BRT100310 - kein Kommentar!
