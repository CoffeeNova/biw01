      R     "GCS7_GrCtrlDB".ParShare[250]; 
      S     #Gr.Par.ParMgr; 
      R     #Gr.Par.ParWgr; 
      R     #Gr.Par.ParGgr; 
//Command
      A     "GCS7_GrCtrlDB".OutEnable[250]; 
      =     #Gr.InEnableGr; 
      A     "GCS7_GrCtrlDB".OutOccupied[250]; 
      =     #Gr.InOccupied; 
      A     "GCS7_GrCtrlDB".CmdFaultReset[250]; 
      R     "GCS7_GrCtrlDB".CmdFaultReset[250]; 
      S     #Gr.CmdFaultReset; 
      A     "GCS7_GrCtrlDB".CmdNext[250]; 
      R     "GCS7_GrCtrlDB".CmdNext[250]; 
      S     #Gr.CmdNext; 

      A     "GCS7_GrCtrlDB".CmdManualOff[250]; 
      AN    #Gr.InManual; 
      S     #Gr.CmdManualOff; 
      A     "GCS7_GrCtrlDB".CmdManualOff[250]; 
      R     "GCS7_GrCtrlDB".CmdManualOff[250]; 

//      U     "GCS7_GrCtrlDB".CmdManualOff[250]
//      O     "GCS7_GrCtrlDB".OutOccupied[250]
//      R     "GCS7_GrCtrlDB".CmdManualOff[250]
//      S     #Gr.CmdManualOff

      A     "GCS7_GrCtrlDB".CmdContinue[250]; 
      R     "GCS7_GrCtrlDB".CmdContinue[250]; 
      S     #Gr.CmdContinue; 
//Init Line Number
      A     "GCS7_GrCtrlDB".ParShare[250]; 
      JCN   WE00; 
      L     0; 
      T     #Gr.InLine; 
WE00: NOP   0; 
NETWORK
TITLE =Call Methode Control
// 1. Handle Interface PCControl      
// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
// 3. Reset global flags of the group
      CALL #Gr (
           FcCode                   := 1);
NETWORK
TITLE =Restart Idling Time

      L     #Gr.Par.ParIdling; 
      L     2; 
      >=I   ; 
      JC    IdTm; 
      T     #Gr.Par.ParIdling; 
IdTm: NOP   0; 
      CLR   ; 
      S     #Gr.CmdRestartIdling; 
NETWORK
TITLE =Prepare global flags



NETWORK
TITLE =Set Pattern (Target position)
////-Target speed fan---
//// If both louvers are closed, the fan speed must reduce  
//      L     "G251M".A_2851_MXZ01.OutPosition    //Fresh Air Flap
//      L     "G252M".A_2850_MXZ01.OutPosition    //Exhaust Air Flap
//      +I    
//      L     200                         //20.0%
//      <I    
//      L     50                          //50% fan speed
//      SPB   En06
//      U     #Gr.InManual
//      SPB   ILO6
//      L     "MAL1S024Par".Par.ParIntMachine.TargetSpeedGermFan
//      SPA   En06
//ILO6: NOP   0
//      L     "MAL1S024Par".Par.ParIntProcess.Speed_ILO_Fan
//En06: NOP   0
//      L     100
//      *I    
//      T     #A_2860_TSC01_COM.OutValuePC    // xxx.xx%  
//
//// If both louvers are closed, the fan speed must reduce  
//      L     "G251M".A_2852_MXZ01.OutPosition    //Fresh Air Flap
//      L     "G252M".A_2850_MXZ01.OutPosition    //Exhaust Air Flap
//      +I    
//      L     200                         //20.0%
//      <I    
//      L     50                          //50% fan speed
//      SPB   En07
//      U     #Gr.InManual
//      SPB   ILO7
//      L     "MAL1S024Par".Par.ParIntMachine.TargetSpeedGermFan
//      SPA   En07
//ILO7: NOP   0
//      L     "MAL1S024Par".Par.ParIntProcess.Speed_ILO_Fan
//En07: NOP   0
//      L     100
//      *I    
//      T     #A_2860_TSC02_COM.OutValuePC    // xxx.xx%  
//BRT100310 - bei 0% Firschluft hat es 100% Umluft, da mechanisch gekoppelt!
//-Target speed fan---
// If both louvers are closed, the fan speed must reduce  
//      L     "G241M".A_2851_MXZ01.OutPosition    //Fresh Air Flap
//      L     "G242M".A_2850_MXZ01.OutPosition    //Exhaust Air Flap
//      +I    
//      L     200                         //20.0%
//      <I    
//      L     50                          //50% fan speed
//      SPB   En06
//17.02.18 donky
// If  summ position of external and internal louvers are smaller than 40%, the fan speed must reduce to minimum (20%)
      L     "G251M".A_2851_MXZ01.OutPosition; 
      L     "G251M".A_2851_MXZ02.OutPosition; 
      >I    ; 
      JCN   A1; 
      TAK   ; 
A1:   T     MW   277; 
      L     "G251M".A_2851_MXZ03.OutPosition; 
      L     0; 
      >I    ; 
      JCN   A2; 
      TAK   ; 
A2:   L     MW   277; 
      +I    ; 
      L     400; 
      <I    ; 
      L     20; 
      JC    En06; 

      A     #Gr.InManual; 
      JC    ILO6; 
      L     "MAL1S024Par".Par.ParIntMachine.TargetSpeedGermFan; 
      AN    "MAL1S024Par".Par.FlagProcess.ProcessEnd; 
      JC    En06; 
      L     20; //BRT170310 - nach Prozess Ende bis Start Entladen mit 20%
      JU    En06; 
ILO6: NOP   0; 
      L     "MAL1S024Par".Par.ParIntProcess.Speed_ILO_Fan; 
En06: NOP   0; 
      L     100; 
      *I    ; 
      T     #A_2860_TSC01_COM.OutValuePC; // xxx.xx%  

//BRT - bei 0% Firschluft hat es 100% Umluft, da mechanisch gekoppelt!
// If both louvers are closed, the fan speed must reduce  
//      L     "G241M".A_2852_MXZ01.OutPosition    //Fresh Air Flap
//      L     "G242M".A_2850_MXZ01.OutPosition    //Exhaust Air Flap
//      +I    
//      L     200                         //20.0%
//      <I    
//      L     50                          //50% fan speed
//      SPB   En07
      A     #Gr.InManual; 
      JC    ILO7; 
//      L     "MAL1S024Par".Par.ParIntMachine.TargetSpeedGermFan
      L     "MAL1S024Par".Par.ParIntProcess.Value26; // Target Speed GermFan 2
      AN    "MAL1S024Par".Par.FlagProcess.ProcessEnd; 
      JC    En07; 
      L     25; //BRT170310 - nach Prozess Ende bis Start Entladen mit 25%
      JU    En07; 
ILO7: NOP   0; 
      L     "MAL1S024Par".Par.ParIntProcess.Speed_ILO_Fan; 
En07: NOP   0; 
      L     100; 
      *I    ; 
      NEGI  ; 
      T     #A_2860_TSC02_COM.OutValuePC; // xxx.xx%  

NETWORK
TITLE =Interlocking WayOk

      L     "G251M".A_2851_MXZ01.OutPosition; //Fresh Air Flap
      L     "G252M".A_2850_MXZ01.OutPosition; //Exhaust Air Flap
      +I    ; 
      L     200; // 20%
      >=I   ; // opened more than min
      =     "G250M".AirMinOpened_1; 

      L     "G251M".A_2851_MXZ02.OutPosition; //Fresh Air Flap
      L     "G252M".A_2850_MXZ01.OutPosition; //Exhaust Air Flap
      +I    ; 
      L     200; // 20%
      >=I   ; // opened more than min
      =     "G250M".AirMinOpened_2; 

NETWORK
TITLE =Interlocking Autostart

      CLR   ; 
      R     #Gr.OutAutostart; 
NETWORK
TITLE =Interlocking Enable
//BRT100310
      AN    "MAL1S024".PCRead.Status.StaImmediateStopASW; 
      A     #Gr.InEnableGr; 
      =     #Gr.OutEnableEl; 

NETWORK
TITLE =Interlocking Next

      A     #Gr.CmdNext; 
      A     #Gr.OutAutostart; 
      AN    #Gr.OutNewWay; 
      =     #tmpCmdNext; 

NETWORK
TITLE =Interlocking Local / HWStop

      A     "PLC_SystemDB".GlobalFlags.Germinating_8; 
//BRT100310 - HW-Stop nur bei NOT-AUS +C28 oder Customer, nicht bei NOT-AUS Maschine!
      A(    ; 
      O     "A_2899_SHE01_I1"; 
      O     "A_2899_SHE02_I1"; 
      O     "A_28_A097Z1_I1"; 
      O     "A_28_A106Z1_I1"; 
      )     ; 
      =     #tmpHWStop; 

      A     #Gr.ModePassive; 
      O     #tmpHWStop; 
      =     #tmpLocal; 

NETWORK
TITLE =Interlocking Start Warning

      NOP   0; 
//- Restart after fault --
      A     #Gr.CmdFaultReset; 
      A     #Gr.VarFault; 
      S     "SYS_StartWarningDB".Area[9].CmdResetStartWarning; 
//- Run Timer --
      OPN   "G250M"; 
      CALL "GCS7_StartWarning" , "SYS_StartWarningDB" (
           InModeHigher             := FALSE,
           InArea                   := 9,
           InEnable                 := #Gr.OutEnableEl,
           InLocal                  := #tmpLocal,
           InStarted                := #Gr.VarStarted,
           InCmdNext                := #tmpCmdNext,
           OutCmdNext               := #tmpCmdNext,
           OutStartwarning          := #Gr.VarStartwarning);
//- Store Startwarning done  --
//- Reset Autostart --
      A     "SYS_StartWarningDB".TmpArea.OutStartReleaseRunningL; 
      FN    #EdgeStartRelease; 
      A     #Gr.VarFault; 
      R     #Gr.OutAutostart; 
NETWORK
TITLE ==A-2860-MXZ01 : GERM FAN 1

      NOP   0; 
//-InLocal--- 
      A     #tmpLocal; 
      =     #A_2860_MXZ01.InLocal; 

//-Interlocking--- 
      A     #Gr.OutEnableEl; 
