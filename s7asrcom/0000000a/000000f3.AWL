	FUNCTION_BLOCK FB 383
	TITLE =G283M CULM BATCH BIN 9005
	//COPYRIGHT:    (C) 2009  BUHLER AG, CH-9240 UZWIL, SWITZERLAND
	//
	//PRODUCT:      WinCoS_BIW01_09
	//
	//TITLE:        CULM BATCH BIN 9005
	//                Machine group control
	//                Maschinengruppensteuerung
	// 
	//--------------------------------------------------------------------------------
	// 
	//IDENT:        AUTHOR:             DEPT:                              DATE:
	// 
	//P01_FB383.1.0 K.Renz              CSB41                              19.Okt.2009
	//             1st Release
	// 
	//--------------------------------------------------------------------------------
	// 
	//PURPOSE: 
	// 
	//MANUAL : 
	// 
	//RESTRICTIONS: 
	// 
	//--------------------------------------------------------------------------------
	AUTHOR  : KR      
	FAMILY  : USW
	VERSION : 9.9
	
	
	VAR
	  Gr       : FB10;	//Standard interface to group
	  InOutValue1    : DINT; //Data for user
	  InOutValue2    : DINT; //Data for user
	  OutWayConflict : BOOL;	//Data for user
	  OutSelectionOk : BOOL;	//Data for user
	  CmdNewValue : BOOL;	//Data for user
	  CmdNewJob   : BOOL;	//Data for user
	  OutCodeOK   : BOOL;	//Data for user
	  InEndOfJob  : BOOL ;	//Data for user
	  staWayOk      : BOOL ;	//Data for user
	  EdgeStartRelease : BOOL ;	//Data for user
	  USW8  : BOOL;	//Data for user
	  USW9 : BOOL;	//Data for user
	  USW10: BOOL;	//Data for user
	  USW11: BOOL;	//Data for user
	  USW12: BOOL;	//Data for user
	  USW13: BOOL;	//Data for user
	  USW14: BOOL;	//Data for user
	  USW15: BOOL;	//Data for user
	     #A_3936_BFT01 : FB84;   // SubType FB14: DigInpCont 
	     #A_3951_BLL01 : FB84;   // SubType FB14: DigInpCont 
	END_VAR
	VAR_TEMP
	   tmpEnable		: BOOL;
	   tmpFlag  		: BOOL;
	   tmpCmdNext		: BOOL;
	   tmpLocal  		: BOOL;
	   tmpAspOk  		: BOOL;
	   tmpHWStop 		: BOOL;
	   tmpInstanzDBNo       : INT ;
	END_VAR
	BEGIN
	NETWORK
	TITLE =
	NETWORK
	TITLE =Read command from group control
	//Parameter
	      SET;
	      S   GCS7_GrCtrlDB.ParMGR[283];
	      R   GCS7_GrCtrlDB.ParWGR[283];
	      R   GCS7_GrCtrlDB.ParGGR[283];
	      R   GCS7_GrCtrlDB.ParFeed[283];
	      R   GCS7_GrCtrlDB.ParShare[283];
	      S   #Gr.Par.ParMgr;
	      R   #Gr.Par.ParWgr;
	      R   #Gr.Par.ParGgr;
	//Command
	      U   #GCS7_GrCtrlDB.OutEnable[283];
	      =   #Gr.InEnableGr;
	      U   #GCS7_GrCtrlDB.OutOccupied[283];
	      =   #Gr.InOccupied;
	      U   #GCS7_GrCtrlDB.CmdFaultReset[283];
	      R   #GCS7_GrCtrlDB.CmdFaultReset[283];
	      S   #Gr.CmdFaultReset;
	      U  #GCS7_GrCtrlDB.CmdNext[283];
	      R   #GCS7_GrCtrlDB.CmdNext[283];
	      S   #Gr.CmdNext;
	      U   #GCS7_GrCtrlDB.CmdManualOff[283];
	      O   #GCS7_GrCtrlDB.OutOccupied[283];
	      R   GCS7_GrCtrlDB.CmdManualOff[283];
	      S   #Gr.CmdManualOff;
	      U  GCS7_GrCtrlDB.CmdContinue[283];
	      R   GCS7_GrCtrlDB.CmdContinue[283];
	      S   #Gr.CmdContinue;
	//Init Line Number
	      U   GCS7_GrCtrlDB.ParShare[283];
	      SPBN WE00; 
	      L     0;
	      T     #Gr.InLine;
	WE00: NOP   0;
	NETWORK
	TITLE =Call Methode Control
	// 1. Handle Interface PCControl      
	// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
	// 3. Reset global flags of the group
	      CALL #Gr (
	           FcCode                   := 1);
	NETWORK
	TITLE =Restart Idling Time
		L  #Gr.Par.ParIdling;
		L  2;
		>=I;
		SPB IdTm;
		T  #Gr.Par.ParIdling;
	IdTm:   NOP 0;
	      CLR; 
	      S    #Gr.CmdRestartIdling; 
	NETWORK
	TITLE =Prepare global flags
	NETWORK
	TITLE =Set Pattern (Target position)
	NETWORK
	TITLE =Interlocking WayOk
	//
	NETWORK
	TITLE =Interlocking Autostart
		CLR;
		R   #Gr.OutAutostart;
	NETWORK
	TITLE =Interlocking Enable
	      U     #Gr.InEnableGr; 
	      =     #Gr.OutEnableEl; 
	      
	NETWORK
	TITLE =Interlocking Next
	      U     #Gr.CmdNext;
	      U     #Gr.OutAutostart;
	      UN    #Gr.OutNewWay;
	      =    #tmpCmdNext;
	      
	NETWORK
	TITLE =Interlocking Local / HWStop
	      U    DB050.DBX31.0;
	      =    #tmpHWStop;
	
	      U    #Gr.ModePassive;
	      O    #tmpHWStop;
	      =    #tmpLocal;
	 
	NETWORK
	TITLE =Interlocking Start Warning
	      NOP 0;
	//- Restart after fault --
	      U     #Gr.CmdFaultReset;
	      U     #Gr.VarFault;
	      S       "SYS_StartWarningDB".Area[12].CmdResetStartWarning;
	//- Run Timer --
	      AUF "G283M";
	      CALL  FB100 , DB100(
	       InModeHigher :=FALSE,
	       InArea     :=12,
	       InEnable   :=#Gr.OutEnableEl,
	       InLocal    :=#tmpLocal,
	       InStarted  :=#Gr.VarStarted,
	       InCmdNext  :=#tmpCmdNext,
	       OutCmdNext :=#tmpCmdNext,
	       OutStartwarning:=#Gr.VarStartWarning);
	//- Store Startwarning done  --
	//- Reset Autostart --
	      U     "SYS_StartWarningDB".TmpArea.OutStartReleaseRunningL;
	      FN    #EdgeStartRelease;
	      U     #Gr.VarFault;
	      R     #Gr.OutAutostart;
	NETWORK
	TITLE ==A-3936-BFT01 : HIGH LEVEL MALT IN CULM BIN
	NOP 0;
	//-Local---
	      SET;
	      =      #A_3936_BFT01.InLocal; 
	// 
	//-Read inputs---
	      UN    E 1141.6; 
	      =     #A_3936_BFT01.InpCovered; 
	      U   DB050.DBX32.5;
	      =     #A_3936_BFT01.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[115];
	       =       #A_3936_BFT01.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_3936_BFT01;
	// 
	//-Write outputs---
	NETWORK
	TITLE ==A-3951-BLL01 : LOW LEVEL MALT IN CULM BIN
	NOP 0;
	//-Local---
	      U    #A_3951_BLL01.InpCovered; 
	      =      #A_3951_BLL01.InLocal; 
	// 
	//-Monitoring---
	      SET;
	      =      #A_3951_BLL01.InMonCovered; 
	// 
	//-Read inputs---
	      UN    E 1143.1; 
	      =     #A_3951_BLL01.InpCovered; 
	      U   DB050.DBX32.5;
	      =     #A_3951_BLL01.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[115];
	       =       #A_3951_BLL01.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_3951_BLL01;
	// 
	//-Write outputs---
	NETWORK
	TITLE =Test Fault
	      U     #A_3936_BFT01.StFault;
	      O     #A_3951_BLL01.StFault;
	      =     #Gr.VarFault; 
	NETWORK
	TITLE =Test Warning
	      CLR;   
	      =     #Gr.OutWarning;
	NETWORK
	TITLE =Test Stopped
	      UN   #Gr.VarFault; 
	      =     #Gr.VarStopped; 
	NETWORK
	TITLE =Test Started
	      UN  #Gr.VarStopped; 
	      UN  #Gr.VarFault; 
	      =     #Gr.VarStarted; 
	NETWORK
	TITLE =Enable Previous Group
	      U    #Gr.VarStarted;
	      =     #Gr.OutEnablePrevGr;
	NETWORK
	TITLE =Call Methode State
	//1. Make State of the group: This function use the signals:
	//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
	//2.Reset Commands to group: e.g.CmdFaultReset
	      CALL #Gr (
	           FcCode                   := 2);
	NETWORK
	TITLE =Init
	      SET; 
	      R    #CmdNewValue;
	      R    #CmdNewJob;
	NETWORK
	TITLE =Write data to group control
	      U   #Gr.StStopped;
	      =   GCS7_GrCtrlDB.InStopped[283];
	      U   #Gr.StStarted;
	      =   GCS7_GrCtrlDB.InStarted[283];
	      U   #Gr.StFault;
	      =   GCS7_GrCtrlDB.InFault[283];
	      U   #Gr.OutWarning;
	      =   GCS7_GrCtrlDB.InWarning[283];
	      U   #Gr.OutManual;
	      =   GCS7_GrCtrlDB.InManual[283];
	      U   #Gr.OutHWStop;
	      =   GCS7_GrCtrlDB.InHWStop[283];
	      U   #Gr.OutDPFault;
	      =   GCS7_GrCtrlDB.InDPFault[283];
	      U   #Gr.OutLoaded;
	      =   GCS7_GrCtrlDB.InLoaded[283];
	      U   #Gr.OutAutoStart;
	      =   GCS7_GrCtrlDB.InAutoStart[283];
	      U   #Gr.VarStartWarning;
	      =   GCS7_GrCtrlDB.InStartWarning[283];
	NETWORK
	TITLE =End of block
	
	BE
	
	END_FUNCTION_BLOCK
