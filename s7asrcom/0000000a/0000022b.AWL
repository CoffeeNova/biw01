M312: NOP   0; 

      U     "MAL1S032".ToASW.InfDataReady; 
      U     "MAL1S032Par".Par.FlagFunction_1.StepLoading; 
      UN    "MAL1S032Par".Par.FlagFunction_1.FinishLoading; 
//MW 2010.04.06      UN    "MAL1S032Par".Par.FlagProcess.ProcessActive
      SPBN  M301; 
      L     1; // Loading
      SPA   M030; 
M301: NOP   0; 

      U     "MAL1S032Par".Par.FlagProcess.TurningFloor; 
