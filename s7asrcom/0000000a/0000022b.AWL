      SPBN  M307; 
      L     7; // Cooling Active
      SPA   M030; 
M307: NOP   0; 

      U     "MAL1S032".ToASW.InfSameBatchPrevSec; 
      U     "MAL1S032".ToASW.InfDataReady; 
      U     "MAL1S032Par".Par.FlagProcess.ProcessEnd; 
      UN    "MAL1S032Par".Par.FlagProcess.ProcessActive; 
      UN    "MAL1S032Par".Par.FlagFunction_1.StepLoading; 
      UN    "MAL1S032Par".Par.FlagFunction_1.StepUnloading; 
      SPBN  M313; 
      L     13; // Can not be unloaded! Same batch in previous section!
      SPA   M030; 
M313: NOP   0; 

      U     "MAL1S032".ToASW.InfDataReady; 
      UN    "MAL1S032".ToASW.InfSameBatchPrevSec; 
      U     "MAL1S032Par".Par.FlagProcess.ProcessEnd; 
//MW 2010.04.06      UN    "MAL1S032Par".Par.FlagProcess.ProcessActive
      UN    "MAL1S032Par".Par.FlagFunction_1.StepUnloading; 
      UN    "MAL1S032Par".Par.FlagFunction_1.FinishUnloading; 
      UN    "MAL1S032Par".Par.FlagFunction_1.StepCleaning; 
      UN    "MAL1S032Par".Par.FlagFunction_1.FinishCleaning; 
      SPBN  M308; 
      L     8; // Wait for Unloading
      SPA   M030; 
M308: NOP   0; 

      U     "SYS_infLog0"; 
      SPBN  M316; 
      L     16; // Process aktive and wait for water! Water not filled!
      SPA   M030; 
M316: NOP   0; 

M030: T     "MAL1S032Par".SectionStatus; 

