M304: NOP   0; 

      U     "MAL1S032".ToASW.InfDataReady; 
      U     "MAL1S032Par".Par.FlagFunction_1.FinishUnloading; 
      UN    "MAL1S032Par".Par.FlagFunction_1.StepCleaning; 
      UN    "MAL1S032Par".Par.FlagFunction_1.FinishCleaning; 
      SPBN  M305; 
      L     5; // Finish unloading
      SPA   M030; 
M305: NOP   0; 

      U     "SYS_infLog0"; 
      SPBN  M306; 
      L     6; // Process End
      SPA   M030; 
M306: NOP   0; 

      U     "MAL1S032Par".Par.FlagProcess.ProcessActive; // Process active
