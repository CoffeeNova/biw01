      U     #Gr.StStopped; 
      =     "GCS7_GrCtrlDB".InStopped[276]; 
      U     #Gr.StStarted; 
      =     "GCS7_GrCtrlDB".InStarted[276]; 
      U     #Gr.StFault; 
      =     "GCS7_GrCtrlDB".InFault[276]; 
