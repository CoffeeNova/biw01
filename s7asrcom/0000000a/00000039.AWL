NETWORK
TITLE =Test Fault

      U     #A_3191_BPS21.StFault; 
      O     #A_3161_KCL12.StFault; 
      O     #A_3161_KCL12_EXT_ON.StFault; 
      O     #A_3161_KCL12_ON.StFault; 
      O     #A_3161_KCL12_RUN.StFault; 
//      O     #A_3161_KCL12_AI.OutFaulted
