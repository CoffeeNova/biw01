      U     #A_2851_BZS02.StTrue; // 100% Frischluft
      SPBN  FL1; 
      L     1000; 
      SPA   INP1; 
FL1:  NOP   0; 
      U     #A_2851_BZS01.StTrue; // 100% Umluft
      SPBN  RL1; 
      L     0; 
      SPA   INP1; 
RL1:  NOP   0; 
