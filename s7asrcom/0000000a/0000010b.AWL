//-Interlocking---
      U     #A_2852_MXZ01.Par.ParManual; 
      U     #A_2852_MXZ01.InManual; 
      SPB   man2; 
      U(    ; 
      L     0; 
      L     #A_2852_MXZ01.InPositionMiddle; // setpoint element
      ==I   ; 
      )     ; 
      U(    ; 
      U     #Gr.InManual; 
      U     #Gr.InOutAutostartMan; 
      ON    #Gr.InManual; 
      )     ; 
      U     #Gr.OutEnableEl; 
      =     #A_2852_MXZ01.InLN; 

