//       InLocal        :=#tmpLocal
//       InStarted      :=#Gr.VarStarted
//       InCmdNext      :=#tmpCmdNext
//       OutCmdNext     :=#tmpCmdNext
//       OutStartwarning:=#Gr.VarStartwarning
////- Store Startwarning done  --
////- Reset Autostart --
//      U     "SYS_StartWarningDB".TmpArea.OutStartReleaseRunningL
//      FN    #EdgeStartRelease
//      U     #Gr.VarFault
//      R     #Gr.OutAutostart
//


NETWORK
TITLE ==A-1127-MXZ21 : PRODUCT SLIDE ST 17

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_1127_MXZ21.InLocal; 
//
//-Interlocking---
      CLR   ; // No automatic --> only manual
      =     #A_1127_MXZ21.InEnableHN; 
      NOT   ; 
      =     #A_1127_MXZ21.InEnableLN; 
//
      SET   ; 
      S     #A_1127_MXZ21.CmdNext; 
//
      U     #Gr.OutSeqStop; 
      =     #A_1127_MXZ21.InSeqStop; 
// 
//-Read inputs---
//      U     "A_1127_MXZ21_I1"
      UN    "A_1127_MXZ21_I1"; 
      =     #A_1127_MXZ21.InpLN; 
//      U     "A_1127_MXZ21_I2"
      UN    "A_1127_MXZ21_I2"; 
      =     #A_1127_MXZ21.InpHN; 
      U     "PLC_SystemDB".HardwareStop.Steeping; 
      =     #A_1127_MXZ21.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[5]; 
      =     #A_1127_MXZ21.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_1127_MXZ21 ;
// 
//-Write outputs---
      U     #A_1127_MXZ21.OutpLN; 
      =     "A_1127_MXZ21_O1"; 
      U     #A_1127_MXZ21.OutpHN; 
      =     "A_1127_MXZ21_O2"; 
NETWORK
TITLE =Test Fault

      U     #A_1127_MXZ21.StFault; 
      =     #Gr.VarFault; 
NETWORK
TITLE =Test Warning

      CLR   ; 
      =     #Gr.OutWarning; 
NETWORK
TITLE =Test Stopped

      U     #A_1127_MXZ21.StLN; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStopped; 
NETWORK
TITLE =Test Started

      U     #A_1127_MXZ21.StHN; 
      UN    #Gr.VarStopped; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStarted; 
NETWORK
TITLE =Enable Previous Group

      U     #Gr.VarStarted; 
      =     #Gr.OutEnablePrevGr; 
NETWORK
TITLE =Call Methode State
//1. Make State of the group: This function use the signals:
//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
//2.Reset Commands to group: e.g.CmdFaultReset
      CALL #Gr (
           FcCode                   := 2);
NETWORK
TITLE =Init

      SET   ; 
      R     #CmdNewValue; 
      R     #CmdNewJob; 
NETWORK
TITLE =Write data to group control

      U     #Gr.StStopped; 
      =     "GCS7_GrCtrlDB".InStopped[66]; 
      U     #Gr.StStarted; 
      =     "GCS7_GrCtrlDB".InStarted[66]; 
      U     #Gr.StFault; 
      =     "GCS7_GrCtrlDB".InFault[66]; 
      U     #Gr.OutWarning; 
      =     "GCS7_GrCtrlDB".InWarning[66]; 
      U     #Gr.OutManual; 
      =     "GCS7_GrCtrlDB".InManual[66]; 
      U     #Gr.OutHWStop; 
      =     "GCS7_GrCtrlDB".InHWStop[66]; 
      U     #Gr.OutDPFault; 
      =     "GCS7_GrCtrlDB".InDPFault[66]; 
      U     #Gr.OutLoaded; 
      =     "GCS7_GrCtrlDB".InLoaded[66]; 
      U     #Gr.OutAutostart; 
      =     "GCS7_GrCtrlDB".InAutoStart[66]; 
      U     #Gr.VarStartwarning; 
      =     "GCS7_GrCtrlDB".InStartWarning[66]; 
NETWORK
TITLE =End of block

      BE    ; 

END_FUNCTION_BLOCK

