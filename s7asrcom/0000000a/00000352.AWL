BEGIN
NETWORK
TITLE =



NETWORK
TITLE =Read command from group control
//Parameter
      SET   ; 
      S     "GCS7_GrCtrlDB".ParMGR[244]; 
      R     "GCS7_GrCtrlDB".ParWGR[244]; 
      R     "GCS7_GrCtrlDB".ParGGR[244]; 
      R     "GCS7_GrCtrlDB".ParFeed[244]; 
      R     "GCS7_GrCtrlDB".ParShare[244]; 
      S     #Gr.Par.ParMgr; 
      R     #Gr.Par.ParWgr; 
      R     #Gr.Par.ParGgr; 
//Command
      U     "GCS7_GrCtrlDB".OutEnable[244]; 
      O(    ; 
      UN    "MAL1S024Par".Par.FlagFunction_1.StepLoading; 
      UN    "MAL1S024Par".Par.FlagProcess.ProcessActive; 
      )     ; 
      =     #Gr.InEnableGr; 
      U     "GCS7_GrCtrlDB".OutOccupied[244]; 
      O(    ; 
      UN    "MAL1S024Par".Par.FlagFunction_1.StepLoading; 
      UN    "MAL1S024Par".Par.FlagProcess.ProcessActive; 
      )     ; 
      =     #Gr.InOccupied; 
      U     "GCS7_GrCtrlDB".CmdFaultReset[244]; 
      R     "GCS7_GrCtrlDB".CmdFaultReset[244]; 
      S     #Gr.CmdFaultReset; 
      U     "GCS7_GrCtrlDB".CmdNext[244]; 
      O     "G247M".A_2815_KCL01I5.OutTrue; // pushbutton lock wall
      O     "G247M".A_2815_KCL01I6.OutTrue; // pushbutton unlock wall
      R     "GCS7_GrCtrlDB".CmdNext[244]; 
      S     #Gr.CmdNext; 

      U     "GCS7_GrCtrlDB".CmdManualOff[244]; 
      UN    #Gr.InManual; 
//BRT100310      SET                               // No PC-Direct-Controll!!!
      S     #Gr.CmdManualOff; 
      U     "GCS7_GrCtrlDB".CmdManualOff[244]; 
      R     "GCS7_GrCtrlDB".CmdManualOff[244]; 

//      U     "GCS7_GrCtrlDB".CmdManualOff[244]
//      O     "GCS7_GrCtrlDB".OutOccupied[244]
//      R     "GCS7_GrCtrlDB".CmdManualOff[244]
//      S     #Gr.CmdManualOff

      U     "GCS7_GrCtrlDB".CmdContinue[244]; 
      R     "GCS7_GrCtrlDB".CmdContinue[244]; 
      S     #Gr.CmdContinue; 
//Init Line Number
      U     "GCS7_GrCtrlDB".ParShare[244]; 
      SPBN  WE00; 
      L     0; 
      T     #Gr.InLine; 
WE00: NOP   0; 
NETWORK
TITLE =Call Methode Control
// 1. Handle Interface PCControl      
// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
// 3. Reset global flags of the group
      CALL #Gr (
           FcCode                   := 1);
NETWORK
TITLE =Restart Idling Time

      L     #Gr.Par.ParIdling; 
      L     2; 
      >=I   ; 
      SPB   IdTm; 
      T     #Gr.Par.ParIdling; 
IdTm: NOP   0; 
      CLR   ; 
      S     #Gr.CmdRestartIdling; 
NETWORK
TITLE =Prepare global flags



NETWORK
TITLE =Set Pattern (Target position)



NETWORK
TITLE =Interlocking WayOk

      U     "G244M".A_2815_BZS43.StTrue; // drive 1 locked
      U     "G244M".A_2815_BZS47.StTrue; // drive 2 locked
      =     "MAL1S024Par".Par.FlagMachine_0.WallCoupled; 

      U     "G244M".A_2815_BZS42.StTrue; // drive 1 locked
      U     "G244M".A_2815_BZS46.StTrue; // drive 2 locked
      =     "MAL1S024Par".Par.FlagMachine_0.WallUnCoupled; 

NETWORK
TITLE =Interlocking Autostart

      CLR   ; 
      R     #Gr.OutAutostart; 
NETWORK
TITLE =Interlocking Enable

      U     #Gr.InEnableGr; 
      =     #Gr.OutEnableEl; 

NETWORK
TITLE =Interlocking Next

      U     #Gr.CmdNext; 
//====>BRT100310
      U(    ; 
      U     #Gr.OutAutostart; 
      O     "G247M".A_2815_KCL01I5.OutTrue; // pushbutton lock wall
      O     "G247M".A_2815_KCL01I6.OutTrue; // pushbutton unlock wall
      )     ; 
//<====BRT100310
      UN    #Gr.OutNewWay; 
      =     #tmpCmdNext; 

NETWORK
TITLE =Interlocking Local / HWStop

      U     "PLC_SystemDB".GlobalFlags.Germinating_8; 
      =     #tmpHWStop; 

      U     #Gr.ModePassive; 
      O     #tmpHWStop; 
      =     #tmpLocal; 

NETWORK
TITLE ==A-2815-BZS42 : COUPLING DRIVE UNLOCKED

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2815_BZS42.InLocal; 
// 
//-Monitoring---
      U     "MAL1S024Par".Par.FlagFunction_1.StepUnloading; // Step unloading
      O     "MAL1S024Par".Par.FlagFunction_1.StepCleaning; // Step cleaning
      UN    #A_2815_BZS42.InpTrue; //BRT100310
      =     #A_2815_BZS42.InMonTrue; 
      CLR   ; 
      =     #A_2815_BZS42.InMonFalse; 
// 
//-PreAlarm---
      CLR   ; 
      =     #A_2815_BZS42.InPreAlarmTrue; 
      CLR   ; 
      =     #A_2815_BZS42.InPreAlarmFalse; 
// 
//-Additional commands---
//      CLR
//      S     #A_2815_BZS42.CmdRunTimeOutTime
// 
//-Read inputs---
      U     "A_2815_BZS42_I1"; 
      =     #A_2815_BZS42.InpTrue; 
      U     "PLC_SystemDB".GlobalFlags.Germinating_8; 
      =     #A_2815_BZS42.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[85]; 
      =     #A_2815_BZS42.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2815_BZS42 ;
// 
//-Write outputs---
NETWORK
TITLE ==A-2815-BZS43 : COUPLING DRIVE LOCKED

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2815_BZS43.InLocal; 
// 
//-Monitoring---
      U     "MAL1S024Par".Par.FlagFunction_1.StepLoading; 
      O     "MAL1S024Par".Par.FlagProcess.ProcessActive; 
      UN    #A_2815_BZS43.InpTrue; //BRT100310
      =     #A_2815_BZS43.InMonTrue; 
      CLR   ; 
      =     #A_2815_BZS43.InMonFalse; 
// 
//-PreAlarm---
      CLR   ; 
      =     #A_2815_BZS43.InPreAlarmTrue; 
      CLR   ; 
      =     #A_2815_BZS43.InPreAlarmFalse; 
// 
//-Additional commands---
//      CLR
//      S     #A_2815_BZS43.CmdRunTimeOutTime
// 
//-Read inputs---
      U     "A_2815_BZS43_I1"; 
      =     #A_2815_BZS43.InpTrue; 
      U     "PLC_SystemDB".GlobalFlags.Germinating_8; 
      =     #A_2815_BZS43.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[85]; 
      =     #A_2815_BZS43.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2815_BZS43 ;
// 
//-Write outputs---
NETWORK
TITLE ==A-2815-BZS47 : COUPLING DRIVE LOCKED

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2815_BZS47.InLocal; 
// 
//-Monitoring---
      U     "MAL1S024Par".Par.FlagFunction_1.StepLoading; 
      O     "MAL1S024Par".Par.FlagProcess.ProcessActive; 
      UN    #A_2815_BZS47.InpTrue; //BRT100310
      =     #A_2815_BZS47.InMonTrue; 
      CLR   ; 
      =     #A_2815_BZS47.InMonFalse; 
// 
//-PreAlarm---
      CLR   ; 
      =     #A_2815_BZS47.InPreAlarmTrue; 
      CLR   ; 
      =     #A_2815_BZS47.InPreAlarmFalse; 
// 
//-Additional commands---
//      CLR
