// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2815_BZS47 ;
// 
//-Write outputs---
NETWORK
TITLE ==A-2815-BZS46 : COUPLING DRIVE UNLOCKED

      NOP   0; 
//-InLocal---
      U     #tmpLocal; 
      =     #A_2815_BZS46.InLocal; 
// 
//-Monitoring---
      U     "MAL1S024Par".Par.FlagFunction_1.StepUnloading; // Step unloading
      O     "MAL1S024Par".Par.FlagFunction_1.StepCleaning; // Step cleaning
      UN    #A_2815_BZS46.InpTrue; //BRT100310
      =     #A_2815_BZS46.InMonTrue; 
      CLR   ; 
