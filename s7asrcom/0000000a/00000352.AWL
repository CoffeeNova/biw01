// 
//-PreAlarm---
      CLR   ; 
      =     #A_2815_BZS46.InPreAlarmTrue; 
      CLR   ; 
      =     #A_2815_BZS46.InPreAlarmFalse; 
// 
//-Additional commands---
//      CLR
//      S     #A_2815_BZS46.CmdRunTimeOutTime
// 
//-Read inputs---
      U     "A_2815_BZS46_I1"; 
      =     #A_2815_BZS46.InpTrue; 
      U     "PLC_SystemDB".GlobalFlags.Germinating_8; 
      =     #A_2815_BZS46.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[85]; 
      =     #A_2815_BZS46.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2815_BZS46 ;
// 
//-Write outputs---
NETWORK
TITLE ==A-2815-MXZ41 : WALL COUPLING DRIVE

      NOP   0; 
//-InLocal--- 
      U     #tmpLocal; 
      =     #A_2815_MXZ41.InLocal; 
//
//-Interlocking--- 
      U     #Gr.OutEnableEl; 
      U     "G247M".A_2815_KCL01I5.InpTrue; // pushbutton lock wall
      UN    #A_2815_BZS43.StTrue; // locked
      =     #A_2815_MXZ41.InEnableFwd; 
      U     #Gr.OutEnableEl; 
      U     #A_2815_BZS46.StTrue; // unlocked drive 2
      U     "G247M".A_2815_KCL01I6.InpTrue; // pushbutton unlock wall
      UN    #A_2815_BZS42.StTrue; // unlocked
      U     "MAL1S024Par".Par.FlagMachine_0.CouplingDischargFinished; 
      =     #A_2815_MXZ41.InEnableRev; 
// 
      U     #tmpCmdNext; 
      S     #A_2815_MXZ41.CmdNext; 
// 
      U     #Gr.OutSeqStop; 
      =     #A_2815_MXZ41.InSeqStop; 
// 
//-Read inputs---
      U     #A_2815_MXZ41.OutpRunRev; 
      =     #A_2815_MXZ41.InpRunRev; 
      U     #A_2815_MXZ41.OutpRunFwd; 
      =     #A_2815_MXZ41.InpRunFwd; 
      U     #A_2815_MXZ41.OutpRunFwd; 
      =     #A_2815_MXZ41.InpRunFwd; 
      U     #A_2815_MXZ41.OutpRunRev; 
      =     #A_2815_MXZ41.InpRunRev; 
      U     "PLC_SystemDB".GlobalFlags.Germinating_8; 
      =     #A_2815_MXZ41.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[85]; 
      =     #A_2815_MXZ41.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2815_MXZ41 ;
// 
//-Write outputs---
      U     #A_2815_MXZ41.OutpRunFwd; 
//BRT100310      =     "A_2815_MXZ41_O1"
      =     "A_2815_MXZ41_O2"; 
      U     #A_2815_MXZ41.OutpRunRev; 
//BRT100310      =     "A_2815_MXZ41_O2"
      =     "A_2815_MXZ41_O1"; 
NETWORK
TITLE ==A-2815-MXZ45 : WALL COUPLING DRIVE

      NOP   0; 
//-InLocal--- 
      U     #tmpLocal; 
      =     #A_2815_MXZ45.InLocal; 
//
//-Interlocking--- 
      U     #Gr.OutEnableEl; 
      U     "G247M".A_2815_KCL01I5.InpTrue; // pushbutton lock wall
      U     #A_2815_BZS43.StTrue; // locked drive 1
      UN    #A_2815_BZS47.StTrue; // locked
      =     #A_2815_MXZ45.InEnableFwd; 
      U     #Gr.OutEnableEl; 
      U     "G247M".A_2815_KCL01I6.InpTrue; // pushbutton unlock wall
      UN    #A_2815_BZS46.StTrue; // unlocked
      U     "MAL1S024Par".Par.FlagMachine_0.CouplingDischargFinished; 
      =     #A_2815_MXZ45.InEnableRev; 
// 
      U     #tmpCmdNext; 
      S     #A_2815_MXZ45.CmdNext; 
// 
      U     #Gr.OutSeqStop; 
      =     #A_2815_MXZ45.InSeqStop; 
// 
//-Read inputs---
      U     #A_2815_MXZ45.OutpRunRev; 
      =     #A_2815_MXZ45.InpRunRev; 
      U     #A_2815_MXZ45.OutpRunFwd; 
      =     #A_2815_MXZ45.InpRunFwd; 
      U     #A_2815_MXZ45.OutpRunFwd; 
      =     #A_2815_MXZ45.InpRunFwd; 
      U     #A_2815_MXZ45.OutpRunRev; 
      =     #A_2815_MXZ45.InpRunRev; 
      U     "PLC_SystemDB".GlobalFlags.Germinating_8; 
      =     #A_2815_MXZ45.InHWStop; 
      U     "PLC_DPInfo".Master[1].SlavesError[85]; 
      =     #A_2815_MXZ45.InDPFault; 
// 
//-Set Parameters---
// 
//-Call object control---
      CALL #A_2815_MXZ45 ;
// 
//-Write outputs---
      U     #A_2815_MXZ45.OutpRunFwd; 
//BRT100310      =     "A_2815_MXZ45_O1"
      =     "A_2815_MXZ45_O2"; 
      U     #A_2815_MXZ45.OutpRunRev; 
//BRT100310      =     "A_2815_MXZ45_O2"
      =     "A_2815_MXZ45_O1"; 
NETWORK
TITLE =Test Fault

      U     #A_2815_BZS42.StFault; 
      O     #A_2815_BZS43.StFault; 
      O     #A_2815_BZS47.StFault; 
      O     #A_2815_BZS46.StFault; 
      O     #A_2815_MXZ41.StFault; 
      O     #A_2815_MXZ45.StFault; 
      =     #Gr.VarFault; 
NETWORK
TITLE =Test Warning

      CLR   ; 
      =     #Gr.OutWarning; 
NETWORK
TITLE =Test Stopped

      UN    #Gr.VarStarted; 
//BRT100310      UN    #Gr.VarFault
      =     #Gr.VarStopped; 
NETWORK
TITLE =Test Started
//      U     #A_2815_MXZ41.StStartedFwd
//      U     #A_2815_MXZ45.StStartedFwd
//      UN    #Gr.VarStopped
//      UN    #Gr.VarFault
//      =     #Gr.VarStarted
//
      U     #Gr.InOccupied; 
      U(    ; 
      O(    ; 
      UN    "MAL1S024Par".Par.FlagFunction_1.StepUnloading; 
      U     #A_2815_BZS43.StTrue; // drive 1 locked
      U     #A_2815_BZS47.StTrue; // drive 2 locked
      )     ; 
      O(    ; 
      U     "MAL1S024Par".Par.FlagFunction_1.StepUnloading; 
      O     "MAL1S024Par".Par.FlagFunction_1.FinishUnloading; 
      O     "MAL1S024Par".Par.FlagMachine_0.CmdTurnRevAfterUnloading; 
      U     #A_2815_BZS42.StTrue; // drive 1 unlocked
      U     #A_2815_BZS46.StTrue; // drive 2 unlocked
      )     ; 
      )     ; 
      UN    #Gr.VarFault; 
      =     #Gr.VarStarted; 

NETWORK
TITLE =Enable Previous Group

      U     #Gr.VarStarted; 
      =     #Gr.OutEnablePrevGr; 
NETWORK
TITLE =Call Methode State
//1. Make State of the group: This function use the signals:
//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
//2.Reset Commands to group: e.g.CmdFaultReset
      CALL #Gr (
           FcCode                   := 2);
NETWORK
TITLE =Init

      SET   ; 
      R     #CmdNewValue; 
      R     #CmdNewJob; 
NETWORK
TITLE =Write data to group control

      U     #Gr.StStopped; 
      =     "GCS7_GrCtrlDB".InStopped[244]; 
      U     #Gr.StStarted; 
      =     "GCS7_GrCtrlDB".InStarted[244]; 
      U     #Gr.StFault; 
      =     "GCS7_GrCtrlDB".InFault[244]; 
      U     #Gr.OutWarning; 
      =     "GCS7_GrCtrlDB".InWarning[244]; 
      U     #Gr.OutManual; 
      =     "GCS7_GrCtrlDB".InManual[244]; 
      U     #Gr.OutHWStop; 
      =     "GCS7_GrCtrlDB".InHWStop[244]; 
      U     #Gr.OutDPFault; 
      =     "GCS7_GrCtrlDB".InDPFault[244]; 
      U     #Gr.OutLoaded; 
      =     "GCS7_GrCtrlDB".InLoaded[244]; 
      U     #Gr.OutAutostart; 
      =     "GCS7_GrCtrlDB".InAutoStart[244]; 
      U     #Gr.VarStartwarning; 
      =     "GCS7_GrCtrlDB".InStartWarning[244]; 
NETWORK
TITLE =End of block

      BE    ; 

END_FUNCTION_BLOCK

