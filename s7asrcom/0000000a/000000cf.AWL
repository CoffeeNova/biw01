      ==I   ; // Cooling off
      )     ; 
      ON    "G105M".A_2160_MXZ01.StStartedFwd; 
      ON    "G105M".A_2160_MXZ02.StStartedFwd; 
      O(    ; 
      L     "G269M".A_3191_BTE11.OutValueUnits; //WET BULB TEMPERATURE OUTSIDE
      L     10; // 1,0C Hysteresis
      +D    ; 
      L     "MAL1S017Par".StepAct.TemperaturBelowFloor; //SET POINT TEMPERATURE RECIPE
      <D    ; 
      )     ; 
      O(    ; 
      L     "G103M".A_2191_BTE02.OutValueUnits; //AIR TEMPERATURE ABOVE FLOOR GM1
      L     10; // 1,0C Hysteresis
      +D    ; 
      L     "MAL1S017Par".StepAct.TemperaturBelowFloor; //SET POINT TEMPERATURE RECIPE
      <D    ; 
      )     ; 
      R     "MAL1S017Par".Par.FlagProcess.Cooling_Activ; 

      U     "MAL1S017Par".Par.FlagProcess.Cooling_Activ; 
      SPB   summ; 

// Check the minimum opening of fresh air louver
      U(    ; 
