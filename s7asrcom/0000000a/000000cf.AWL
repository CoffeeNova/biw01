      SET   ; 
      S     "MAL1S017StepValues".SendData; // send average step data to PIC
// Next step
      L     "MAL1S017Par".Par.ParIntMachine.ActualStep; 
      L     "MAL1S017".SecNom.GERM.RecipeHeader.NumberOfSteps; 
      >I    ; 
      SPB   Pend; // Last step is finished
// 
      L     "MAL1S017Par".Par.ParIntMachine.ActualStep; 
      L     1; 
      +I    ; 
      T     "MAL1S017Par".Par.ParIntMachine.ActualStep; 
      L     0; 
      T     "MAL1S017Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
      SET   ; 
      R     "MAL1S017Par".Par.FlagProcess.MessageNoNextStep; // Meldung "No next step"
      SPA   E000; 
Pend: NOP   0; // Process has finished
      SET   ; 
      R     "MAL1S017Par".Par.FlagProcess.ProcessActive; 
      S     "MAL1S017Par".Par.FlagProcess.ProcessEnd; // germinating Process finished
// 
      L     0; // clear step number
      T     "MAL1S017Par".Par.ParIntMachine.ActualStep; 
E000: NOP   0; 

NETWORK
TITLE =ASW_6020: Copy germinating-recipe-data in Parameter DB

      U     "MAL1S017Par".Par.FlagProcess.ProcessActive; 
      SPB   NoCl; 
// Clear process time in buffer
      L     0; 
      T     "MAL1S017Par".ActTimeStepProcess.SteptimeActHour; 
      T     "MAL1S017Par".ActTimeStepProcess.SteptimeActMin; 
      T     "MAL1S017Par".ActTimeStepProcess.SteptimeActMinOverall; 

      T     "MAL1S017Par".Processtime.Day; 
      T     "MAL1S017Par".Processtime.Hour; 
      T     "MAL1S017Par".Processtime.Min; 
      T     "MAL1S017Par".Processtime.ProcessTimeActive; 
      L     L#0; // clear actual step time
      T     "MAL1S017Par".Par.ProcessValue.StepTimeAct; // in ms

NoCl: NOP   0; 
// Clear content recipe in buffer
      L     0; 
      T     #tmpInt; 
      CALL "SIEMENS_FILL" (
           BVAL                     := #tmpInt,
           RET_VAL                  := #tmpRET,
           BLK                      := "MAL1S017Par".RecipeHeader);
// 
      U     "MAL1S017".ToASW.InfDataReady; 
      SPBN  No00; 
// Germ: Copy step loading,unloading,cleaning to parameterDB
      CALL "SIEMENS_BLKMOV" (
           SRCBLK                   := "MAL1S017".SecNom.GERM.RecipeHeader.Loading,
           RET_VAL                  := "MAL1S017Par".Par.Error.Value,
           DSTBLK                   := "MAL1S017Par".RecipeHeader.Loading);
      CALL "SIEMENS_BLKMOV" (
           SRCBLK                   := "MAL1S017".SecNom.GERM.RecipeHeader.Unloading,
           RET_VAL                  := "MAL1S017Par".Par.Error.Value,
           DSTBLK                   := "MAL1S017Par".RecipeHeader.Unloading);
      CALL "SIEMENS_BLKMOV" (
           SRCBLK                   := "MAL1S017".SecNom.GERM.RecipeHeader.Cleaning,
           RET_VAL                  := "MAL1S017Par".Par.Error.Value,
           DSTBLK                   := "MAL1S017Par".RecipeHeader.Cleaning);

No00: NOP   0; 
      U     "MAL1S017Par".Par.FlagProcess.ProcessActive; 
      SPBN  No03; 
// Germ: Copy common parameter
      CALL "SIEMENS_BLKMOV" (
           SRCBLK                   := "MAL1S017".SecNom.GERM.RecipeHeader,
           RET_VAL                  := "MAL1S017Par".Par.Error.Value,
           DSTBLK                   := "MAL1S017Par".RecipeHeader);
// 
// Copy step parameter
      U(    ; 
      L     "MAL1S017Par".Par.ParIntMachine.ActualStep; 
      L     1; 
      <I    ; 
      )     ; 
      O(    ; 
      L     "MAL1S017Par".Par.ParIntMachine.ActualStep; 
      L     "MAL1S017".SecNom.GERM.RecipeHeader.NumberOfSteps; 
      >I    ; 
      )     ; 
      SPB   NoXX; // error wrong step number
      CALL "SYS_Copy_Array_Element" (
           Source                   := "MAL1S017".SecNom.GERM.Step,
           Destination              := "MAL1S017Par".StepAct,
           Index                    := "MAL1S017Par".Par.ParIntMachine.ActualStep,
           Fault                    := "MAL1S017Par".Par.Error.Bit);

No03: NOP   0; 
NoXX: NOP   0; 
