      U     "MAL1S017Par".Par.FlagProcess.ProcessActive; 
      UN    "MAL1S017Par".Par.FlagProcess.ProcessEnd; 
      UN    "MAL1S017Par".Par.FlagFunction_1.StepLoading; 
      SPBN  M302; 
      L     2; // Process aktive
      SPA   M030; 
M302: NOP   0; 

      U     "MAL1S017".ToASW.InfDataReady; 
      U     "MAL1S017Par".Par.FlagFunction_1.StepUnloading; 
      UN    "MAL1S017Par".Par.FlagFunction_0.InfReleaseUnloading; 
      SPBN  M321; 
      L     21; // Wait for release unloading!
      SPA   M030; 
M321: NOP   0; 

      U     "MAL1S017".ToASW.InfDataReady; 
      U     "MAL1S017Par".Par.FlagFunction_1.StepUnloading; 
      SPBN  M303; 
      L     3; // Unloading
      SPA   M030; 
M303: NOP   0; 

      U     "MAL1S017".ToASW.InfDataReady; 
      U     "MAL1S017".PCRead.Step.StEmptied; 
//JVX        U     "G202M"._2305_M131_Ma.Ma.OutAdjusted    // travelling drive on target position
      U     "MAL1S017Par".Par.FlagFunction_1.StepCleaning; 
      UN    "MAL1S017Par".Par.FlagFunction_1.FinishCleaning; 
      SPBN  M315; 
      L     15; // Cleaning not finished!
      SPA   M030; 
M315: NOP   0; 

      U(    ; 
      U     "G109M".Gr.OutEnableEl; // Pump P1
      UN    "G109M".Gr.OutEnablePrevGr; // Pump P1
      )     ; 
      U     "MAL1S017Par".Par.FlagFunction_1.StepCleaning; 
      UN    "MAL1S017Par".Par.FlagFunction_1.FinishCleaning; 
      U     "MAL1S017".ToASW.InfDataReady; 
      SPBN  M318; 
      L     18; // Cleaning is not possible! Wait for water!
      SPA   M030; 
M318: NOP   0; 

      U     "MAL1S017".ToASW.InfDataReady; 
      U     "MAL1S017Par".Par.FlagFunction_1.StepCleaning; 
      UN    "MAL1S017Par".Par.FlagFunction_1.FinishCleaning; 
      SPBN  M304; 
      L     4; // Cleaning
      SPA   M030; 
M304: NOP   0; 

      U     "MAL1S017".ToASW.InfDataReady; 
      U     "MAL1S017Par".Par.FlagFunction_1.FinishUnloading; 
      UN    "MAL1S017Par".Par.FlagFunction_1.StepCleaning; 
      UN    "MAL1S017Par".Par.FlagFunction_1.FinishCleaning; 
      SPBN  M305; 
      L     5; // Finish unloading
      SPA   M030; 
M305: NOP   0; 

//    U     "MAL1S017".ToASW.InfDataReady
//    U     "MAL1S017Par".Par.FlagProcess.ProcessEnd
//    UN    "MAL1S017Par".Par.FlagFunction_1.StepUnloading
      U     "SYS_infLog0"; 
      SPBN  M306; 
      L     6; // Process End
      SPA   M030; 
M306: NOP   0; 

      U     "SYS_infLog0"; 
      SPBN  M307; 
      L     7; // Cooling Active
      SPA   M030; 
M307: NOP   0; 

      U     "MAL1S017".ToASW.InfSameBatchPrevSec; 
      O     "MAL1S017".ToASW.InfSameJobPrevSec; 
      U     "MAL1S017".ToASW.InfDataReady; 
      UN    "MAL1S017".PCRead.Step.StReady; 
      U     "MAL1S017Par".Par.FlagProcess.ProcessEnd; 
      UN    "MAL1S017Par".Par.FlagProcess.ProcessActive; 
      UN    "MAL1S017Par".Par.FlagFunction_1.StepLoading; 
      UN    "MAL1S017Par".Par.FlagFunction_1.StepUnloading; 
      SPBN  M313; 
      L     13; // Can not be unloaded! Same batch in previous section!
      SPA   M030; 
M313: NOP   0; 

      U     "MAL1S017".ToASW.InfDataReady; 
      U     "MAL1S017Par".Par.FlagProcess.ProcessEnd; 
      UN    "MAL1S017".ToASW.InfSameBatchPrevSec; 
      UN    "MAL1S017".ToASW.InfSameJobPrevSec; 
      UN    "MAL1S017Par".Par.FlagFunction_1.StepUnloading; 
      UN    "MAL1S017Par".Par.FlagFunction_1.FinishUnloading; 
      UN    "MAL1S017Par".Par.FlagFunction_1.StepCleaning; 
      UN    "MAL1S017Par".Par.FlagFunction_1.FinishCleaning; 
      SPBN  M308; 
      L     8; // Wait for Unloading
      SPA   M030; 
M308: NOP   0; 

      U     "SYS_infLog0"; 
      SPBN  M316; 
      L     16; // Process aktive and wait for water! Water not filled!
      SPA   M030; 
M316: NOP   0; 

M030: T     "MAL1S017Par".SectionStatus; 

