      U     "MAL1S017Par".Par.FlagGen.TurningStarted; 
      U(    ; 
      O     "MAL1S017Par".Par.FlagFunction_1.TurningRight; 
      O     "MAL1S017Par".Par.FlagFunction_1.TurningLeft; 
