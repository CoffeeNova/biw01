M311: NOP   0; 

      U     "MAL1S017".PCRead.Step.StActive; 
      O     "MAL1S017".PCRead.Step.StReady; 
      U     "MAL1S017Par".Par.FlagFunction_1.FinishLoading; 
      U     "MAL1S017Par".Par.FlagFunction_1.LoadFirstPartEnd; 
      U     "MAL1S017".ToASW.InfDataReady; 
      UN    "MAL1S017Par".Par.FlagProcess.ProcessActive; 
      SPBN  M312; 
      L     12; // Wait for start process
      SPA   M030; 
M312: NOP   0; 

      U     "MAL1S017".ToASW.InfDataReady; 
      U     "MAL1S017Par".Par.FlagFunction_1.StepLoading; 
      UN    "MAL1S017Par".Par.FlagFunction_1.FinishLoading; 
      SPBN  M301; 
      L     1; // Loading
      SPA   M030; 
M301: NOP   0; 

      U(    ; // JV040908
      O     "MAL1S017Par".Par.FlagFunction_1.TurningRight; 
      O     "MAL1S017Par".Par.FlagFunction_1.TurningLeft; 
      )     ; 
      U     "MAL1S017Par".Par.FlagProcess.ProcessActive; 
      U     "MAL1S017Par".Par.FlagGen.TurningStarted; 
      U     "G104M".Gr.OutEnablePrevGr; // turning screws
      U     "SYS_infLog0"; // Text im WinCC falsch!!
      SPBN  M314; 
      L     14; // Turning  //JV040908
      SPA   M030; 
M314: NOP   0; 

      U     "MAL1S017Par".Par.FlagProcess2.Turning1WithSpraying; 
      O     "MAL1S017Par".Par.FlagProcess2.Turning2WithSpraying; 
      U(    ; 
      O     "MAL1S017Par".Par.FlagFunction_1.TurningRight; 
      O     "MAL1S017Par".Par.FlagFunction_1.TurningLeft; 
      )     ; 
      U     "MAL1S017Par".Par.FlagProcess.ProcessActive; 
      U     "MAL1S017Par".Par.FlagGen.TurningStarted; 
      U     "G104M".Gr.OutEnablePrevGr; // turning screws
      U     "G108M".Gr.OutEnablePrevGr; // spraying valve
//      UN    "G083M".Gr.OutEnablePrevGr  // Pump PFW2 fresh water
      SPBN  M317; 
      L     17; // Spraying is not possible! Wait for water!
      SPA   M030; 
M317: NOP   0; 

//    U     "G202M".Gr.CmdOff           // Traveling Drive Germinationbox 1
//    ON    "G202M".Gr.InOutAutostartMan    // Traveling Drive Germinationbox 1
      U     "G202M".Gr.InManual; // Traveling Drive Germinationbox 1
      UN    "G202M".Gr.VarFault; // Traveling Drive Germinationbox 1
