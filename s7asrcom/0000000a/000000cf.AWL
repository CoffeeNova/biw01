//Conditions to activate cooling -- With flap regulation
      U(    ; 
      L     1; 
      L     "MAL1S017Par".StepAct.Cooling; // On/Off
      ==I   ; // Cooling on
      U     "MAL1S017Par".Par.FlagProcess.ProcessActive; 
      )     ; 
      U     "G105M".A_2160_MXZ01.StStartedFwd; 
      U     "G105M".A_2160_MXZ02.StStartedFwd; 
      U(    ; 
      L     "G269M".A_3191_BTE11.OutValueUnits; //WET BULB TEMPERATURE OUTSIDE
      L     20; // 2,0C Hysteresis
      -D    ; 
      L     "MAL1S017Par".StepAct.TemperaturBelowFloor; //SET POINT TEMPERATURE RECIPE
      >=D   ; 
      )     ; 
      U(    ; 
      L     "G103M".A_2191_BTE01.OutValueUnits; //AIR TEMPERATURE BELOW FLOOR GM1
      L     10; // 1,0C Hysteresis
      -D    ; 
      L     "MAL1S017Par".StepAct.TemperaturBelowFloor; //SET POINT TEMPERATURE RECIPE
      >=D   ; 
      )     ; 
      U(    ; 
      L     "G103M".A_2191_BTE02.OutValueUnits; //AIR TEMPERATURE ABOVE FLOOR GM1
      L     20; // 2,0C Hysteresis
      -D    ; 
      L     "MAL1S017Par".StepAct.TemperaturBelowFloor; //SET POINT TEMPERATURE RECIPE
      >=D   ; 
      )     ; 
      S     "MAL1S017Par".Par.FlagProcess.Cooling_Activ; 

      U(    ; 
      L     0; 
