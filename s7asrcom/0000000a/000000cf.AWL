// setpoint waste air                    = 100% - setpoint fresh air
      L     100; 
      L     "MAL1S017Par".StepAct.PosFreshAirFlapBasic; 
      -I    ; 
      T     "MAL1S017Par".StepAct.PosWasteAirFlapBasic; 

// setpoint limit maximal position fresh air
