TITLE =Test Fault

      U     #A_3291_BPS21.StFault; 
      O     #A_3261_KCL11.StFault; 
//      O     #A_3261_KCL11_AI.OutFaulted
//      O     #A_3261_KCL11_AO.StFault
      O     #A_3261_KCL11_EXT_ON.StFault; 
