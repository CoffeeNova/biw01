      =     #A_3261_KCL11_AO.InLocal; 

//-Interlocking---
      CLR   ; 
      =     #A_3261_KCL11_AO.InDisable; 

//-Values and parameters---
      L     "Regler Brenner 3".LMN_PER; 
      T     #A_3261_KCL11_AO.InOutValuePc; 
      T     "A_3261_KCL11_AO1"; 
// 
//-Read inputs---
      U     "PLC_SystemDB".GlobalFlags.Kilning_2; 
