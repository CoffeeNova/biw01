	      UN    "MAL1S017Par".Par.FlagProcess.ProcessActive; 
	      O     "MAL1S017Par".Par.FlagFunction_1.StepCleaning; 
	      O     "MAL1S017Par".Par.FlagFunction_1.StepLoading; 
	      SPBN  No00; 
	// Germ: Copy step loading,unloading,cleaning to parameterDB
	      CALL "SIEMENS_BLKMOV" (
		   SRCBLK                   := "MAL1S017".SecNom.Germ.RecipeHeader.Loading,
		   RET_VAL                  := "MAL1S017Par".Par.Error.Value,
		   DSTBLK                   := "MAL1S017Par".RecipeHeader.Loading);
	      CALL "SIEMENS_BLKMOV" (
		   SRCBLK                   := "MAL1S017".SecNom.Germ.RecipeHeader.Unloading,
