		   DSTBLK                   := "MAL1S017Par".RecipeHeader.Unloading);
	      CALL "SIEMENS_BLKMOV" (
		   SRCBLK                   := "MAL1S017".SecNom.Germ.RecipeHeader.Cleaning,
		   RET_VAL                  := "MAL1S017Par".Par.Error.Value,
		   DSTBLK                   := "MAL1S017Par".RecipeHeader.Cleaning);
	
	No00: NOP   0; 
	      U     "MAL1S017Par".Par.FlagProcess.ProcessActive; 
	      SPBN  No03; 
	// Germ: Copy common parameter
	      CALL "SIEMENS_BLKMOV" (
		   SRCBLK                   := "MAL1S017".SecNom.Germ.RecipeHeader,
		   RET_VAL                  := "MAL1S017Par".Par.Error.Value,
		   DSTBLK                   := "MAL1S017Par".RecipeHeader);
	// 
	// Copy step parameter
	      U(    ; 
	      L     "MAL1S017Par".Par.ParIntMachine.ActualStep; 
	      L     1; 
	      <I    ; 
	      )     ; 
	      O(    ; 
	      L     "MAL1S017Par".Par.ParIntMachine.ActualStep; 
	      L     "MAL1S017".SecNom.GERM.RecipeHeader.NumberOfSteps; 
	      >I    ; 
	      )     ; 
	      SPB   NoXX; // error wrong step number
	      CALL "SYS_Copy_Array_Element" (
		   Source                   := "MAL1S017".SecNom.Germ.Step,
		   Destination              := "MAL1S017Par".StepAct,
		   Index                    := "MAL1S017Par".Par.ParIntMachine.ActualStep,
		   Fault                    := "MAL1S017Par".Par.Error.Bit);
	
