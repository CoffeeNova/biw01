	      T     "MAL1S017Par".Processtime.Min; 
	      T     "MAL1S017Par".Processtime.ProcessTimeActive; 
	      L     L#0; // clear actual step time
	      T     "MAL1S017Par".Par.ProcessValue.StepTimeAct; // in ms
	
	NoCl: NOP   0; 
	// Clear content recipe in buffer
	      L     0; 
	      T     #tmpInt; 
	      CALL "SIEMENS_FILL" (
		   BVAL                     := #tmpInt,
		   RET_VAL                  := #tmpRet,
		   BLK                      := "MAL1S017Par".RecipeHeader);
