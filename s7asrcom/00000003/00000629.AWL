	//                  and sensor is ok
	
	// Total hours 0..32767
	      L     "MAL1S017Par".ActTimeStepProcess.SteptimeActMinOverall; // Steptime in min overall
	      ITD   ; 
	      DTR   ; 
	      L     6.000000e+001; // min/h
	      /R    ; 
	      TRUNC ; 
	      T     "MAL1S017Par".ActTimeStepProcess.StepTimeActHourTotal; // Steptime in hours overall
	// 
	// 1 Kriterium - steptime minimal 
	      U(    ; 
	      L     "MAL1S017Par".ActTimeStepProcess.StepTimeActHourTotal; // Steptime in hours overall
	      L     "MAL1S017Par".StepAct.StepTime_Min; // Target steptime actual step
	      >=I   ; 
	      )     ; 
	      U(    ; // and  one of 
	// 2 Kriterium - or delta temperature
	      O(    ; 
	      UN     G103M.A_2191_BTE02.OutOverflow; //AIR TEMPERATURE ABOVE FLOOR
	      UN     G103M.A_2191_BTE02.OutFaultAI; //AIR TEMPERATURE ABOVE FLOOR
	      UN     G103M.A_2191_BTE01.OutOverflow; //AIR TEMPERATURE BELOW FLOOR
	      UN     G103M.A_2191_BTE01.OutFaultAI; //AIR TEMPERATURE BELOW FLOOR
	      U     "MAL1S017Par".Par.FlagProcess1.DeltaTempNextStep; 
