	AUTHOR  : KR      
	FAMILY  : USW
	VERSION : 9.9
	
	
	VAR
	  Gr       : FB10;	//Standard interface to group
	  InOutValue1    : DINT; //Data for user
	  InOutValue2    : DINT; //Data for user
	  OutWayConflict : BOOL;	//Data for user
	  OutSelectionOk : BOOL;	//Data for user
	  CmdNewValue : BOOL;	//Data for user
	  CmdNewJob   : BOOL;	//Data for user
	  OutCodeOK   : BOOL;	//Data for user
	  InEndOfJob  : BOOL ;	//Data for user
	  staWayOk      : BOOL ;	//Data for user
	  EdgeStartRelease : BOOL ;	//Data for user
	  USW8  : BOOL;	//Data for user
	  USW9 : BOOL;	//Data for user
	  USW10: BOOL;	//Data for user
	  USW11: BOOL;	//Data for user
	  USW12: BOOL;	//Data for user
	  USW13: BOOL;	//Data for user
	  USW14: BOOL;	//Data for user
	  USW15: BOOL;	//Data for user
	     #A_2914_BFT07 : FB84;   // SubType FB14: DigInpCont 
	     #A_2914_SHE42 : FB14;   // DigInpCont 
	     #A_2914_MXZ01MA : FB1042;   // Motor+6DI 
	     #A_2914_QXV31 : FB13;   // VLS 
	END_VAR
	VAR_TEMP
	   tmpEnable		: BOOL;
	   tmpFlag  		: BOOL;
	   tmpCmdNext		: BOOL;
	   tmpLocal  		: BOOL;
	   tmpAspOk  		: BOOL;
	   tmpHWStop 		: BOOL;
	   tmpInstanzDBNo       : INT ;
	END_VAR
	BEGIN
	NETWORK
	TITLE =
	NETWORK
	TITLE =Read command from group control
	//Parameter
	      SET;
	      S   GCS7_GrCtrlDB.ParMGR[158];
	      R   GCS7_GrCtrlDB.ParWGR[158];
	      R   GCS7_GrCtrlDB.ParGGR[158];
	      R   GCS7_GrCtrlDB.ParFeed[158];
	      R   GCS7_GrCtrlDB.ParShare[158];
	      S   #Gr.Par.ParMgr;
	      R   #Gr.Par.ParWgr;
	      R   #Gr.Par.ParGgr;
	//Command
	      U   #GCS7_GrCtrlDB.OutEnable[158];
	      =   #Gr.InEnableGr;
	      U   #GCS7_GrCtrlDB.OutOccupied[158];
	      =   #Gr.InOccupied;
	      U   #GCS7_GrCtrlDB.CmdFaultReset[158];
	      R   #GCS7_GrCtrlDB.CmdFaultReset[158];
	      S   #Gr.CmdFaultReset;
	      U  #GCS7_GrCtrlDB.CmdNext[158];
	      R   #GCS7_GrCtrlDB.CmdNext[158];
	      S   #Gr.CmdNext;
	      U   #GCS7_GrCtrlDB.CmdManualOff[158];
	      O   #GCS7_GrCtrlDB.OutOccupied[158];
	      R   GCS7_GrCtrlDB.CmdManualOff[158];
	      S   #Gr.CmdManualOff;
	      U  GCS7_GrCtrlDB.CmdContinue[158];
	      R   GCS7_GrCtrlDB.CmdContinue[158];
	      S   #Gr.CmdContinue;
	//Init Line Number
	      U   GCS7_GrCtrlDB.ParShare[158];
	      SPBN WE00; 
	      L     0;
	      T     #Gr.InLine;
	WE00: NOP   0;
	NETWORK
	TITLE =Call Methode Control
	// 1. Handle Interface PCControl      
	// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
	// 3. Reset global flags of the group
	      CALL #Gr (
	           FcCode                   := 1);
	NETWORK
	TITLE =Restart Idling Time
		L  #Gr.Par.ParIdling;
		L  2;
		>=I;
		SPB IdTm;
		T  #Gr.Par.ParIdling;
	IdTm:   NOP 0;
	      CLR; 
	      S    #Gr.CmdRestartIdling; 
	NETWORK
	TITLE =Prepare global flags
	NETWORK
	TITLE =Set Pattern (Target position)
	NETWORK
	TITLE =Interlocking WayOk
	//
	NETWORK
	TITLE =Interlocking Autostart
		CLR;
		R   #Gr.OutAutostart;
	NETWORK
	TITLE =Interlocking Enable
	      U     #Gr.InEnableGr; 
	      =     #Gr.OutEnableEl; 
	      
	NETWORK
	TITLE =Interlocking Next
	      U     #Gr.CmdNext;
	      U     #Gr.OutAutostart;
	      UN    #Gr.OutNewWay;
	      =    #tmpCmdNext;
	      
	NETWORK
	TITLE =Interlocking Local / HWStop
	      U    DB050.DBX31.0;
	      =    #tmpHWStop;
	
	      U    #Gr.ModePassive;
	      O    #tmpHWStop;
	      =    #tmpLocal;
	 
	NETWORK
	TITLE ==A-2914-BFT07 : STOWING DETECTOR DISCHARGE
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_2914_BFT07.InLocal; 
	// 
	//-Monitoring---
	      CLR;
	      =      #A_2914_BFT07.InMonCovered; 
	      SET;
	      =      #A_2914_BFT07.InMonUncovered; 
	// 
	//-PreAlarm---
	      CLR;
	      =      #A_2914_BFT07.InPreAlarmCovered; 
	      CLR;
	      =      #A_2914_BFT07.InPreAlarmUncovered; 
	// 
	//-Additional commands---
	//      CLR
	//      S     #A_2914_BFT07.CmdRunTimeOutTime
	// 
	//-Read inputs---
	      UN    E 1124.4; 
	      =     #A_2914_BFT07.InpCovered; 
	      U   DB050.DBX31.0;
	      =     #A_2914_BFT07.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[113];
	       =       #A_2914_BFT07.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2914_BFT07;
	// 
	//-Write outputs---
	NETWORK
	TITLE ==A-2914-SHE42 : CABLE PULL SWITCH RIGHT 2
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_2914_SHE42.InLocal; 
	// 
	//-Monitoring---
	      SET;
	      =      #A_2914_SHE42.InMonTrue; 
	      CLR;
	      =      #A_2914_SHE42.InMonFalse; 
	// 
	//-PreAlarm---
	      CLR;
	      =      #A_2914_SHE42.InPreAlarmTrue; 
	      CLR;
	      =      #A_2914_SHE42.InPreAlarmFalse; 
	// 
	//-Additional commands---
	//      CLR
	//      S     #A_2914_SHE42.CmdRunTimeOutTime
	// 
	//-Read inputs---
	      U     E 1122.5; 
	      =     #A_2914_SHE42.InpTrue; 
	      U   DB050.DBX31.0;
	      =     #A_2914_SHE42.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[112];
	       =       #A_2914_SHE42.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2914_SHE42;
	// 
	//-Write outputs---
	NETWORK
	TITLE ==A-2914-MXZ01MA : BELT CONVEYOR 4600 GERM 3
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_2914_MXZ01MA.Ma.InLocal; 
	//
	//-Interlocking---
	      UN    #A_2914_BFT07.StFault;
	      UN    #A_2914_SHE42.StFault;
	      UN    #A_2914_SHE42.StFalse;
	      UN    #A_2914_SHE42.StCToTrue;
	      U     #Gr.OutEnableEl; 
	      =     #A_2914_MXZ01MA.Ma.InEnableFwd; 
	//
	      U    #tmpCmdNext;
	      S     #A_2914_MXZ01MA.Ma.CmdNext; 
	//
	      U     #Gr.OutSeqStop; 
	      =     #A_2914_MXZ01MA.Ma.InSeqStop; 
	// 
	//=Values and commands of elements===
	//-=A-2914-SHE41---
	//      CLR
	//      S     #A_2914_MXZ01MA.A_2914_SHE41.CmdRunTimeOutTime
	//-=A-2914-BSA13---
	//      CLR
	//      S     #A_2914_MXZ01MA.A_2914_BSA13.CmdRunTimeOutTime
	//-=A-2914-BZS14/15---
	//      CLR
	//      S     #A_2914_MXZ01MA.A_2914_BZS14_15.CmdRunTimeOutTime
	//-=A-2914-BZS16/17---
	//      CLR
	//      S     #A_2914_MXZ01MA.A_2914_BZS16_17.CmdRunTimeOutTime
	//-=A-2914-SHE43---
	//      CLR
	//      S     #A_2914_MXZ01MA.A_2914_SHE43.CmdRunTimeOutTime
	//-=A-2914-SHE44---
	//      CLR
	//      S     #A_2914_MXZ01MA.A_2914_SHE44.CmdRunTimeOutTime
	// 
	//-Read inputs of elements---
	      U     E 1122.4; 
	      =     #A_2914_MXZ01MA.A_2914_SHE41.InpTrue; 
	      U   DB050.DBX31.0;
	      =     #A_2914_MXZ01MA.A_2914_SHE41.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[112];
	       =       #A_2914_MXZ01MA.A_2914_SHE41.InDPFault;
	      U     E 1124.5; 
	      =     #A_2914_MXZ01MA.A_2914_BSA13.InpTrue; 
	       U     PLC_DPInfo.Master[1].SlavesError[113];
	       =       #A_2914_MXZ01MA.A_2914_BSA13.InDPFault;
	      U     E 1124.6; 
	      =     #A_2914_MXZ01MA.A_2914_BZS14_15.InpTrue; 
	       U     PLC_DPInfo.Master[1].SlavesError[113];
	       =       #A_2914_MXZ01MA.A_2914_BZS14_15.InDPFault;
	      U     E 1124.7; 
	      =     #A_2914_MXZ01MA.A_2914_BZS16_17.InpTrue; 
	       U     PLC_DPInfo.Master[1].SlavesError[113];
	       =       #A_2914_MXZ01MA.A_2914_BZS16_17.InDPFault;
	      U     E 1122.6; 
	      =     #A_2914_MXZ01MA.A_2914_SHE43.InpTrue; 
	      U   DB050.DBX31.0;
	      =     #A_2914_MXZ01MA.A_2914_SHE43.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[112];
	       =       #A_2914_MXZ01MA.A_2914_SHE43.InDPFault;
	      U     E 1122.7; 
	      =     #A_2914_MXZ01MA.A_2914_SHE44.InpTrue; 
	      U   DB050.DBX31.0;
	      =     #A_2914_MXZ01MA.A_2914_SHE44.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[112];
	       =       #A_2914_MXZ01MA.A_2914_SHE44.InDPFault;
	      U     E 1118.1; 
	      =     #A_2914_MXZ01MA.A_2914_MXZ01.InpRunFwd; 
	      U   DB050.DBX31.2;
	      =     #A_2914_MXZ01MA.A_2914_MXZ01.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[112];
	       =       #A_2914_MXZ01MA.A_2914_MXZ01.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2914_MXZ01MA;
	// 
	//-Write outputs---
	      U     #A_2914_MXZ01MA.A_2914_MXZ01.OutpRunFwd; 
	      =     A 1118.1; 
	NETWORK
	TITLE ==A-2914-QXV31 : BALL VALVE SPRAY WATER OPEN
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_2914_QXV31.InLocal; 
	//
	//-Interlocking---
	      U A_2914_MXZ01MA.Ma.StStartedFwd;
	         =     #A_2914_QXV31.InEnableHN; 
	         NOT;
	         =     #A_2914_QXV31.InEnableLN; 
	//
	         SET;
	         S     #A_2914_QXV31.CmdNext; 
	//
	      U     #Gr.OutSeqStop; 
	      =     #A_2914_QXV31.InSeqStop; 
	// 
	//-Read inputs---
	      UN   #A_2914_QXV31.OutpHN; 
	      =     #A_2914_QXV31.InpLN; 
	      U   #A_2914_QXV31.OutpHN; 
	      =     #A_2914_QXV31.InpHN;
	      U   DB050.DBX31.0;
	      =     #A_2914_QXV31.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[113];
	       =       #A_2914_QXV31.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2914_QXV31;
	// 
	//-Write outputs---
	      U     #A_2914_QXV31.OutpHN; 
	      =     A 1124.1; 
	NETWORK
	TITLE =Test Fault
	      U     #A_2914_BFT07.StFault;
	      O     #A_2914_SHE42.StFault;
	      O     #A_2914_MXZ01MA.Ma.StFault;
	      O     #A_2914_QXV31.StFault;
	      =     #Gr.VarFault; 
	NETWORK
	TITLE =Test Warning
	      CLR;   
	      =     #Gr.OutWarning;
	NETWORK
	TITLE =Test Stopped
	      U     #A_2914_MXZ01MA.Ma.StStopped; 
	      UN   #Gr.VarFault; 
	      =     #Gr.VarStopped; 
	NETWORK
	TITLE =Test Started
	      U     #A_2914_MXZ01MA.Ma.StStartedFwd; 
	      U     #A_2914_QXV31.StHN; 
	      UN  #Gr.VarStopped; 
	      UN  #Gr.VarFault; 
	      =     #Gr.VarStarted; 
	NETWORK
	TITLE =Enable Previous Group
	      U    #Gr.VarStarted;
	      =     #Gr.OutEnablePrevGr;
	NETWORK
	TITLE =Call Methode State
	//1. Make State of the group: This function use the signals:
	//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
	//2.Reset Commands to group: e.g.CmdFaultReset
	      CALL #Gr (
	           FcCode                   := 2);
	NETWORK
	TITLE =Init
	      SET; 
	      R    #CmdNewValue;
	      R    #CmdNewJob;
	NETWORK
	TITLE =Write data to group control
	      U   #Gr.StStopped;
	      =   GCS7_GrCtrlDB.InStopped[158];
	      U   #Gr.StStarted;
	      =   GCS7_GrCtrlDB.InStarted[158];
	      U   #Gr.StFault;
	      =   GCS7_GrCtrlDB.InFault[158];
	      U   #Gr.OutWarning;
	      =   GCS7_GrCtrlDB.InWarning[158];
	      U   #Gr.OutManual;
	      =   GCS7_GrCtrlDB.InManual[158];
	      U   #Gr.OutHWStop;
	      =   GCS7_GrCtrlDB.InHWStop[158];
	      U   #Gr.OutDPFault;
	      =   GCS7_GrCtrlDB.InDPFault[158];
	      U   #Gr.OutLoaded;
	      =   GCS7_GrCtrlDB.InLoaded[158];
	      U   #Gr.OutAutoStart;
	      =   GCS7_GrCtrlDB.InAutoStart[158];
	      U   #Gr.VarStartWarning;
	      =   GCS7_GrCtrlDB.InStartWarning[158];
	NETWORK
	TITLE =End of block
	
	BE
	
	END_FUNCTION_BLOCK
