	FUNCTION_BLOCK FB 144
	TITLE =G044M CO2 FAN AND FESH AIR FLAP STEEPING 7
	//COPYRIGHT:    (C) 2009  BUHLER AG, CH-9240 UZWIL, SWITZERLAND
	//
	//PRODUCT:      WinCoS_BIW01_09
	//
	//TITLE:        CO2 FAN AND FESH AIR FLAP STEEPING 7
	//                Machine group control
	//                Maschinengruppensteuerung
	// 
	//--------------------------------------------------------------------------------
	// 
	//IDENT:        AUTHOR:             DEPT:                              DATE:
	// 
	//P01_FB144.1.0 K.Renz              CSB41                              13.Jan.2010
	//             1st Release
	// 
	//--------------------------------------------------------------------------------
	// 
	//PURPOSE: 
	// 
	//MANUAL : 
	// 
	//RESTRICTIONS: 
	// 
	//--------------------------------------------------------------------------------
	AUTHOR  : KR      
	FAMILY  : USW
	VERSION : 9.9
	
	
	VAR
	  Gr       : FB10;	//Standard interface to group
	  InOutValue1    : DINT; //Data for user
	  InOutValue2    : DINT; //Data for user
	  OutWayConflict : BOOL;	//Data for user
	  OutSelectionOk : BOOL;	//Data for user
	  CmdNewValue : BOOL;	//Data for user
	  CmdNewJob   : BOOL;	//Data for user
	  OutCodeOK   : BOOL;	//Data for user
	  InEndOfJob  : BOOL ;	//Data for user
	  staWayOk      : BOOL ;	//Data for user
	  EdgeStartRelease : BOOL ;	//Data for user
	  USW8  : BOOL;	//Data for user
	  USW9 : BOOL;	//Data for user
	  USW10: BOOL;	//Data for user
	  USW11: BOOL;	//Data for user
	  USW12: BOOL;	//Data for user
	  USW13: BOOL;	//Data for user
	  USW14: BOOL;	//Data for user
	  USW15: BOOL;	//Data for user
	     #A_1117_MXZ11 : FB12;   // Motor 
	     #A_1167_MXZ_BZS01_02 : FB13;   // VLS 
	END_VAR
	VAR_TEMP
	   tmpEnable		: BOOL;
	   tmpFlag  		: BOOL;
	   tmpCmdNext		: BOOL;
	   tmpLocal  		: BOOL;
	   tmpAspOk  		: BOOL;
	   tmpHWStop 		: BOOL;
	   tmpInstanzDBNo       : INT ;
	END_VAR
	BEGIN
	NETWORK
	TITLE =
	NETWORK
	TITLE =Read command from group control
	//Parameter
	      SET;
	      S   GCS7_GrCtrlDB.ParMGR[44];
	      R   GCS7_GrCtrlDB.ParWGR[44];
	      R   GCS7_GrCtrlDB.ParGGR[44];
	      R   GCS7_GrCtrlDB.ParFeed[44];
	      R   GCS7_GrCtrlDB.ParShare[44];
	      S   #Gr.Par.ParMgr;
	      R   #Gr.Par.ParWgr;
	      R   #Gr.Par.ParGgr;
	//Command
	      U   #GCS7_GrCtrlDB.OutEnable[44];
	      =   #Gr.InEnableGr;
	      U   #GCS7_GrCtrlDB.OutOccupied[44];
	      =   #Gr.InOccupied;
	      U   #GCS7_GrCtrlDB.CmdFaultReset[44];
	      R   #GCS7_GrCtrlDB.CmdFaultReset[44];
	      S   #Gr.CmdFaultReset;
	      U  #GCS7_GrCtrlDB.CmdNext[44];
	      R   #GCS7_GrCtrlDB.CmdNext[44];
	      S   #Gr.CmdNext;
	      U   #GCS7_GrCtrlDB.CmdManualOff[44];
	      O   #GCS7_GrCtrlDB.OutOccupied[44];
	      R   GCS7_GrCtrlDB.CmdManualOff[44];
	      S   #Gr.CmdManualOff;
	      U  GCS7_GrCtrlDB.CmdContinue[44];
	      R   GCS7_GrCtrlDB.CmdContinue[44];
	      S   #Gr.CmdContinue;
	//Init Line Number
	      U   GCS7_GrCtrlDB.ParShare[44];
	      SPBN WE00; 
	      L     0;
	      T     #Gr.InLine;
	WE00: NOP   0;
	NETWORK
	TITLE =Call Methode Control
	// 1. Handle Interface PCControl      
	// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
	// 3. Reset global flags of the group
	      CALL #Gr (
	           FcCode                   := 1);
	NETWORK
	TITLE =Restart Idling Time
		L  #Gr.Par.ParIdling;
		L  2;
		>=I;
		SPB IdTm;
		T  #Gr.Par.ParIdling;
	IdTm:   NOP 0;
	      CLR; 
	      S    #Gr.CmdRestartIdling; 
	NETWORK
	TITLE =Prepare global flags
	NETWORK
	TITLE =Set Pattern (Target position)
	NETWORK
	TITLE =Interlocking WayOk
	//
	NETWORK
	TITLE =Interlocking Autostart
		CLR;
		R   #Gr.OutAutostart;
	NETWORK
	TITLE =Interlocking Enable
	      U     #Gr.InEnableGr; 
	      =     #Gr.OutEnableEl; 
	      
	NETWORK
	TITLE =Interlocking Next
	      U     #Gr.CmdNext;
	      U     #Gr.OutAutostart;
	      UN    #Gr.OutNewWay;
	      =    #tmpCmdNext;
	      
	NETWORK
	TITLE =Interlocking Local / HWStop
	      U    DB050.DBX31.2;
	      =    #tmpHWStop;
	
	      U    #Gr.ModePassive;
	      O    #tmpHWStop;
	      =    #tmpLocal;
	 
	NETWORK
	TITLE =Interlocking Start Warning
	      NOP 0;
	//- Restart after fault --
	      U     #Gr.CmdFaultReset;
	      U     #Gr.VarFault;
	      S       "SYS_StartWarningDB".Area[1].CmdResetStartWarning;
	//- Run Timer --
	      AUF "G044M";
	      CALL  FB100 , DB100(
	       InModeHigher :=FALSE,
	       InArea     :=1,
	       InEnable   :=#Gr.OutEnableEl,
	       InLocal    :=#tmpLocal,
	       InStarted  :=#Gr.VarStarted,
	       InCmdNext  :=#tmpCmdNext,
	       OutCmdNext :=#tmpCmdNext,
	       OutStartwarning:=#Gr.VarStartWarning);
	//- Store Startwarning done  --
	//- Reset Autostart --
	      U     "SYS_StartWarningDB".TmpArea.OutStartReleaseRunningL;
	      FN    #EdgeStartRelease;
	      U     #Gr.VarFault;
	      R     #Gr.OutAutostart;
	NETWORK
	TITLE ==A-1117-MXZ11 : CO2 FAN ST 7
	NOP 0;
	//-InLocal--- 
	      U     #tmpLocal; 
	      =     #A_1117_MXZ11.InLocal; 
	//
	//-Interlocking--- 
	      U     #Gr.OutEnableEl; 
	      =     #A_1117_MXZ11.InEnableFwd; 
	// 
	      U    #tmpCmdNext;
	      S     #A_1117_MXZ11.CmdNext; 
	// 
	      U     #Gr.OutSeqStop; 
	      =     #A_1117_MXZ11.InSeqStop; 
	// 
	//-Read inputs---
	      U     E 024.6; 
	      =     #A_1117_MXZ11.InpRunFwd; 
	      U   DB050.DBX31.2;
	      =     #A_1117_MXZ11.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[5];
	       =       #A_1117_MXZ11.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_1117_MXZ11;
	// 
	//-Write outputs---
	      U     #A_1117_MXZ11.OutpRunFwd; 
	      =     A 024.6; 
	NETWORK
	TITLE ==A-1167-MXZ/BZS01/02 : CO2 VALVE ST 7
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_1167_MXZ_BZS01_02.InLocal; 
	//
	//-Interlocking---
	      U     #A_1117_MXZ11.StStartedFwd;
	         =     #A_1167_MXZ_BZS01_02.InEnableHN; 
	         NOT;
	         =     #A_1167_MXZ_BZS01_02.InEnableLN; 
	//
	         SET;
	         S     #A_1167_MXZ_BZS01_02.CmdNext; 
	//
	      U     #Gr.OutSeqStop; 
	      =     #A_1167_MXZ_BZS01_02.InSeqStop; 
	// 
	//-Read inputs---
	      U     E 051.2; 
	      =     #A_1167_MXZ_BZS01_02.InpLN;
	      U     E 051.3; 
	      =     #A_1167_MXZ_BZS01_02.InpHN; 
	      X     A 049.5; 
	      X     E 051.1; 
	      O;
	      X     A 049.4; 
	      X     E 051.0; 
	      =     #A_1167_MXZ_BZS01_02.InpFaultDev; 
	      U   DB050.DBX31.2;
	      =     #A_1167_MXZ_BZS01_02.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[8];
	       O     PLC_DPInfo.Master[1].SlavesError[9];
	       =       #A_1167_MXZ_BZS01_02.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_1167_MXZ_BZS01_02;
	// 
	//-Write outputs---
	      U     #A_1167_MXZ_BZS01_02.OutpLN; 
	      =     A 049.5; 
	      U     #A_1167_MXZ_BZS01_02.OutpHN; 
	      =     A 049.4; 
	NETWORK
	TITLE =Test Fault
	      U     #A_1117_MXZ11.StFault;
	      O     #A_1167_MXZ_BZS01_02.StFault;
	      =     #Gr.VarFault; 
	NETWORK
	TITLE =Test Warning
	      CLR;   
	      =     #Gr.OutWarning;
	NETWORK
	TITLE =Test Stopped
	      U     #A_1117_MXZ11.StStopped; 
	      UN   #Gr.VarFault; 
	      =     #Gr.VarStopped; 
	NETWORK
	TITLE =Test Started
	      U     #A_1117_MXZ11.StStartedFwd; 
	      U     #A_1167_MXZ_BZS01_02.StHN; 
	      UN  #Gr.VarStopped; 
	      UN  #Gr.VarFault; 
	      =     #Gr.VarStarted; 
	NETWORK
	TITLE =Enable Previous Group
	      U    #Gr.VarStarted;
	      =     #Gr.OutEnablePrevGr;
	NETWORK
	TITLE =Call Methode State
	//1. Make State of the group: This function use the signals:
	//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
	//2.Reset Commands to group: e.g.CmdFaultReset
	      CALL #Gr (
	           FcCode                   := 2);
	NETWORK
	TITLE =Init
	      SET; 
	      R    #CmdNewValue;
	      R    #CmdNewJob;
	NETWORK
	TITLE =Write data to group control
	      U   #Gr.StStopped;
	      =   GCS7_GrCtrlDB.InStopped[44];
	      U   #Gr.StStarted;
	      =   GCS7_GrCtrlDB.InStarted[44];
	      U   #Gr.StFault;
	      =   GCS7_GrCtrlDB.InFault[44];
	      U   #Gr.OutWarning;
	      =   GCS7_GrCtrlDB.InWarning[44];
	      U   #Gr.OutManual;
	      =   GCS7_GrCtrlDB.InManual[44];
	      U   #Gr.OutHWStop;
	      =   GCS7_GrCtrlDB.InHWStop[44];
	      U   #Gr.OutDPFault;
	      =   GCS7_GrCtrlDB.InDPFault[44];
	      U   #Gr.OutLoaded;
	      =   GCS7_GrCtrlDB.InLoaded[44];
	      U   #Gr.OutAutoStart;
	      =   GCS7_GrCtrlDB.InAutoStart[44];
	      U   #Gr.VarStartWarning;
	      =   GCS7_GrCtrlDB.InStartWarning[44];
	NETWORK
	TITLE =End of block
	
	BE
	
	END_FUNCTION_BLOCK
