	      =     #A_2715_BZS46.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[75];
	       =       #A_2715_BZS46.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2715_BZS46;
	// 
	//-Write outputs---
	NETWORK
	TITLE ==A-2715-MXZ41 : WALL COUPLING DRIVE
	NOP 0;
	//-InLocal--- 
	      U     #tmpLocal; 
	      =     #A_2715_MXZ41.InLocal; 
	//
	//-Interlocking--- 
	      U     #Gr.OutEnableEl; 
	      =     #A_2715_MXZ41.InEnableFwd; 
	      U     #Gr.OutEnableEl; 
	      =     #A_2715_MXZ41.InEnableRev; 
	// 
	      U    #tmpCmdNext;
	      S     #A_2715_MXZ41.CmdNext; 
	// 
	      U     #Gr.OutSeqStop; 
	      =     #A_2715_MXZ41.InSeqStop; 
	// 
	//-Read inputs---
	      U     #A_2715_MXZ41.OutpRunRev; 
	      =     #A_2715_MXZ41.InpRunRev; 
	      U     #A_2715_MXZ41.OutpRunFwd; 
	      =     #A_2715_MXZ41.InpRunFwd; 
	      U     #A_2715_MXZ41.OutpRunFwd;
	      =     #A_2715_MXZ41.InpRunFwd; 
	      U     #A_2715_MXZ41.OutpRunRev; 
	      =     #A_2715_MXZ41.InpRunRev; 
	      U   DB050.DBX32.1;
	      =     #A_2715_MXZ41.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[75];
	       =       #A_2715_MXZ41.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2715_MXZ41;
	// 
	//-Write outputs---
	      U     #A_2715_MXZ41.OutpRunFwd; 
	      =     A 761.0; 
	      U     #A_2715_MXZ41.OutpRunRev; 
	      =     A 761.1; 
	NETWORK
	TITLE ==A-2715-MXZ45 : WALL COUPLING DRIVE
	NOP 0;
	//-InLocal--- 
	      U     #tmpLocal; 
	      =     #A_2715_MXZ45.InLocal; 
	//
	//-Interlocking--- 
	      U     #A_2715_MXZ41.StStartedFwd;
	      U     #Gr.OutEnableEl; 
	      =     #A_2715_MXZ45.InEnableFwd; 
	      U     #A_2715_MXZ41.StStartedFwd;
	      U     #Gr.OutEnableEl; 
	      =     #A_2715_MXZ45.InEnableRev; 
	// 
	      U    #tmpCmdNext;
	      S     #A_2715_MXZ45.CmdNext; 
	// 
	      U     #Gr.OutSeqStop; 
	      =     #A_2715_MXZ45.InSeqStop; 
	// 
	//-Read inputs---
	      U     #A_2715_MXZ45.OutpRunRev; 
	      =     #A_2715_MXZ45.InpRunRev; 
	      U     #A_2715_MXZ45.OutpRunFwd; 
	      =     #A_2715_MXZ45.InpRunFwd; 
	      U     #A_2715_MXZ45.OutpRunFwd;
	      =     #A_2715_MXZ45.InpRunFwd; 
	      U     #A_2715_MXZ45.OutpRunRev; 
	      =     #A_2715_MXZ45.InpRunRev; 
	      U   DB050.DBX32.1;
	      =     #A_2715_MXZ45.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[75];
	       =       #A_2715_MXZ45.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2715_MXZ45;
	// 
	//-Write outputs---
	      U     #A_2715_MXZ45.OutpRunFwd; 
	      =     A 761.2; 
	      U     #A_2715_MXZ45.OutpRunRev; 
	      =     A 761.3; 
	NETWORK
	TITLE =Test Fault
	      U     #A_2715_BZS42.StFault;
	      O     #A_2715_BZS43.StFault;
	      O     #A_2715_BZS47.StFault;
	      O     #A_2715_BZS46.StFault;
	      O     #A_2715_MXZ41.StFault;
	      O     #A_2715_MXZ45.StFault;
	      =     #Gr.VarFault; 
	NETWORK
	TITLE =Test Warning
	      CLR;   
	      =     #Gr.OutWarning;
	NETWORK
	TITLE =Test Stopped
	      U     #A_2715_MXZ41.StStopped; 
	      U     #A_2715_MXZ45.StStopped; 
	      UN   #Gr.VarFault; 
	      =     #Gr.VarStopped; 
	NETWORK
	TITLE =Test Started
	      U     #A_2715_MXZ41.StStartedFwd; 
	      U     #A_2715_MXZ45.StStartedFwd; 
	      UN  #Gr.VarStopped; 
	      UN  #Gr.VarFault; 
	      =     #Gr.VarStarted; 
	NETWORK
	TITLE =Enable Previous Group
	      U    #Gr.VarStarted;
	      =     #Gr.OutEnablePrevGr;
	NETWORK
	TITLE =Call Methode State
	//1. Make State of the group: This function use the signals:
	//        Gr.VarStopped, Gr.VarStarted, Gr.VarFault
	//2.Reset Commands to group: e.g.CmdFaultReset
	      CALL #Gr (
	           FcCode                   := 2);
	NETWORK
	TITLE =Init
	      SET; 
	      R    #CmdNewValue;
	      R    #CmdNewJob;
	NETWORK
	TITLE =Write data to group control
	      U   #Gr.StStopped;
	      =   GCS7_GrCtrlDB.InStopped[224];
	      U   #Gr.StStarted;
	      =   GCS7_GrCtrlDB.InStarted[224];
	      U   #Gr.StFault;
	      =   GCS7_GrCtrlDB.InFault[224];
	      U   #Gr.OutWarning;
	      =   GCS7_GrCtrlDB.InWarning[224];
	      U   #Gr.OutManual;
	      =   GCS7_GrCtrlDB.InManual[224];
	      U   #Gr.OutHWStop;
	      =   GCS7_GrCtrlDB.InHWStop[224];
	      U   #Gr.OutDPFault;
	      =   GCS7_GrCtrlDB.InDPFault[224];
	      U   #Gr.OutLoaded;
	      =   GCS7_GrCtrlDB.InLoaded[224];
	      U   #Gr.OutAutoStart;
	      =   GCS7_GrCtrlDB.InAutoStart[224];
	      U   #Gr.VarStartWarning;
	      =   GCS7_GrCtrlDB.InStartWarning[224];
	NETWORK
	TITLE =End of block
	
	BE
	
	END_FUNCTION_BLOCK
