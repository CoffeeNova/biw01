	     #A_2715_BZS46 : FB14;   // DigInpCont 
	     #A_2715_MXZ41 : FB12;   // Motor 
	     #A_2715_MXZ45 : FB12;   // Motor 
	END_VAR
	VAR_TEMP
	   tmpEnable		: BOOL;
	   tmpFlag  		: BOOL;
	   tmpCmdNext		: BOOL;
	   tmpLocal  		: BOOL;
	   tmpAspOk  		: BOOL;
	   tmpHWStop 		: BOOL;
	   tmpInstanzDBNo       : INT ;
	END_VAR
	BEGIN
	NETWORK
	TITLE =
	NETWORK
	TITLE =Read command from group control
	//Parameter
	      SET;
	      S   GCS7_GrCtrlDB.ParMGR[224];
	      R   GCS7_GrCtrlDB.ParWGR[224];
	      R   GCS7_GrCtrlDB.ParGGR[224];
	      R   GCS7_GrCtrlDB.ParFeed[224];
	      R   GCS7_GrCtrlDB.ParShare[224];
	      S   #Gr.Par.ParMgr;
	      R   #Gr.Par.ParWgr;
	      R   #Gr.Par.ParGgr;
	//Command
	      U   #GCS7_GrCtrlDB.OutEnable[224];
	      =   #Gr.InEnableGr;
	      U   #GCS7_GrCtrlDB.OutOccupied[224];
	      =   #Gr.InOccupied;
	      U   #GCS7_GrCtrlDB.CmdFaultReset[224];
	      R   #GCS7_GrCtrlDB.CmdFaultReset[224];
	      S   #Gr.CmdFaultReset;
	      U  #GCS7_GrCtrlDB.CmdNext[224];
	      R   #GCS7_GrCtrlDB.CmdNext[224];
	      S   #Gr.CmdNext;
	      U   #GCS7_GrCtrlDB.CmdManualOff[224];
	      O   #GCS7_GrCtrlDB.OutOccupied[224];
	      R   GCS7_GrCtrlDB.CmdManualOff[224];
	      S   #Gr.CmdManualOff;
	      U  GCS7_GrCtrlDB.CmdContinue[224];
	      R   GCS7_GrCtrlDB.CmdContinue[224];
	      S   #Gr.CmdContinue;
	//Init Line Number
	      U   GCS7_GrCtrlDB.ParShare[224];
	      SPBN WE00; 
	      L     0;
	      T     #Gr.InLine;
	WE00: NOP   0;
	NETWORK
	TITLE =Call Methode Control
	// 1. Handle Interface PCControl      
	// 2. Prepare mode --> ModePassive,ModeActive,ModeIdling      
	// 3. Reset global flags of the group
	      CALL #Gr (
	           FcCode                   := 1);
	NETWORK
	TITLE =Restart Idling Time
		L  #Gr.Par.ParIdling;
		L  2;
		>=I;
		SPB IdTm;
		T  #Gr.Par.ParIdling;
	IdTm:   NOP 0;
	      CLR; 
	      S    #Gr.CmdRestartIdling; 
	NETWORK
	TITLE =Prepare global flags
	NETWORK
	TITLE =Set Pattern (Target position)
	NETWORK
	TITLE =Interlocking WayOk
	//
	NETWORK
	TITLE =Interlocking Autostart
		CLR;
		R   #Gr.OutAutostart;
	NETWORK
	TITLE =Interlocking Enable
	      U     #Gr.InEnableGr; 
	      =     #Gr.OutEnableEl; 
	      
	NETWORK
	TITLE =Interlocking Next
	      U     #Gr.CmdNext;
	      U     #Gr.OutAutostart;
	      UN    #Gr.OutNewWay;
	      =    #tmpCmdNext;
	      
	NETWORK
	TITLE =Interlocking Local / HWStop
	      U    DB050.DBX32.1;
	      =    #tmpHWStop;
	
	      U    #Gr.ModePassive;
	      O    #tmpHWStop;
	      =    #tmpLocal;
	 
	NETWORK
	TITLE ==A-2715-BZS42 : COUPLING DRIVE UNLOCKED
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_2715_BZS42.InLocal; 
	// 
	//-Monitoring---
	      CLR;
	      =      #A_2715_BZS42.InMonTrue; 
	      CLR;
	      =      #A_2715_BZS42.InMonFalse; 
	// 
	//-PreAlarm---
	      CLR;
	      =      #A_2715_BZS42.InPreAlarmTrue; 
	      CLR;
	      =      #A_2715_BZS42.InPreAlarmFalse; 
	// 
	//-Additional commands---
	//      CLR
	//      S     #A_2715_BZS42.CmdRunTimeOutTime
	// 
	//-Read inputs---
	      U     E 763.0; 
	      =     #A_2715_BZS42.InpTrue; 
	      U   DB050.DBX32.1;
	      =     #A_2715_BZS42.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[75];
	       =       #A_2715_BZS42.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2715_BZS42;
	// 
	//-Write outputs---
	NETWORK
	TITLE ==A-2715-BZS43 : COUPLING DRIVE LOCKED
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_2715_BZS43.InLocal; 
	// 
	//-Monitoring---
	      CLR;
	      =      #A_2715_BZS43.InMonTrue; 
	      CLR;
	      =      #A_2715_BZS43.InMonFalse; 
	// 
	//-PreAlarm---
	      CLR;
	      =      #A_2715_BZS43.InPreAlarmTrue; 
	      CLR;
	      =      #A_2715_BZS43.InPreAlarmFalse; 
	// 
	//-Additional commands---
	//      CLR
	//      S     #A_2715_BZS43.CmdRunTimeOutTime
	// 
	//-Read inputs---
	      U     E 763.1; 
	      =     #A_2715_BZS43.InpTrue; 
	      U   DB050.DBX32.1;
	      =     #A_2715_BZS43.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[75];
	       =       #A_2715_BZS43.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2715_BZS43;
	// 
	//-Write outputs---
	NETWORK
	TITLE ==A-2715-BZS47 : COUPLING DRIVE LOCKED
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_2715_BZS47.InLocal; 
	// 
	//-Monitoring---
	      CLR;
	      =      #A_2715_BZS47.InMonTrue; 
	      CLR;
	      =      #A_2715_BZS47.InMonFalse; 
	// 
	//-PreAlarm---
	      CLR;
	      =      #A_2715_BZS47.InPreAlarmTrue; 
	      CLR;
	      =      #A_2715_BZS47.InPreAlarmFalse; 
	// 
	//-Additional commands---
	//      CLR
	//      S     #A_2715_BZS47.CmdRunTimeOutTime
	// 
	//-Read inputs---
	      U     E 763.3; 
	      =     #A_2715_BZS47.InpTrue; 
	      U   DB050.DBX32.1;
	      =     #A_2715_BZS47.InHWStop;
	       U     PLC_DPInfo.Master[1].SlavesError[75];
	       =       #A_2715_BZS47.InDPFault;
	// 
	//-Set Parameters---
	// 
	//-Call object control---
	      CALL  #A_2715_BZS47;
	// 
	//-Write outputs---
	NETWORK
	TITLE ==A-2715-BZS46 : COUPLING DRIVE UNLOCKED
	NOP 0;
	//-InLocal---
	      U     #tmpLocal; 
	      =     #A_2715_BZS46.InLocal; 
	// 
	//-Monitoring---
	      CLR;
	      =      #A_2715_BZS46.InMonTrue; 
	      CLR;
	      =      #A_2715_BZS46.InMonFalse; 
	// 
	//-PreAlarm---
	      CLR;
	      =      #A_2715_BZS46.InPreAlarmTrue; 
	      CLR;
	      =      #A_2715_BZS46.InPreAlarmFalse; 
	// 
	//-Additional commands---
	//      CLR
	//      S     #A_2715_BZS46.CmdRunTimeOutTime
	// 
